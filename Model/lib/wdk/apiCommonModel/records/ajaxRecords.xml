<wdkModel>

<!-- notes

 -these are the recordclasses used for the Ajax Question pages

-->

  <recordClassSet name="AjaxRecordClasses">

<!--=====================================================================-->
<!-- INTERPRO TERM CLASSES-->
<!--=====================================================================-->

        <!-- Interpro Term class -->
        <recordClass idPrefix="" 
          name="InterproTermClass" type="Ontology">
            <projectParamRef ref="recordParams.Project"/>
            <table name="InterproTerms" displayName="Pfam Terms"
                   queryRef="AjaxTables.InterproTermList">
                <columnAttribute name="accession"/>
                <columnAttribute name="name"/>
                <columnAttribute name="description"/>
            </table>
        </recordClass> 

