<wdkModel>
    <recordClassSet name="IsolateRecordClasses" includeProjects="CryptoDB,ApiDB,PlasmoDB">

      <recordClass idPrefix="" name="IsolateRecordClass" type="Isolate" includeProjects="CryptoDB,ApiDB">

        <projectParamRef ref="recordParams.Project"/>

      <!-- =============================================================== -->
      <!--   Reporters -->
      <!-- =============================================================== -->

      <reporter name="tabular" displayName="Summary - TAB Delimited"
                implementation="org.gusdb.wdk.model.report.TabularReporter" />

      <reporter name="fullRecord" displayName="Detailed" excludeProjects="ApiDB" 
                  implementation="org.gusdb.wdk.model.report.FullRecordReporter" />


      <!-- =================================================================== -->
      <!--  Attributes  ++++++++-->
      <!-- =================================================================== -->

      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <!-- sequence attributes -->  
      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

      <attributeQueryRef ref="IsolateAttributes.Sequence" includeProjects="CryptoDB,ApiDB">
         <columnAttribute name="pcr_primers" displayName="PCR primer" internal="true" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="country" displayName="Country" internal="true" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="organism" displayName="Organism" internal="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="strain" displayName="Strain" internal="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="specific_host" displayName="Host" internal="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="isolation_source" displayName="Isolation Source" internal="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="description" displayName="Description" internal="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="query_name" displayName="Query Name" internal="true" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="target_name" displayName="Target Name" internal="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="min_subject_start" displayName="Target Start" internal="true" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="max_subject_end" displayName="Target End" internal="true" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="note" displayName="Note" internal="true" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="product" displayName="Isolate Product" internal="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="is_reference" displayName="Is Reference" internal="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="map" displayName="Mapping" internal="false" includeProjects="CryptoDB,ApiDB"/>
      </attributeQueryRef>

      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <!-- Protein Sequence attributes -->  
      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

      <attributeQueryRef ref="IsolateAttributes.ProteinSequence" includeProjects="CryptoDB,ApiDB">
         <columnAttribute name="protein_sequence" displayName="Protein Sequence" internal="true" includeProjects="CryptoDB"/>
         <columnAttribute name="gene_description" displayName="Gene Description" internal="true" includeProjects="CryptoDB,ApiDB"/>
      </attributeQueryRef>

      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <!-- DNA Sequence attributes -->  
      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

      <attributeQueryRef ref="IsolateAttributes.DNASequence" includeProjects="CryptoDB,ApiDB">
         <columnAttribute name="sequence" displayName="DNA Sequence" internal="true" includeProjects="CryptoDB,ApiDB"/>
      </attributeQueryRef>

      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <!-- Gene Overlap attributes -->  
      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

      <attributeQueryRef ref="IsolateAttributes.GeneOverlap" includeProjects="CryptoDB,ApiDB">
         <columnAttribute name="gene" displayName="Gene" internal="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="gene_product" displayName="Gene Product" internal="false" includeProjects="CryptoDB,ApiDB"/>
      </attributeQueryRef>

      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <!-- Reference attributes -->  
      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

      <attributeQueryRef ref="IsolateAttributes.Reference" includeProjects="CryptoDB,ApiDB">
         <columnAttribute name="title" displayName="Title" internal="true" inReportMaker="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="author" displayName="Author" internal="true" inReportMaker="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="year" displayName="Year" internal="true"  inReportMaker="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="journal_or_book_name" displayName="Journal Name" inReportMaker="false" internal="true" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="journal_vol" displayName="Volume" internal="true" inReportMaker="false" includeProjects="CryptoDB,ApiDB"/>
         <columnAttribute name="journal_page" displayName="Page" internal="true" inReportMaker="false" includeProjects="CryptoDB,ApiDB"/>
      </attributeQueryRef>


     <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
     <!-- Isolates Publication table -->
     <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

     <table name="Reference" includeProjects="CryptoDB,ApiDB"
            displayName="Isolates Publication"
            queryRef="IsolateTables.Reference">
        <columnAttribute displayName="Title" name="title"/>
        <columnAttribute displayName="Author" name="author"/>
        <columnAttribute displayName="Journal" name="journal_or_book_name"/>
     </table>

     <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
     <!-- overview -->
     <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
     <textAttribute name="overview" displayName="Overview"
          internal="true" inReportMaker="false" includeProjects="CryptoDB,ApiDB">
          <text>
          <![CDATA[
           <table>
             <tr>
              <td width=18%>Isolate ID:</td>
              <td>$$primaryKey$$ <a href="http://www.ncbi.nlm.nih.gov/entrez/viewer.fcgi?db=nuccore&id=$$primaryKey$$" target=_blank><font size="-1">click here to Genbank</font></a></td>
             </tr>
             <tr>
              <td>Description:</td>
              <td>$$description$$</td>
             </tr>
             <tr>
              <td>Organism:</td>
              <td><i>$$organism$$</i></td>
             </tr>
             <tr>
              <td>Strain:</td>
              <td>$$strain$$</td>
             </tr>
             <tr>
              <td>Host:</td>
              <td>$$specific_host$$</td>
             </tr>
             <tr>
              <td>Product:</td>
              <td>$$product$$</td>
             </tr>
             <tr>
              <td>Note:</td>
              <td>$$note$$</td>
             </tr>
             <tr>
              <td>PCR Primer:</td>
              <td>$$pcr_primers$$</td>
             </tr>
             <tr>
              <td>Isolation Source:</td>
              <td>$$isolation_source$$</td>
             </tr>
           </table>
          ]]>
          </text>
     </textAttribute>

     </recordClass>

      <recordClass idPrefix="" name="BarcodeRecordClass" type="Isolate" includeProjects="PlasmoDB">


        <projectParamRef ref="recordParams.Project"/>

      <!-- =============================================================== -->
      <!--   Reporters -->
      <!-- =============================================================== -->

      <reporter name="tabular" displayName="Summary - TAB Delimited"
                implementation="org.gusdb.wdk.model.report.TabularReporter" />

      <attributeQueryRef ref="IsolateAttributes.Barcode" includeProjects="PlasmoDB">
         <columnAttribute name="strain" displayName="Strain" internal="false" includeProjects="PlasmoDB"/>
         <columnAttribute name="country" displayName="Country" internal="false" includeProjects="PlasmoDB"/>
         <columnAttribute name="collected_by" displayName="Collected_By" internal="false" includeProjects="PlasmoDB"/>
         <columnAttribute name="note" displayName="Note" internal="false" includeProjects="PlasmoDB"/>
         <columnAttribute name="na_sequence_id" displayName="NA Sequence ID" internal="false" includeProjects="PlasmoDB"/>
         <columnAttribute name="sequence" displayName="Sequence" internal="false" includeProjects="PlasmoDB"/>
      </attributeQueryRef>

     <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
     <!-- Isolates Barcode SNP table -->
     <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

     <table name="SNP" includeProjects="PlasmoDB"
            displayName="Isolates SNP Features"
            queryRef="IsolateTables.SNP">
        <columnAttribute displayName="Allele" name="allele"/>
        <columnAttribute displayName="Chromosome" name="chromosome"/>
        <columnAttribute displayName="Position" name="start_min"/>
        <columnAttribute displayName="SNP ID" name="snp_id"/>
     </table>

     </recordClass>





  </recordClassSet>
</wdkModel>

