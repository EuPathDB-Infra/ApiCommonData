<wdkModel>

  <!-- notes

   - eventually projectId and Project param should be combined


  -->

    <paramSet name="recordParams">
    
      <stringParam name="primaryKey" prompt="Primary Key"/>

      <stringParam name="projectId" prompt="Project ID"/>

      <stringParam name="source_id" prompt="Primary Key"/>

      <stringParam name="project_id" prompt="Project ID"/>

      <stringParam name="Query" prompt="Project ID"/>

      <enumParam name="Project" prompt="Project">
        <useTermOnly value="true"/>
       <enumList>
         <enumValue>
            <term>CryptoDB</term>
            <internal>CryptoDB</internal>
          </enumValue>
          <enumValue>
            <term>PlasmoDB</term>
            <internal>PlasmoDB</internal>
          </enumValue>
          <enumValue>
            <term>ToxoDB</term>
            <internal>ToxoDB</internal>
          </enumValue>
      </enumList>
    </enumParam>

    <answerParam name="gene_answer"
                 recordClassRef="GeneRecordClasses.GeneRecordClass"
                 prompt="Gene Answer" />

        
    <enumParam name="instance_gene"
               prompt="instances">
        <enumList>
            <enumValue>
                <term>any_instance</term>
                <internal>any_instance</internal>
            </enumValue>
            <enumValue>
                <term>toxo_genes</term>
                <internal>toxo_genes</internal>
            </enumValue>
            <enumValue>
                <term>gt1_instances</term>
                <internal>gt1_instances</internal>
            </enumValue>
            <enumValue>
                <term>me49_instances</term>
                <internal>me49_instances</internal>
            </enumValue>
            <enumValue>
                <term>veg_instances</term>
                <internal>veg_instances</internal>
            </enumValue>
            <enumValue>
                <term>all_instances</term>
                <internal>all_instances</internal>
            </enumValue>
        </enumList>
    </enumParam>
    
    <enumParam name="strain_summary"
               prompt="View Type">
        <enumList>
            <enumValue>
                <term>Instances</term>
                <internal>Instances</internal>
            </enumValue>
        </enumList>
    </enumParam>
      
  </paramSet>

</wdkModel>

