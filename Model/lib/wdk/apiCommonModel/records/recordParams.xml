<wdkModel>

  <!-- notes

   - eventually projectId and Project param should be combined


  -->

    <paramSet name="recordParams">
    
      <stringParam name="primaryKey" prompt="Primary Key"/>

      <stringParam name="projectId" prompt="Project ID"/>

      <stringParam name="source_id" prompt="Primary Key"/>

      <stringParam name="project_id" prompt="Project ID"/>

      <stringParam name="Query" prompt="Project ID"/>

      <enumParam name="Project" prompt="Project">
        <useTermOnly value="true"/>
       <enumList>
         <enumValue>
            <term>CryptoDB</term>
            <internal>CryptoDB</internal>
          </enumValue>
          <enumValue>
            <term>PlasmoDB</term>
            <internal>PlasmoDB</internal>
          </enumValue>
          <enumValue>
            <term>ToxoDB</term>
            <internal>ToxoDB</internal>
          </enumValue>
      </enumList>
    </enumParam>

    <answerParam name="gene_answer"
                 recordClassRef="GeneRecordClasses.GeneRecordClass"
                 prompt="Gene Answer" />

        
    <enumParam name="strains"
               prompt="Strains">
        <enumList>
            <enumValue>
                <term>All</term>
                <internal>all</internal>
            </enumValue>
            <enumValue>
                <term>ME49</term>
                <internal>ME49</internal>
            </enumValue>
            <enumValue>
                <term>VEG</term>
                <internal>VEG</internal>
            </enumValue>
            <enumValue>
                <term>GT1</term>
                <internal>GT1</internal>
            </enumValue>
        </enumList>
    </enumParam>
    
    <enumParam name="instance_gene"
               prompt="View Type">
        <enumList>
            <enumValue>
                <term>Genes</term>
                <internal>gene</internal>
            </enumValue>
            <enumValue>
                <term>Instances</term>
                <internal>instance</internal>
            </enumValue>
        </enumList>
    </enumParam>
      
  </paramSet>

</wdkModel>

