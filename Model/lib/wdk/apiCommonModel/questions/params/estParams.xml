<wdkModel>

  <!--===========================================================================-->
  <!-- Params  -->
  <!--===========================================================================-->

    <paramSet name="estParams">


      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <!-- EST ID  -->
      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <stringParam name="est_id"
                     prompt="EST ID"
                     substitute="yes">
            <help>Enter an EST ID (Accession Number)</help>
            <suggest includeProjects="PlasmoDB"
               sample="AU087658, BQ739759, N98084"
               default="AU087658"/>
            <suggest  includeProjects="ToxoDB"
               sample="AA009308, AA011973, W99666"
               default="AA009308"/>
            <suggest includeProjects="ApiDB"
               sample="AU087658, N98084,AA009308, AA011973"
               default="AU087658"/>
      </stringParam>

      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <!-- Blast database type  -->
      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
    <enumParam name="BlastDatabaseType"
                     prompt="Target"
                     multiPick="false">
      <help>
             The type of sequence you want to BLAST against (the subject)
      </help>
      <enumList>
        <enumValue>
          <term>EST</term>
          <internal>EST</internal>
        </enumValue>
<!--        <enumValue>
          <term>Translated EST</term>
          <internal>EST Translated</internal>
        </enumValue> -->
      </enumList>
    </enumParam>

  <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
  <!-- overlap or not  -->  
  <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

    <enumParam name="overlapOrNot"
                     prompt="EST"
                     multiPick="false"
                     quote="false">
      <help>
             Select yes to return only those EST alignments that are consistent.  We define define consistent as having >= 95% identity (the majority have >= 99%) and aligning the EST sequence in a manner that is consistent with a gene model.  Ie, there can be gaps (representing introns) in the target genomic sequence but not in the EST query.
      </help>
      <enumList excludeProjects="CryptoDB">   
        <enumValue>
          <term>does overlap with a gene</term>
          <internal>INTERSECT</internal>
        </enumValue>
        <enumValue>
          <term>does not overlap with a gene</term>
          <internal>MINUS</internal>
        </enumValue>
      </enumList>   
      <enumList includeProjects="CryptoDB">   
        <enumValue>
          <term>does overlap with a gene</term>
          <internal>IN</internal>
        </enumValue>
        <enumValue>
          <term>does not overlap with a gene</term>
          <internal>NOT IN</internal>
        </enumValue>
      </enumList>   
    </enumParam>

  </paramSet>


  <!--===========================================================================-->
  <!-- Vocab Queries  -->
  <!--===========================================================================-->


</wdkModel>
