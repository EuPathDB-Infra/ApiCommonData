<wdkModel>

  <!--===========================================================================-->
  <!-- Params  -->
  <!--===========================================================================-->

    <paramSet name="estParams">


      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <!-- EST ID  -->
      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <stringParam name="est_id"
                     prompt="EST ID"
                     substitute="yes">
            <help>Enter an EST ID (Accession Number)</help>
            <suggest includeProjects="PlasmoDB"
               sample="AU087658, BQ739759, N98084"
               default="AU087658"/>
            <suggest  includeProjects="ToxoDB"
               sample="AA009308, AA011973, W99666"
               default="AA009308"/>
            <suggest includeProjects="ApiDB"
               sample="AU087658, N98084,AA009308, AA011973"
               default="AU087658"/>
      </stringParam>

      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <!-- Blast database type  -->
      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
    <enumParam name="BlastDatabaseType"
                     prompt="Target"
                     multiPick="false">
      <help>
             The type of sequence you want to BLAST against (the subject)
      </help>
      <enumList>
        <enumValue>
          <term>EST</term>
          <internal>ESTs</internal>
        </enumValue>
        <enumValue>
          <term>Translated EST</term>
          <internal>ESTs Translated</internal>
        </enumValue>
      </enumList>
    </enumParam>

  </paramSet>


  <!--===========================================================================-->
  <!-- Vocab Queries  -->
  <!--===========================================================================-->


</wdkModel>
