<wdkModel>
  <!-- notes

     - should ms_assay param be a controlled vocab pulled from the database?

  -->

  <paramSet name="orfParams">

    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
    <!-- ID -->
    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
        <datasetParam name="orf_id" excludeProjects="ApiDB"
                     prompt="ORF ID input set">
            <help>Input a comma delimited set of ORF ids, or upload a file</help>
            <suggest includeProjects="CryptoDB"
                     sample="AAEL01000259-5-3339-3085, AAEE01000009*, AAEL*"
                     default="AAEL01000259-5-3339-3085"/>
            <suggest includeProjects="GiardiaDB"
                     sample="AACB02000007-6-227160-226945, AACB02000014-2-51002-51532"
                     default="AACB02000007-6-227160-226945"/>
            <suggest includeProjects="PlasmoDB"
                    sample="MAL1-1-10639-11520, MAL1-1-105160-105438"
                    default="MAL1-1-105160-105438"/>
            <suggest includeProjects="ToxoDB"
                     sample="Ia-1-1000180-1000404, Ia-6-99955-99632"
                     default="Ia-1-1000180-1000404"/>
            <suggest includeProjects="TrichDB"
                     sample="DS113375-1-114214-114468, DS113534-2-30548-30718"
                     default="DS113375-1-114214-114468"/>
            <suggest includeProjects="ApiDB"
                     sample="AAEL01000259-5-3339-3085, AAEE01000009*, Ia-6-99955-99632, MAL1-1-105160-105438"
                     default="AAEL01000259-5-3339-3085"/>
        </datasetParam>


        <stringParam name="orf_id" includeProjects="ApiDB"
                     prompt="ORF ID">
        <help>
             Enter an ORF ID.  Wildcards (*) are allowed.
        </help>
            <suggest includeProjects="ApiDB" 
                     sample="AAEL01000259-5-3339-3085, AAEE01000009*, Ia-6-99955-99632, MAL1-1-105160-105438"
                     default="AAEL01000259-5-3339-3085"/>
        </stringParam>

        <stringParam name="min_orf_length"
                     quote="false"
                     prompt="Minimum ORF length">
          <help>
              Enter the minimum length of ORF to return (NOTE: 50 aa is the shortest that have been computed).
          </help>
          <suggest sample="50, 100, 250" default="50"/>
        </stringParam>


    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
    <!-- blast db type -->
    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
<!-- currently using the param defined in sharedParams.xml  Jan 28, 2008 -->

    <enumParam name="BlastDatabaseType"
                     prompt="Target Data Type"
                     multiPick="false">
      <help>
             Organism(s) from which the sequences in the BLAST database originated
      </help>
      <useTermOnly value="true" includeProjects="ApiDB"/>
      <enumList>
        <enumValue>
          <term>ORF</term>
          <internal>ORF</internal>
        </enumValue>
      </enumList>
    </enumParam>

    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
    <!-- mass spec assay -->
    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
    <enumParam name="orf_ms_assay" includeProjects="ApiDB,CryptoDB,ToxoDB,GiardiaDB"
             prompt="Assay Type"
             multiPick="true"
             visible="true">
      <useTermOnly value="true" includeProjects="ApiDB"/>
      <enumList>
          <enumValue includeProjects="ApiDB,CryptoDB">
            <display><![CDATA[1D Gel LC-MS/MS<sup>&#182;</sup> (C.p.)]]></display>
            <term>Wastling MassSpec 1D Gel LC-MS/MS</term>
            <internal>Wastling MassSpec 1D Gel LC-MS/MS</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,CryptoDB">
            <display><![CDATA[2D Gel LC-MS/MS<sup>&#182;</sup> (C.p.)]]></display>
            <term>Wastling MassSpec 2D Gel LC-MS/MS</term>
            <internal>Wastling MassSpec 2D Gel LC-MS/MS</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,CryptoDB">
            <display><![CDATA[MudPit Soluble fractions<sup>&#182;</sup> (C.p.)]]></display>
            <term>Wastling MassSpec MudPit Soluble</term>
            <internal>Wastling MassSpec MudPit Soluble</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,CryptoDB">
            <display><![CDATA[MudPit Insoluble fractions<sup>&#182;</sup> (C.p.)]]></display>
            <term>Wastling MassSpec MudPit Insoluble</term>
            <internal>Wastling MassSpec MudPit Insoluble</internal>
          </enumValue>
        
  <enumValue includeProjects="ApiDB,GiardiaDB">
           <display><![CDATA[DTA-Select Filter (G.l.)]]></display>
           <term>DTASelect-filter_032108_Proteomics</term>
           <internal>DTASelect-filter_032108_Proteomics</internal>
          </enumValue>

  <enumValue includeProjects="ApiDB,ToxoDB">
            <display><![CDATA[1-D SDS PAGE<sup>1</sup> (T.g)]]></display>
            <term>Wastling 1-D SDS PAGE</term>
            <internal>Wastling 1-D SDS PAGE</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <display><![CDATA[1-D SDS PAGE Soluble Fraction<sup>1</sup> (T.g)]]></display>
            <term>Wastling 1-D SDS PAGE Soluble</term>
            <internal>Wastling 1-D SDS PAGE Soluble</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <display><![CDATA[1-D SDS PAGE Insoluble Fraction<sup>1</sup> (T.g)]]></display>
            <term>Wastling 1-D SDS PAGE Insoluble</term>
            <internal>Wastling 1-D SDS PAGE Insoluble</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <display><![CDATA[MudPIT Insoluble Fraction<sup>1</sup> (T.g)]]></display>
            <term>Wastling MudPIT Insoluble</term>
            <internal>Wastling MudPIT Insoluble</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <display><![CDATA[MudPIT Soluble Fraction<sup>1</sup> (T.g)]]></display>
            <term>Wastling MudPIT Soluble</term>
            <internal>Wastling MudPIT Soluble</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <display><![CDATA[Rhoptry Fraction<sup>1*</sup> (T.g)]]></display>
            <term>Wastling Rhoptry</term>
            <internal>Wastling Rhoptry</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <display><![CDATA[Conoid-enriched Fraction<sup>2</sup> (T.g)]]></display>
            <term>Murray Conoid-enriched Fraction</term>
            <internal>Murray Conoid-enriched Fraction</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <display><![CDATA[Conoid-depleted Fraction<sup>2</sup> (T.g)]]></display>
            <term>Murray Conoid-depleted Fraction</term>
            <internal>Murray Conoid-depleted Fraction</internal>
          </enumValue>

        

      </enumList>
    </enumParam>

  </paramSet>
</wdkModel>

