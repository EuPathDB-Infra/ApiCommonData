<wdkModel>

  <!-- notes

     - should ms_assay param be a controlled vocab pulled from the database?

  -->

  <paramSet name="orfParams">

    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
    <!-- ID -->
    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
        <stringParam name="orf_id"
                     prompt="ORF ID"
                     help="Enter an ORF ID.  Wildcards (*) are allowed."
                     substitute="yes">
            <suggest includeProjects="PlasmoDB"
                     sample="MAL1-1-10639-11520, MAL1-1-105160-105438"
                     default="MAL1-1-10639-11520"/>
            <suggest includeProjects="ToxoDB" 
                     sample="Ia-1-1000180-1000404, Ia-6-99955-99632"
                     default="Ia-1-1000180-1000404"/>
            <suggest includeProjects="CryptoDB"
                     sample="AAEL01000259-5-3339-3085, AAEE01000009*, AAEL*"
                     default="AAEL01000259-5-3339-3085"/>
            <suggest includeProjects="ApiDB" 
                     sample="AAEL01000259-5-3339-3085, AAEE01000009*, Ia-6-99955-99632, MAL1-1-105160-105438"
                     default="AAEL01000259-5-3339-3085"/>
        </stringParam>


    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
    <!-- blast db type -->
    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
      <enumParam name="BlastDatabaseType"
                     prompt="Target"
                     multiPick="false">
        <help>
             Organism(s) from which the sequences in the BLAST database originated
        </help>
        <useTermOnly value="true" includeProjects="ApiDB"/>
        <enumValue>
          <term>ORF (Protein)</term>
          <internal>orfs</internal>
        </enumValue>
      </enumParam>

    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
    <!-- mass spec assay -->
    <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
    <enumParam name="ms_assay" includeProjects="ApiDB,CryptoDB,ToxoDB"
             prompt="Assay Type"
             multiPick="true"
             visible="true">
        <useTermOnly value="true" includeProjects="ApiDB"/>
          <enumValue includeProjects="ApiDB,CryptoDB">
            <term><![CDATA[1D Gel LC-MS/MS<sup>&#182;</sup> (C.p.)]]></term>
            <internal>'Wastling MassSpec 1D Gel LC-MS/MS'</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,CryptoDB">
            <term><![CDATA[2D Gel LC-MS/MS<sup>&#182;</sup> (C.p.)]]></term>
            <internal>'Wastling MassSpec 2D Gel LC-MS/MS'</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,CryptoDB">
            <term><![CDATA[MudPit Soluble fractions<sup>&#182;</sup> (C.p.)]]></term>
            <internal>'Wastling MassSpec MudPit Soluble'</internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,CryptoDB">
            <term><![CDATA[MudPit Insoluble fractions<sup>&#182;</sup> (C.p.)]]></term>
            <internal>'Wastling MassSpec MudPit Insoluble'<internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <term><![CDATA[1-D SDS PAGE<sup>1</sup> (T.g)]]></term>
            <internal>'Wastling 1-D SDS PAGE'<internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <term><![CDATA[1-D SDS PAGE Soluble Fraction<sup>1</sup> (T.g)]]></term>
            <internal>'Wastling 1-D SDS PAGE Soluble'<internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <term><![CDATA[1-D SDS PAGE Insoluble Fraction<sup>1</sup> (T.g)]]></term>
            <internal>'Wastling 1-D SDS PAGE Insoluble'<internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <term><![CDATA[MudPIT Insoluble Fraction<sup>1</sup> (T.g)]]></term>
            <internal>'Wastling MudPIT Soluble'<internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <term><![CDATA[MudPIT Soluble Fraction<sup>1</sup> (T.g)]]></term>
            <internal>'Wastling MudPIT Soluble'<internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <term><![CDATA[Rhoptry Fraction<sup>1*</sup> (T.g)]]></term>
            <internal>'Wastling Rhoptry''<internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <term><![CDATA[Conoid-enriched Fraction<sup>2</sup> (T.g)]]></term>
            <internal>'Murray Conoid-enriched Fraction'<internal>
          </enumValue>
          <enumValue includeProjects="ApiDB,ToxoDB">
            <term><![CDATA[Conoid-depleted Fraction<sup>2</sup> (T.g)]]></term>
            <internal>'Murray Conoid-depleted Fraction'<internal>
          </enumValue>
      </enumParam>

  </paramSet>
</wdkModel>

