<wdkModel>

  <!-- notes

  -->

  <querySet name="AssemblyIds">

    <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
    <!-- Library  -->
    <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

    <sqlQuery name="AssembliesByLibrary" includeProjects="CryptoDB,ToxoDB" 
             isCacheable='true'>
        <paramRef ref="sharedParams.libraryId"/>
        <column name="source_id" />
        <column name="project_id"/>
        <sql>
            SELECT ea.source_id, ea.project_id
            FROM apidb.EstAttributes ea
            WHERE ea.library_id in ($$libraryId$$)
              AND ea.project_id = '@PROJECT_ID@'
       </sql>
    </sqlQuery>

  </querySet>
</wdkModel>
