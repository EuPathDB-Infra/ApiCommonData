       <paramRef ref="sharedParams.BlastAlgorithm" quote="false" />
       <paramRef ref="genomicSimilarityParams.BlastDatabaseOrganism" quote="false" />
       <paramRef ref="sharedParams.BlastQuerySequence" quote="false" />
