       <paramRef ref="sharedParams.-v" quote="false" />
       <paramRef ref="sharedParams.-b" quote="false" />
       <paramRef ref="sharedParams.-filter" quote="false" />

       <wsColumn name="source_id" width="32" wsName="Identifier"/>
