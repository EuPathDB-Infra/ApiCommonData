            <paramValue name="BlastQuerySequence">ACGCACTCCTGGCGGCCGGGCGGCTCGAACATACGAGAATCGATAGAAGAATCAGTTCGTCTGCCGTG</paramValue>
        </testParamValues>

        <testParamValues includeProjects="PlasmoDB">
