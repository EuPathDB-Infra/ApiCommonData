<wdkModel>


  <!--===========================================================================-->
  <!-- Gene Blast Params  -->
  <!--===========================================================================-->

    <paramSet name="GeneBlastDbParams">

      <enumParam name="BlastDatabaseType"
                     prompt="Target"
                     multiPick="false"
        <help>
           <text>
             The type of the target database (eg CDS, translated CDS, Protein)
           </text>
        </help>
        <enumList>
          <enumValue term="Proteins"
                 internal=   "proteins"/>
          <enumValue term="Transcripts"
                 internal=   "Transcripts"/>
          <enumValue term="Translated Transcripts"
                 internal=   "Transcripts Translated"/>
        </enumList>
      </enumParam>

      <enumParam name="BlastDatabaseOrganism"
                     prompt="Organism"
                     multiPick="false"
        <help>
           <text>
             Organism(s) from which the sequences in the BLAST database originated
           </text>
        </help>
        <enumList>
          <enumValue term="P. berghei" includeProjects="ApiDB,PlasmoDB"
                 internal=   "Pberghei"/>
          <enumValue term="P. chabaudi" includeProjects="ApiDB,PlasmoDB"
                 internal=   "Pchabaudi"/>
          <enumValue term="P. falciparum" includeProjects="ApiDB,PlasmoDB"
                 internal=   "Pfalciparum"/>
          <enumValue term="P. falciparum mitocondrial" includeProjects="ApiDB,PlasmoDB"
                 internal=   "PfalciparumMito"/>
          <enumValue term="P. falciparum plastid" includeProjects="ApiDB,PlasmoDB"
                 internal=   "PfalciparumPlastid"/>
          <enumValue term="P. knowlesi" includeProjects="ApiDB,PlasmoDB"
                 internal=   "Pknowlesi"/>
          <enumValue term="P. vivax" includeProjects="ApiDB,PlasmoDB"
                 internal=   "Pvivax"/>
          <enumValue term="P. yoelii" includeProjects="ApiDB,PlasmoDB"
                 internal=   "Pyoelii"/>
          <enumValue term="any" includeProjects="ApiDB,PlasmoDB"
                 internal=   "Plasmodium"/>

          <enumValue term="T. gondii ME49"  includeProjects="ApiDB,ToxoDB"
                 internal=   "Tgondii"/> 
          <enumValue term="T. gondi apicoplast" includeProjects="ApiDB,ToxoDB"
                 internal=   "TgondiiApicoplast"/>
          <enumValue term="T. gondii GT1"  includeProjects="ApiDB,ToxoDB"
                 internal=   "TgondiiGT1"/> 
          <enumValue term="T. gondii RH (chr Ia, Ib)"  includeProjects="ApiDB,ToxoDB"
                 internal=   "TgondiiRH"/> 
          <enumValue term="T. gondii VEG"  includeProjects="ApiDB,ToxoDB"
                 internal=   "TgondiiVeg"/> 

          <enumValue term="C. hominis" includeProjects="ApiDB,CryptoDB"
                 internal=   "Chominis"/> 
          <enumValue term="C. parvum" includeProjects="ApiDB,CryptoDB"
                 internal=   "Cparvum"/> 
          <enumValue term="C. parvum Chr6" includeProjects="ApiDB,CryptoDB"
                 internal=   "CparvumChr6"/> 

        </enumList>
      </enumParam>

    </paramSet>


  <!--===========================================================================-->
  <!-- Genomic Sequence Blast Params  -->
  <!--===========================================================================-->

    <paramSet name="SequenceBlastDbParams">

      <enumParam name="BlastDatabaseType"
                     prompt="Target"
                     translated CDS, Protein)" multiPick="false"
                     help="The type of the target database (eg CDS,
        <help>
           <text>
             Organism(s) from which the sequences in the BLAST database originated
           </text>
        </help>
        <enumList>
          <enumValue term="Genome"
                 internal=   "genomic"/>
          <enumValue term="Translated Genome"
                 internal=   "genomic Translated"/>
        </enumList>
      </enumParam>
     </paramSet>


  <!--===========================================================================-->
  <!-- ORF Blast Params  -->
  <!--===========================================================================-->


    <paramSet name="OrfBlastDbParams">

      <enumParam name="BlastDatabaseType"
                     prompt="Target"
                     translated CDS, Protein)" multiPick="false"
                     help="The type of the target database (eg CDS,
        <help>
           <text>
             Organism(s) from which the sequences in the BLAST database originated
           </text>
        </help>
        <enumList>
          <enumValue term="ORF (Protein)"
                 internal=   "orfs"/>
        </enumList>
      </enumParam>
     </paramSet>


  <!--===========================================================================-->
  <!-- EST Blast Params  -->
  <!--===========================================================================-->
          <enumValue term="'C. muris'"
                 internal=        'Cmuris'"/> 
          <enumValue term="'C. parvum EST'"
                 internal=        'CparvumEST'"/> 

    <paramSet name="EstBlastDbParams">

      <enumParam name="BlastDatabaseType"
                     prompt="Target"
                     translated CDS, Protein)" multiPick="false"
                     help="The type of the target database (eg CDS,
        <help>
           <text>
             Organism(s) from which the sequences in the BLAST database originated
           </text>
        </help>
        <enumList>
          <enumValue term="EST"
                 internal=   "ESTs"/>
          <enumValue term="Translated EST"
                 internal=   "ESTs Translated"/>
        </enumList>
      </enumParam>
     </paramSet>


  <!--===========================================================================-->
  <!-- ORF Motif Params  -->
  <!--===========================================================================-->

    <paramSet name="OrfMotifSearchParams">

      <enumParam name="Dataset"
                        prompt="Organism"
			multiPick="true"
        <help>
           <text>
             Choose one or more organisms to search with the given motif
           </text>
        </help>
        <enumList>
          <enumValue term="P. falciparum"
                 internal=   "Pfalciparum/PfalciparumOrfs_plasmoDB-5.3.fasta"/>
          <enumValue term="P. vivax"
                 internal=   "Pvivax/PvivaxOrfs_plasmoDB-5.3.fasta"/>
          <enumValue term="P. yoelii"
                 internal=   "Pyoelii/PyoeliiOrfs_plasmoDB-5.3.fasta"/>
          <enumValue term="P. berghei"
                 internal=   "Pberghei/PbergheiOrfs_plasmoDB-5.3.fasta"/>
          <enumValue term="P. chabaudi"
                 internal=   "Pchabaudi/PchabaudiOrfs_plasmoDB-5.3.fasta"/>
          <enumValue term="P. gallinaceum"
                 internal=   "Pgallinaceum/PgallinaceumOrfs_plasmoDB-5.3.fasta"/>
          <enumValue term="P. knowlesi"
                 internal=   "Pknowlesi/PknowlesiOrfs_plasmoDB-5.3.fasta"/>
          <enumValue term="P. reichenowi"
                 internal=   "Preichenowi/PreichenowiOrfs_plasmoDB-5.3.fasta"/>
        </enumList>
      </enumParam>
     </paramSet>


  <!--===========================================================================-->
  <!-- Gene Epitope Params  -->
  <!--===========================================================================-->

    <paramSet name="epitopeParams">

      <enumParam name="confidence"
                        prompt="Confidence"
                        multiPick="true"
        <help>
           <text>
             Epitopes are mapped to genes in PlasmoDB using high quality BLAST similarities (percent identity >= 97%) between the GenBank proteins listed on IEDB and the proteins found in PlasmoDB, and exact string matching.  Confidence levels are assigned to each set of epitopes based on the quality of this mapping.  A high confidence is assigned to a set where all of the epitopes are found on a given BLAST hit.  A medium confidence is assigned to a set where either some, but not all, of the epitopes in a set are found on a given BLAST hit, or where all of the epitopes in a set are found in a given protein, but the protein was not identified as a match by BLAST.  A low confidence is assigned to sets where some, but not all, of the epitopes are found in a given protein, but the protein was not identified by BLAST.  Choose one or more assigned confidence levels to search for epitopes in.
           </text>
        </help>
        <enumList>
          <enumValue term="High"
                 internal=   "'Full Set On Blast Hit'"/>
          <enumValue term="Low"
                 internal=   "'Not Full Set Not on Blast Hit'"/>
          <enumValue term="Medium"
                 internal=   "'Full Set Not on Blast Hit','Not Full Set On Blast Hit'"/>
        </enumList>
      </enumParam>
    </paramSet>

 
  <!--===========================================================================-->
  <!-- Id query Params  -->
  <!--===========================================================================-->

    <paramSet name="idQueryParams">

        <stringParam name="locus_tag"
                     prompt="Gene ID"
                     help="Enter an ID, such as a sequencing-center gene ID. The search is case insensitive, so capitalization does not matter. Asterisks can be used as wild cards (e.g. ' PF11_* ').  Genes whose IDs have changed can be found by either the old or the new ID. GI numbers (e.g. 23508535), Entrez Protein Locus IDs (e.g. NP_701204.1), PDB IDs (e.g. 1HN6), or GenBank IDs (e.g. AAB36701.1) can also be used."
                     substitute="yes"
            <suggest sample="PF11_0344, PY01656, Pv092275"
                     default="PF11_0344"/>
        </stringParam>

        <stringParam name="orf_id"
                     prompt="ORF ID"
                     help="Enter an ORF ID"
                     substitute="yes"
            <suggest sample="MAL1-1-10639-11520, MAL1-1-105160-105438"
                     default="MAL1-1-10639-11520"/>
        </stringParam>

        <stringParam name="est_id"
                     prompt="EST ID"
                     help="Enter an EST ID (Accession Number)"
                     substitute="yes"
            <suggest sample="AU087658, BQ739759, N98084"
                     default="AU087658"/>
        </stringParam>

        <stringParam name="snp_id"
                     prompt="SNP ID"
                     substitute="yes"
                     help="Enter an SNP ID"
            <suggest default="CombinedSNP.MAL11.3000"
                     sample="CombinedSNP.MAL11.3000, WTSI_PFAL.GHANA1.0.MAL10.1384630.1384630.0"/>
        </stringParam>
     </paramSet>
</wdkModel>
