#!/usr/bin/bash
set -e

# use this script to do the initial unpack in manual delivery
# run from, eg, IEDB/2.4

cp fromProvider/iedb_export.zip workspace
mkdir workspace/tmp
cd workspace/tmp
unzip ../iedb_export.zip
rm ../iedb_export.zip
cd ../..
mkdir -p final/iedb_export_fixed
iedbFixXml workspace/tmp final/iedb_export_fixed
rm -r workspace/tmp
