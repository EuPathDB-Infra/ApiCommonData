    <info
        displayName="Gregarina niphandrodes contigs"
        project="ToxoDB"
        organisms="@@GniphandrodesFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Gregarina niphandrodes</i> obtained from NCBI 
        ]]>
      </description>
    </info>
  </resource>

</resources>




