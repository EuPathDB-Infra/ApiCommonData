    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/sequences.gb --output_file @@dataDir@@/PyoeliiProduct.tab --feat_with_geneid mRNA --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product</unpack>
