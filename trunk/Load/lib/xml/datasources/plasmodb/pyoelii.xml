<resources>
  <resource plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
            resource="Pyoelii_Contigs_RSRC"
            version="@@Pyoelii_Contigs_VER@@">
    <manualGet fileOrDir="PlasmoDB/pyoelii/genome/contig/%RESOURCE_VERSION%/sequences.gb" />
    <pluginArgs>--extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/PlasmoDB/pyGenbank2gus.xml --inputFileOrDir @@dataDir@@/sequences_fixed.gb --fileFormat genbank --soCvsVersion @@SO_VER@@ --defaultOrganism "Plasmodium yoelii yoelii str. 17XNL" --seqSoTerm "contig" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out</pluginArgs>
    <unpack>filterContaminantsFromGenbankFile --seqFile @@dataDir@@/sequences.gb --outFile @@dataDir@@/sequences_fixed.gb --contaminantFile @@dataDir@@/contaminants.txt</unpack>
  </resource>
  <resource plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
            resource="Pyoelii_Kappe_Annotation_RSRC"
            version="@@Pyoelii_Kappe_Annotation_VER@@">
    <manualGet fileOrDir="PlasmoDB/pyoelii/genePrediction/From_Kappe/%RESOURCE_VERSION%/" />
    <pluginArgs>--extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/PlasmoDB/PyoeliiGffFeatures2gus.xml --inputFileOrDir @@dataDir@@/PyoeliiKappe_fixed.gff --fileFormat gff2  --gff2GroupTag "GenePrediction"  --soCvsVersion @@SO_VER@@ --defaultOrganism "Plasmodium yoelii yoelii str. 17XNL" --seqSoTerm "contig" --naSequenceSubclass ExternalNASequence --seqIdColumn source_id  --seqExtDbName "Pyoelii_Contigs_RSRC" --seqExtDbRlsVer @@Pyoelii_Contigs_VER@@ --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out</pluginArgs>
    <unpack>cat @@dataDir@@/@@Pyoelii_Kappe_Annotation_VER@@/ReAnnotatedPyoelii.tab | perl -pe 's/PY_PFF0395c\t23480141\t21\t46\t-/PY_PFF0395c\t23480141\t21\t46\t+/' > @@dataDir@@/ReAnnotatedPyoelii_fixed.tab</unpack>
    <unpack>createGffForYoeliiGenes --infile @@dataDir@@/ReAnnotatedPyoelii_fixed.tab --gfffile @@dataDir@@/PyoeliiKappe.gff --idmapfile @@dataDir@@/@@Pyoelii_Kappe_Annotation_VER@@/PyoeliiGI.txt</unpack>
    <unpack>dumpSequencesFromTable.pl --outputFile @@dataDir@@/Pyoelii.fasta --idSQL "SELECT enas.source_id, enas.sequence FROM dots.externalnasequence enas, sres.externaldatabase ed, sres.externaldatabaserelease edr where enas.external_database_release_id = edr.external_database_release_id AND edr.external_database_id = ed.external_database_id AND ed.name = 'P.yoelii_Contigs' AND edr.version = @@Pyoelii_Contigs_VER@@ "</unpack>
    <unpack>fixMercatorOffsetsInGFF.pl --f @@dataDir@@/Pyoelii.fasta --g @@dataDir@@/PyoeliiKappe.gff --o @@dataDir@@/PyoeliiKappe_fixed_1.gff</unpack>
    <unpack>fixYoeliiGFF --inputFile @@dataDir@@/PyoeliiKappe_fixed_1.gff --outputFile @@dataDir@@/PyoeliiKappe_fixed.gff</unpack>
  </resource>
  <resource plugin="GUS::Community::Plugin::InsertECMapping"
            resource="Pyoelii_ECNumbers_RSRC"
