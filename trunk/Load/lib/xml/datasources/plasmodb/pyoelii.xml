    <manualGet fileOrDir="PlasmoDB/pyoelii/function/@@Pyoelii_Contigs_VER@@/ECNumbers_Pyoelii/ec_numbers.txt" />
    <pluginArgs>--ECMappingFile @@dataDir@@/ec_numbers.txt --evidenceCode "Annotation Center" --aaSeqLocusTagMappingSql "SELECT aa_sequence_id FROM dots.GeneFeature g, dots.Transcript t, dots.TranslatedAAFeature taf WHERE g.source_id = ? AND g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id"</pluginArgs>
  </resource>
