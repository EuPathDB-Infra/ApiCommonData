<resources>
  <resource plugin="GUS::Supported::Plugin::LoadFastaSequences"
            resource="Cfasci_contigs_RSRC"
            version="@@Cfasci_contigs_VER@@">
    <manualGet fileOrDir="TriTrypDB/cfasci/genome/contig/%RESOURCE_VERSION%/final/supercontigs.fasta" />
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 5656 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/supercontigs.fasta --SOTermName 'supercontig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::LoadNothing"
            resource="Cfasci_RNASeq_Beverley_RSRC"
            version="@@Cfasci_RNASeq_Beverley_VER@@">
    <getAndUnpackOutput dir="@@dataDir@@/final" />
    <getAndUnpackOutput file="@@dataDir@@/final/s_8_1_sequence.txt" />
    <getAndUnpackOutput file="@@dataDir@@/final/s_8_2_sequence.txt" />
    <manualGet fileOrDir="TriTrypDB/cfasci/rnaSeq/Beverley_Crithidia/%RESOURCE_VERSION%/final/" />
    <pluginArgs>--dbRefExternalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%"</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Cfasci_contigs_DBRefs_RSRC"
            version="12.0">
    <manualGet fileOrDir="TriTrypDB/cfasci/dbxref/dbRefNASequence/%RESOURCE_VERSION%/final/dbxref_contig_id.tab" />
    <pluginArgs>-DbRefMappingFile @@dataDir@@/dbxref_contig_id.tab --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier" --tableName "DbRefNASequence" --geneExternalDatabaseSpec 'Cfasci_contigs_RSRC|@@Cfasci_contigs_VER@@'</pluginArgs>
  </resource>
</resources>
