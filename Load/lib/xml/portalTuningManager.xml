    <externalDependency name="dots.ExternalNaSequence"/>

    <unionization>
      <source dblink="CRYPTO.PITAYA.RCC.UGA.EDU"/>
      <source dblink="GIARDIA.PITAYA.RCC.UGA.EDU"/>
      <source dblink="PLASMO.PITAYA.RCC.UGA.EDU"/>
      <source dblink="TOXO.PITAYA.RCC.UGA.EDU"/>
      <source dblink="TRITRYP.PITAYA.RCC.UGA.EDU"/>

      <source>
        <query> 
          <![CDATA[ 
            select *
            from apidb.EstAttributes@AMITO.PITAYA.RCC.UGA.EDU
            where project_id = 'TrichDB' 
          ]]> 
        </query>
      </source>

      <source>
        <query> 
          <![CDATA[           
            select cast('ApiDB' as varchar2(47)) as project_id,
                   cast(enas.source_id as varchar2(255)) as source_id,
                   enas.a_count, enas.c_count, enas.g_count, enas.t_count,
                   enas.other_count, enas.length,
                   cast('Ncbi dbEST' as varchar2(120)) as dbest_name,
                   cast('Other Apicomplexan (no Cmp,Pbfvy,Tg)' as varchar2(40)) as organism,
                   cast(5794 as number(10)) as ncbi_tax_id,
                   cast('Other Apicomplexan ESTs' as varchar2(80)) as external_db_name,
                   cast(9999 as number(5)) as library_idx
            from dots.ExternalNaSequence enas 
            where enas.taxon_id = (select taxon_id from sres.TaxonName where name = 'Apicomplexa')
        ]]> 
      </query>
    </source>
    </unionization>
  </tuningTable>

