
<resource resource="GlambliaGenomic" version="giardiadb@giardiaVersion@"
              displayName="G.lamblia genomics"
              category="Genome"
              plugin="GUS::Supported::Plugin::LoadFastaSequences"
              extDbName="GiardiaDB"
              extDbRlsVer="@giardiaVersion@"
              extDbRlsDescrip="Glamblia Genomics obtained from giardiadb@giardiaVersion@ downloads"
              dbCommit="@dbcommit@">
        <manualGet fileOrDir="rsync://europa.pcbi.upenn.edu:/files/cbil/data/cbil/apiSiteFiles/downloadSite/GiardiaDB/release-1.1/Glamblia/GlambliaGenomic_GiardiaDB-@giardiaVersion@.fasta "
            email="help@GiardiaDB.org"
            contact="GiardiaDB"
            institution="GiardiaDB"
        />
        <pluginArgs>--externalDatabaseName %EXT_DB_NAME% --externalDatabaseVersion %EXT_DB_RLS_VER% --sequenceFile @downloadDir@/GlambliaGenomic/GlambliaGenomic_GiardiaDB-1.1.fasta --SOTermName supercontig --ncbiTaxId @GlTaxid@ --regexSourceId '^>*(?:\w*\|)*(\S+)' --tableName 'DoTS::ExternalNASequence' --noCheck</pluginArgs>
        <description>
            <![CDATA[
            G.lamblia Genomics from GiardiaDB@giardiaVersion@ downloads.
            ]]>
        </description>
    </resource>


