<resources>

<!--ISFResource-->

  <resource resource="EhellemATCC50504_genome_RSRC" version="@@EhellemATCC50504_genome_VER@@"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="MicrosporidiaDB/ehellemATCC50504/genome/ehellemATCC50504/%RESOURCE_VERSION%/final/embl/"/>
    <pluginArgs>--organism "Encephalitozoon hellem ATCC 50504" --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --fileFormat embl --inputFileOrDir @@dataDir@@/embl --mapFile @@gusHome@@/lib/xml/isf/MicrosporidiaDB/EhellemEmbl2Gus.xml --soCvsVersion @@SO_VER@@ --bioperlTreeoutput @@dataDir@@/bioperlTree.out --validationLog @@dataDir@@/validation.log --seqSoTerm chromosome</pluginArgs>
  </resource>



  <resource resource="EhellemATCC50504_ProductName_RSRC" version="@@EhellemATCC50504_genome_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile">
    <manualGet fileOrDir="MicrosporidiaDB/ehellemATCC50504/function/EhellemATCC50504_genome_RSRC_product/%RESOURCE_VERSION%/product.txt"/>
    <pluginArgs>--file @@dataDir@@/product.txt --productDbName EhellemATCC50504_genome_RSRC --productDbVer @@EhellemATCC50504_genome_VER@@</pluginArgs>
  </resource>

  <resource resource="EhellemATCC50504_Comment_RSRC" version="@@EhellemATCC50504_genome_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertNaFeatCommentFromTabFile">
    <manualGet fileOrDir="MicrosporidiaDB/ehellemATCC50504/genome/EhellemATCC50504_genome_RSRC_geneComment/%RESOURCE_VERSION%/comments.txt"/>
    <pluginArgs>--file @@dataDir@@/comments.txt --genomeDbName EhellemATCC50504_genome_RSRC --genomeDbVer @@EhellemATCC50504_genome_VER@@ </pluginArgs>
  </resource>


<!--ESTResource-->
  <resource resource="EhellemATCC50504_dbEST_RSRC" version="@@EhellemATCC50504_dbEST_VER@@"
      plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId 27973 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
  </resource>




</resources>
