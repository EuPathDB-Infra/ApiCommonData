<resources>

<!--ISFResource-->

  <resource resource="NceranaeBRL01_genome_RSRC" version="@@NceranaeBRL01_genome_VER@@"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="MicrosporidiaDB/nceranaeBRL01/genome/%RESOURCE_VERSION%/final/genome.gbf"/>
    <pluginArgs>--organism "Nosema ceranae BRL01" --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --fileFormat embl --inputFileOrDir @@dataDir@@/genome.gbf --mapFile @@gusHome@@/lib/xml/isf/MicrosporidiaDB/nceranaeGenbank2Gus.xml --soCvsVersion @@SO_VER@@ --bioperlTreeoutput @@dataDir@@/bioperlTree.out --validationLog @@dataDir@@/validation.log --seqSoTerm contig</pluginArgs>
  </resource>



  <resource resource="NceranaeBRL01_ProductName_RSRC" version="@@NceranaeBRL01_genome_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile">
    <manualGet fileOrDir="MicrosporidiaDB/nceranaeBRL01/function/NceranaeBRL01_genome_RSRC_product/%RESOURCE_VERSION%/product.txt"/>
    <pluginArgs>--file @@dataDir@@/product.txt --productDbName NceranaeBRL01_genome_RSRC --productDbVer @@NceranaeBRL01_genome_VER@@</pluginArgs>
  </resource>

  <resource resource="NceranaeBRL01_Comment_RSRC" version="@@NceranaeBRL01_genome_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertNaFeatCommentFromTabFile">
    <manualGet fileOrDir="MicrosporidiaDB/nceranaeBRL01/genome/NceranaeBRL01_genome_RSRC_geneComment/%RESOURCE_VERSION%/comments.txt"/>
    <pluginArgs>--file @@dataDir@@/comments.txt --genomeDbName NceranaeBRL01_genome_RSRC --genomeDbVer @@NceranaeBRL01_genome_VER@@ </pluginArgs>
  </resource>

  <resource resource="NceranaeBRL01_altids_dbxref_RSRC" version="@@NceranaeBRL01_genome_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertDBxRefs">
    <manualGet fileOrDir="MicrosporidiaDB/nceranaeBRL01/dbxref/NceranaeBRL01_genome_RSRC/%RESOURCE_VERSION%/NceranaeBRL01_altids.txt"/>
    <pluginArgs>--DbRefMappingFile @@dataDir@@/NceranaeBRL01_altids.txt --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'NceranaeBRL01_genome_RSRC|@@NceranaeBRL01_genome_VER@@' --idType 'alternate id'</pluginArgs>
  </resource>



  <resource resource="NceranaeBRL01_gi_dbxref_RSRC" version="@@NceranaeBRL01_genome_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertDBxRefs">
    <manualGet fileOrDir="MicrosporidiaDB/nceranaeBRL01/dbxref/NceranaeBRL01_genome_RSRC/%RESOURCE_VERSION%/NceranaeBRL01_gi.txt"/>
    <pluginArgs>--DbRefMappingFile @@dataDir@@/NceranaeBRL01_gi.txt --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'NceranaeBRL01_genome_RSRC|@@NceranaeBRL01_genome_VER@@' --idType 'alternate id'</pluginArgs>
  </resource>

<!--ESTResource-->
  <resource resource="NceranaeBRL01_dbEST_RSRC" version="@@NceranaeBRL01_dbEST_VER@@"
      plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId 40302 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
  </resource>




</resources>
