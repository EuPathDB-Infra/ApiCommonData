<resources>

<resource resource="EcuniculiEc2_genome_RSRC" version="@@EcuniculiEc2_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="MicrosporidiaDB/ecuniculiEC2/genome/contigs/%RESOURCE_VERSION%/final/Ence_cuni_EC2_V1.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 989654 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Ence_cuni_EC2_V1.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Encephalitozoon cuniculi EC2 contigs"
        project="AmoebaDB"
        organisms="@@EcuniculiEc2FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Encephalitozoon cuniculi EC2</i> obtained from the Broad Institutue
        ]]>
      </description>
    </info>
  </resource>

<resource resource="EcuniculiEc2SupercontigAGP_RSRC" version="@@EcuniculiEc2SupercontigAGP_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile">
    <manualGet fileOrDir="MicrosporidiaDB/ecuniculiEC2/genome/agp/%RESOURCE_VERSION%/final/Ence_cuni_EC2_V1.agp"/>
    <pluginArgs>--agpFile @@dataDir@@/Ence_cuni_EC2_V1.agp --seqPieceExtDbName "EcuniculiEc2_genome_RSRC" --seqPieceExtDbRlsVer @@EcuniculiEc2_genome_VER@@ --virSeqExtDbName %RESOURCE_NAME% --virSeqExtDbRlsVer %RESOURCE_VERSION% --virtualSeqSOTerm "supercontig" --ncbiTaxId "989654" --soVer @@SO_VER@@ --notChromosomes</pluginArgs>
    <info
        displayName="Encephalitozoon cuniculi EC2 contig map to scaffolds"
        project="AmoebaDB"
        organisms="@@EcuniculiEc2FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig to scaffold map for <i>Encephalitozoon cuniculi EC2</i> obtained from the Broad Institutue 
        ]]>
      </description>
    </info>
  </resource>

</resources>



