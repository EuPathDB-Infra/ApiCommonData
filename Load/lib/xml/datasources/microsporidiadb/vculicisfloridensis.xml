<resources>

<resource resource="Vculicisfloridensis_genome_RSRC" version="@@Vculicisfloridensis_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="MicrosporidiaDB/vculicisFloridensis/genome/contigs/%RESOURCE_VERSION%/final/Vavr_culi_floridensis_V1.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 948595 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Vavr_culi_floridensis_V1.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Vavraia culicis floridensis contigs"
        project="AmoebaDB"
        organisms="@@VculicisfloridensisFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Vavraia culicis floridensis</i> obtained from the Broad Institutue
        ]]>
      </description>
    </info>
  </resource>

<resource resource="VculicisfloridensisSupercontigAGP_RSRC" version="@@VculicisfloridensisSupercontigAGP_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile">
    <manualGet fileOrDir="MicrosporidiaDB/vculicisFloridensis/genome/agp/%RESOURCE_VERSION%/final/Vavr_culi_floridensis_V1.agp"/>
    <pluginArgs>--agpFile @@dataDir@@/Vavr_culi_floridensis_V1.agp --seqPieceExtDbName "Vculicisfloridensis_genome_RSRC" --seqPieceExtDbRlsVer @@Vculicisfloridensis_genome_VER@@ --virSeqExtDbName %RESOURCE_NAME% --virSeqExtDbRlsVer %RESOURCE_VERSION% --virtualSeqSOTerm "supercontig" --ncbiTaxId 948595 --soVer @@SO_VER@@ --notChromosomes</pluginArgs>
    <info
        displayName="Vavraia culicis floridensis contig map to scaffolds"
        project="AmoebaDB"
        organisms="@@VculicisfloridensisFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig to scaffold map for <i>Vavraia culicis floridensis</i> obtained from the Broad Institutue 
        ]]>
      </description>
    </info>
  </resource>

</resources>



