<resources>

<resource resource="EcuniculiEc1_genome_RSRC" version="@@EcuniculiEc1_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="MicrosporidiaDB/ecuniculiEC1/genome/contigs/%RESOURCE_VERSION%/final/Ence_cuni_EC1_V1.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 986730 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Ence_cuni_EC1_V1.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Encephalitozoon cuniculi EC1 contigs"
        project="AmoebaDB"
        organisms="@@EcuniculiEc1FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Encephalitozoon cuniculi EC1</i> obtained from the Broad Institutue
        ]]>
      </description>
    </info>
  </resource>

<resource resource="EcuniculiEc1SupercontigAGP_RSRC" version="@@EcuniculiEc1SupercontigAGP_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile">
    <manualGet fileOrDir="MicrosporidiaDB/ecuniculiEC1/genome/agp/%RESOURCE_VERSION%/final/Ence_cuni_EC1_V1.agp"/>
    <pluginArgs>--agpFile @@dataDir@@/Ence_cuni_EC1_V1.agp --seqPieceExtDbName "EcuniculiEc1_genome_RSRC" --seqPieceExtDbRlsVer @@EcuniculiEc1_genome_VER@@ --virSeqExtDbName %RESOURCE_NAME% --virSeqExtDbRlsVer %RESOURCE_VERSION% --virtualSeqSOTerm "supercontig" --ncbiTaxId "986730" --soVer @@SO_VER@@ --notChromosomes</pluginArgs>
    <info
        displayName="Encephalitozoon cuniculi EC1 contig map to scaffolds"
        project="AmoebaDB"
        organisms="@@EcuniculiEc1FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig to scaffold map for <i>Encephalitozoon cuniculi EC1</i> obtained from the Broad Institutue 
        ]]>
      </description>
    </info>
  </resource>

</resources>



