<resources>

<resource resource="EcuniculiEc3_genome_RSRC" version="@@EcuniculiEc3_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="MicrosporidiaDB/ecuniculiEC3/genome/contigs/%RESOURCE_VERSION%/final/Ence_cuni_EC3_V1.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 989655 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Ence_cuni_EC3_V1.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Encephalitozoon cuniculi EC3 contigs"
        project="AmoebaDB"
        organisms="@@EcuniculiEc3FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Encephalitozoon cuniculi EC3</i> obtained from the Broad Institutue
        ]]>
      </description>
    </info>
  </resource>

<resource resource="EcuniculiEc3SupercontigAGP_RSRC" version="@@EcuniculiEc3SupercontigAGP_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile">
    <manualGet fileOrDir="MicrosporidiaDB/ecuniculiEC3/genome/agp/%RESOURCE_VERSION%/final/Ence_cuni_EC3_V1.agp"/>
    <pluginArgs>--agpFile @@dataDir@@/Ence_cuni_EC3_V1.agp --seqPieceExtDbName "EcuniculiEc3_genome_RSRC" --seqPieceExtDbRlsVer @@EcuniculiEc3_genome_VER@@ --virSeqExtDbName %RESOURCE_NAME% --virSeqExtDbRlsVer %RESOURCE_VERSION% --virtualSeqSOTerm "supercontig" --ncbiTaxId 989655 --soVer @@SO_VER@@ --notChromosomes</pluginArgs>
    <info
        displayName="Encephalitozoon cuniculi EC3 contig map to scaffolds"
        project="AmoebaDB"
        organisms="@@EcuniculiEc3FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig to scaffold map for <i>Encephalitozoon cuniculi EC3</i> obtained from the Broad Institutue 
        ]]>
      </description>
    </info>
  </resource>

</resources>



