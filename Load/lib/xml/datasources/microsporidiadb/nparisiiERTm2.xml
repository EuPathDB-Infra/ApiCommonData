<resources>

<resource resource="NparisiiERTm2_genome_RSRC" version="@@NparisiiERTm2_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="MicrosporidiaDB/nparisiiERTm2/genome/contigs/%RESOURCE_VERSION%/final/Nema_parisii_ERTm2_V1.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 10000002 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Nema_parisii_ERTm2_V1.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Nematocida parisii ERTm2 contigs"
        project="AmoebaDB"
        organisms="@@NparisiiERTm2FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Nematocida parisii ERTm2</i> obtained from the Broad Institutue
        ]]>
      </description>
    </info>
  </resource>

<resource resource="NparisiiERTm2SupercontigAGP_RSRC" version="@@NparisiiERTm2SupercontigAGP_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile">
    <manualGet fileOrDir="MicrosporidiaDB/nparisiiERTm2/genome/agp/%RESOURCE_VERSION%/final/Nema_parisii_ERTm2_V1.agp"/>
    <pluginArgs>--agpFile @@dataDir@@/Nema_parisii_ERTm2_V1.agp --seqPieceExtDbName "NparisiiERTm2_genome_RSRC" --seqPieceExtDbRlsVer @@NparisiiERTm2_genome_VER@@ --virSeqExtDbName %RESOURCE_NAME% --virSeqExtDbRlsVer %RESOURCE_VERSION% --virtualSeqSOTerm "supercontig" --ncbiTaxId 10000002 --soVer @@SO_VER@@ --notChromosomes</pluginArgs>
    <info
        displayName="Nematocida parisii ERTm2 contig map to scaffolds"
        project="AmoebaDB"
        organisms="@@Nematocida parisii ERTm2@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig to scaffold map for <i>Nematocida parisii ERTm2</i> obtained from the Broad Institutue 
        ]]>
      </description>
    </info>
  </resource>

</resources>



