<resources>

<resource resource="NparisiiERTm1_genome_RSRC" version="@@NparisiiERTm1_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="MicrosporidiaDB/nparisiiERTm1/genome/contigs/%RESOURCE_VERSION%/final/Nema_parisii_ERTm1_V3.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 881290 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Nema_parisii_ERTm1_V3.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Nematocida parisii ERTm1 contigs"
        project="AmoebaDB"
        organisms="@@NparisiiERTm1FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Nematocida parisii ERTm1</i> obtained from the Broad Institutue
        ]]>
      </description>
    </info>
  </resource>

<resource resource="NparisiiERTm1SupercontigAGP_RSRC" version="@@NparisiiERTm1SupercontigAGP_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile">
    <manualGet fileOrDir="MicrosporidiaDB/nparisiiERTm1/genome/agp/%RESOURCE_VERSION%/final/Nema_parisii_ERTm1_V3.agp"/>
    <pluginArgs>--agpFile @@dataDir@@/Nema_parisii_ERTm1_V3.agp --seqPieceExtDbName "NparisiiERTm1_genome_RSRC" --seqPieceExtDbRlsVer @@NparisiiERTm1_genome_VER@@ --virSeqExtDbName %RESOURCE_NAME% --virSeqExtDbRlsVer %RESOURCE_VERSION% --virtualSeqSOTerm "supercontig" --ncbiTaxId 881290 --soVer @@SO_VER@@ --notChromosomes</pluginArgs>
    <info
        displayName="Nematocida parisii ERTm1 contig map to scaffolds"
        project="AmoebaDB"
        organisms="@@NparisiiERTm1FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig to scaffold map for <i>Nematocida parisii ERTm1</i> obtained from the Broad Institutue 
        ]]>
      </description>
    </info>
  </resource>

</resources>



