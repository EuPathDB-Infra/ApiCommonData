<resources>
  <resource resource="Pc_dbEST_RSRC" version="@Pc_dbEST_VER@"
      plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId 5825 --outputFile @dataDir@/taxonIdFile</unpack>
    <unpack>createPropFile --file @dataDir@/dbest.prop --propList "dbestLogin:@dbestLogin@,dbestPswd:@dbestPswd@"</unpack>
    <pluginArgs>--span 500 --taxonFile @dataDir@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @SO_VER@ --dbestLoginFile @dataDir@/dbest.prop --dbestConnect "dbi:Oracle:dbst100n" </pluginArgs>
    <info
        displayName="Plasmodium dbEST ESTs"
        project="PlasmoDB"
        organisms="@PcOrganismFullName@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Plasmodium ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Pchabaudi_chromosomes_RSRC" version="@Pchabaudi_chromosomes_VER@"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="PlasmoDB/pchabaudi/genome/chromosome/%RESOURCE_VERSION%"/>
    <pluginArgs>--extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --mapFile isf/PlasmoDB/PchabaudiEmbl2gus.xml --inputFileOrDir @dataDir@/%RESOURCE_VERSION%  --inputFileExtension "embl" --fileFormat embl --soCvsVersion @SO_VER@ --defaultOrganism "Plasmodium chabaudi chabaudi"  --seqSoTerm "chromosome" --validationLog validation.log --bioperlTreeOutput bioperlTree.out</pluginArgs>
    <info
        displayName="P. chabaudi (AS) chromosome sequences and annotation"
        project="PlasmoDB"
        organisms="@PcOrganismFullName@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Annotation of the <i>P. chabaudi</i> AS chromosomes obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute, 2009-03-24. Includes sequence and gene models. 
   The Wellcome Trust Sanger Institute plans on publishing the completed and annotated sequences (i.e. 8X assembly and updated annotation) of P. chabaudi AS in a peer-reviewed journal as soon as possible. Permission of the principal investigator should be obtained before publishing analyses of the sequence/open reading frames/genes on a chromosome or genome scale.
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Pc_IEDB_Epitopes_RSRC" version="@Pc_IEDB_Epitopes_VER@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="PlasmoDB/pchabaudi/epitope/IEDB/%RESOURCE_VERSION%"/>
    <unpack>mkdir @dataDir@/IEDB_Epitopes_fixed</unpack>
    <unpack>createEpitopeFilesFromExcel --excelDir @dataDir@/%RESOURCE_VERSION%  --outDir @dataDir@/IEDB_Epitopes_fixed </unpack>
    <pluginArgs>--seqFileDir @dataDir@/IEDB_Epitopes_fixed/fasta --externalDatabaseName %RESOURCE_NAME% --externalDatabaseVersion %RESOURCE_VERSION% --soTermName "protein_binding_site" --regexSourceId ">(\d+)|" --tableName "DoTS::MotifAASequence" --logFrequency 100 --regexTaxonName "\|.*\|(.*)" --regexName "\|(.*)\|.*"</pluginArgs>
    <info
        displayName="IEDB Epitopes"
        project="PlasmoDB"
        organisms="@PcOrganismFullName@"
        category="Proteome"
        publicUrl="http://www.immuneepitope.org"
        contact="IEDB"
        email=""
        institution="IEDB">
      <description>
        <![CDATA[
         IEDB epitopes.
        ]]>
      </description>
    </info>
  </resource>

