    <pluginArgs>--extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/PlasmoDB/pfDNABindingSite2gus.xml --inputFileOrDir @@dataDir@@/allSites_reformat.gff --fileFormat gff2 --soCvsVersion @@SO_VER@@  --gff2GroupTag ID --naSequenceSubclass ExternalNASequence --seqIdColumn source_id --seqExtDbName "Pvivax_contigsGB_RSRC"   --seqExtDbRlsVer @@Pvivax_contigsGB_VER@@</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertMassSpecFeaturesAndSummaries"
