<resources>
  <resource plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
            resource="Pvivax_contigsGB_RSRC"
            version="@@Pvivax_contigsGB_VER@@">
    <manualGet fileOrDir="PlasmoDB/pvivax/genome/contig/%RESOURCE_VERSION%/sequences.gb.gz" />
    <pluginArgs>--extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/PlasmoDB/PvivaxGenbank2gus.xml --inputFileOrDir @@dataDir@@/sequences.gb --fileFormat genbank --soCvsVersion @@SO_VER@@ --organism "Plasmodium vivax SaI-1"  --seqSoTerm "contig" --validationLog validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out</pluginArgs>
    <unpack>gunzip @@dataDir@@/sequences.gb.gz</unpack>
  </resource>
  <resource parentResource="Pvivax_contigsGB_RSRC"
            plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
            resource="Pvivax_mitochondrial_RSRC"
            version="@@Pvivax_mitochondrial_VER@@">
    <manualGet fileOrDir="PlasmoDB/pvivax/genome/mitochondrial/%RESOURCE_VERSION%/sequences.gb" />
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/organellarFeatures2gus.xml --inputFileOrDir @@dataDir@@/P.vivax_mitochondrial_fixed/sequences_fixed.gb --fileFormat genbank --soCvsVersion @@SO_VER@@ --organism "Plasmodium vivax SaI-1"  --seqSoTerm "mitochondrial_chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out</pluginArgs>
    <unpack>mkdir @@dataDir@@/P.vivax_mitochondrial_fixed</unpack>
    <unpack>cat @@dataDir@@/sequences.gb | perl -pe 's/protein_id=/secondary_id=/g' > @@dataDir@@/P.vivax_mitochondrial_fixed/sequences_fixed.gb</unpack>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile"
            resource="PvivaxChromosomeAGP_RSRC"
            version="@@PvivaxChromosomeAGP_VER@@">
    <manualGet fileOrDir="PlasmoDB/pvivax/genome/AGP/%RESOURCE_VERSION%/" />
    <pluginArgs>--agpFile @@dataDir@@/@@PvivaxChromosomeAGP_VER@@/AAKM00000000_20070904_abrev.agp --seqPieceExtDbName "Pvivax_contigsGB_RSRC" --seqPieceExtDbRlsVer @@Pvivax_contigsGB_VER@@ --virSeqExtDbName %RESOURCE_NAME% --virSeqExtDbRlsVer %RESOURCE_VERSION% --virtualSeqSOTerm "chromosome" --ncbiTaxId "126793" --soVer @@SO_VER@@ --chromosomeOrderMappingFile @@dataDir@@/@@PvivaxChromosomeAGP_VER@@/chromosomeOrderMapping.txt</pluginArgs>
  </resource>
  <resource plugin="GUS::Community::Plugin::InsertECMapping"
            resource="Pvivax_ECNumbers_RSRC"
            version="@@Pvivax_contigsGB_VER@@">
    <manualGet fileOrDir="PlasmoDB/pvivax/function/ECNumbers_Pvivax/PvivaxECNumbers" />
    <pluginArgs>--ECMappingFile @@dataDir@@/PvivaxECNumbers --evidenceCode "jCVI" --aaSeqLocusTagMappingSql "SELECT aa_sequence_id FROM dots.GeneFeature g, dots.Transcript t, dots.TranslatedAAFeature taf WHERE g.source_id = ? AND g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id"</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile"
            resource="Pvivax_ProductName_RSRC"
            version="@@Pvivax_contigsGB_VER@@">
    <manualGet fileOrDir="PlasmoDB/pvivax/genome/contig/%RESOURCE_VERSION%/sequences.gb" />
    <pluginArgs>--file @@dataDir@@/PvivaxProduct.tab --genomeDbName Pvivax_contigsGB_RSRC --genomeDbVer @@Pvivax_contigsGB_VER@@ </pluginArgs>
    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/sequences.gb --output_file @@dataDir@@/PvivaxProduct.tab --feat_with_geneid mRNA --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product</unpack>
    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/sequences.gb --output_file @@dataDir@@/PvivaxProduct.tab --feat_with_geneid pseudogenic_transcript --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product --append</unpack>
  </resource>
  <resource plugin="GUS::Supported::Plugin::LoadFastaSequences"
            resource="Pv_IEDB_Epitopes_RSRC"
            version="@@Pv_IEDB_Epitopes_VER@@">
    <getAndUnpackOutput dir="@@dataDir@@/IEDB_Epitopes_fixed/fasta" />
    <getAndUnpackOutput file="@@dataDir@@/IEDB_Epitopes_fixed/IEDBExportPviv.txt" />
    <getAndUnpackOutput file="@@dataDir@@/IEDB_Epitopes_fixed/fasta/IEDBExportPviv.fsa" />
    <manualGet fileOrDir="PlasmoDB/pvivax/epitope/IEDB/%RESOURCE_VERSION%/IEDBExportPviv.txt" />
    <pluginArgs>--seqFileDir @@dataDir@@/IEDB_Epitopes_fixed/fasta --externalDatabaseName %RESOURCE_NAME% --externalDatabaseVersion %RESOURCE_VERSION% --soTermName "protein_binding_site" --regexSourceId ">(\d+)|" --tableName "DoTS::MotifAASequence" --logFrequency 100 --regexNcbiTaxId "\|.*\|(.*)" --regexName "\|(.*)\|.*"</pluginArgs>
    <unpack>mkdir -p @@dataDir@@/IEDB_Epitopes_fixed/fasta</unpack>
    <unpack>extractFromIEDB --inputFile @@dataDir@@/IEDBExportPviv.txt --outputFile @@dataDir@@/IEDB_Epitopes_fixed/IEDBExportPviv.txt </unpack>
    <unpack>generateEpitopeFastaFile --inputFile @@dataDir@@/IEDB_Epitopes_fixed/IEDBExportPviv.txt --outputFile @@dataDir@@/IEDB_Epitopes_fixed/fasta/IEDBExportPviv.fsa </unpack>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Pv_IEDB_Epitopes_DBxRefs_RSRC"
            version="@@Pv_IEDB_Epitopes_VER@@">
    <manualGet fileOrDir="global/empty" />
    <pluginArgs>--DbRefMappingFile @@dataDir@@/dbRefs.txt --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier, remark" --tableName "AASequenceDbRef"</pluginArgs>
    <unpack>createEpitopeDbRefFile --inputDir @@dataDir@@/../Pv_IEDB_Epitopes_RSRC/IEDB_Epitopes_fixed --outputFile @@dataDir@@/dbRefs.txt</unpack>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::CorrectGeneAliases"
            resource="Pv_CorrectGeneAliases_RSRC"
            version="1">
    <manualGet fileOrDir="global/empty" />
    <pluginArgs>--extDbRelSpecList "Pvivax_contigsGB_RSRC|@@Pvivax_contigsGB_VER@@"</pluginArgs>
    <unpack>cleanUpDBRefNaFeature.pl</unpack>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
