  <resource plugin="GUS::Supported::Plugin::dbEST"
            resource="Pv_dbEST_RSRC"
            version="@@Pv_dbEST_VER@@">
    <manualGet fileOrDir="global/empty" />
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
    <unpack>getSubTaxa --ncbiTaxId 5855 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::LoadEstsFromFastaFile"
            resource="WatanabePv_PredictedTranscripts_RSRC"
            version="@@WatanabePv_PredictedTranscripts_VER@@">
    <manualGet fileOrDir="PlasmoDB/pvivax/EST/WatanabeESTs/%RESOURCE_VERSION%/reffull_pv.fas" />
    <pluginArgs>--sourceIdRegex "\|(\w+\(\S+\))$" --fastaFile  @@dataDir@@/reffull_pv.fas --externalDatabaseName %RESOURCE_NAME% --externalDatabaseVer %RESOURCE_VERSION%  --SOTermName 'transcript' --ncbiTaxId 5855  --libraryVector 'pME18SFL3' --contactName 'Junichi Watanabe' --contactAddress1 "Institute of Medical Science; The University of Tokyo, Department of Parasitology" --contactAddress2 "4-6-1, Shirokanedai, Minatoku, Tokyo 108-8639, Japan" --putativeFullLength --libraryName 'derivedFromWatanabePv' --libraryDesc "Predicted full length transcript sequence produced after BLAT alignment of the WatanabePv EST library sequences to the genome and correction of the cDNA sequences using annotated gene sequences."</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::LoadEstsFromFastaFile"
            resource="WatanabePv_EstLibrary_RSRC"
            version="2006-11-02">
    <manualGet fileOrDir="PlasmoDB/pvivax/EST/WatanabeESTs/%RESOURCE_VERSION%/onepass_pv.fas" />
    <pluginArgs>--sourceIdRegex "\|(\w+)$" --fastaFile  @@dataDir@@/onepass_pv.fas --externalDatabaseName %RESOURCE_NAME% --externalDatabaseVer %RESOURCE_VERSION%  --SOTermName 'EST' --ncbiTaxId 5855  --libraryVector 'pME18SFL3' --contactName 'Junichi Watanabe' --contactAddress1 "Institute of Medical Science; The University of Tokyo, Department of Parasitology" --contactAddress2 "4-6-1, Shirokanedai, Minatoku, Tokyo 108-8639, Japan" --libraryName 'WatanabePv' --libraryDesc "Blood samples were collected from the Indonesian patients with P. vivax who were diagnosed by Giemsa staining of the blood smear and gave the written informed consent. Heparinized whole blood was filtrated through Plasmodipur filter to remove white blood cells. Parasites were homogenized using the Polytron Homogenizer, frozen in liquid nitrogen and sent to Japan with dry ice."</pluginArgs>
  </resource>
  <resource plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
            resource="Pv_TransFactorBindingSites_Llinas_RSRC"
