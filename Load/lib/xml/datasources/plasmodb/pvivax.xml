<resources>

<!--ISFResource-->
  <resource resource="Pvivax_contigsGB_RSRC" version="@@Pvivax_contigsGB_VER@@"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="PlasmoDB/pvivax/genome/contig/%RESOURCE_VERSION%/sequences.gb.gz"/>
    <unpack>gunzip @@dataDir@@/sequences.gb.gz</unpack>
    <pluginArgs>--extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/PlasmoDB/PvivaxGenbank2gus.xml --inputFileOrDir @@dataDir@@/sequences.gb --fileFormat genbank --soCvsVersion @@SO_VER@@ --organism "Plasmodium vivax SaI-1"  --seqSoTerm "contig" --validationLog validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out</pluginArgs>
    <info
        displayName="P. vivax contig sequence and annotation from Genbank"
        project="PlasmoDB"
        organisms="@@PvOrganismFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         <i>P. vivax</i> , Salvador I isolate, contig sequence and annotation obtained from Genbank (modification date 2007-06-13) and submitted by Jane Carlton at The Institute for Genomic Research.
        ]]>
      </description>
    <publication pmid="18843361"/>
    </info>
  </resource>

  <resource resource="Pvivax_mitochondrial_RSRC" version="@@Pvivax_mitochondrial_VER@@" parentResource="Pvivax_contigsGB_RSRC"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="PlasmoDB/pvivax/genome/mitochondrial/%RESOURCE_VERSION%/sequences.gb"/>
        <unpack>mkdir @@dataDir@@/P.vivax_mitochondrial_fixed</unpack>
        <unpack>cat @@dataDir@@/sequences.gb | perl -pe 's/protein_id=/secondary_id=/g' > @@dataDir@@/P.vivax_mitochondrial_fixed/sequences_fixed.gb</unpack>
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/organellarFeatures2gus.xml --inputFileOrDir @@dataDir@@/P.vivax_mitochondrial_fixed/sequences_fixed.gb --fileFormat genbank --soCvsVersion @@SO_VER@@ --organism "Plasmodium vivax SaI-1"  --seqSoTerm "mitochondrial_chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out</pluginArgs>
    <info
        displayName="P. vivax strain Salvador I mitochondrial DNA sequence and annotation"
        project="PlasmoDB"
        organisms="@@PvOrganismFullName@@"
        category="Organellar"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         <i>P. vivax</i> mitochondrial sequence and annotation obtained from Genbank (modification date 2005-07-12) and submitted by Parasitology, Faculty of Medicine, Chulalongkorn University,  Thailand.
        ]]>
      </description>
    <publication pmid="15901839"/>
    </info>
  </resource>

