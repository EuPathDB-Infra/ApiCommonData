  <resource resource="Pk_IEDB_Epitopes_RSRC" version="@Pk_IEDB_Epitopes_VER@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="PlasmoDB/pknowlesi/epitope/IEDB/%RESOURCE_VERSION%"/>
