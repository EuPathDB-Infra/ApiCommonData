     <resource resource="pk_dbEST" version="2009-02-23"
       project="PlasmoDB"
        plugin="GUS::Supported::Plugin::dbEST"
        displayName="Plasmodium dbEST ESTs"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/">
        <manualGet fileOrDir = "common/empty"
        />
        <unpack>getSubTaxa --ncbiTaxId 5850 --outputFile @dataDir@/taxonIdFile</unpack>
        <unpack>createPropFile --file @dataDir@/dbest.prop --propList "dbestLogin:@dbestLogin@,dbestPswd:@dbestPswd@"</unpack>
        <pluginArgs>--span 500 --taxonFile @dataDir@/taxonIdFile --extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --soVer @soVer@ --dbestLoginFile @dataDir@/dbest.prop --dbestConnect "dbi:Oracle:musbld" </pluginArgs>
        <description>
            <![CDATA[
                  Plasmodium ESTs from dbEST.
          ]]>
        </description>
    </resource>

    <resource resource="P.knowlesi_chromosomes" version=@pkChromosomeVer@
