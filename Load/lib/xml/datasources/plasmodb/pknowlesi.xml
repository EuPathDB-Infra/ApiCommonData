   <resource resource="IEDB_Epitopes" version="2009-03-18"
        publicUrl="http://www.immuneepitope.org"
        displayName="IEDB Epitopes"
        category="Proteome"
        plugin="GUS::Supported::Plugin::LoadFastaSequences"
        <manualGet fileOrDir="PlasmoDB/pknowlesi/epitope/IEDB/2009-03-18" 
            contact="IEDB"
            email=""
            institution="IEDB"
        />
        <unpack>mkdir @dataDir@/IEDB_Epitopes_fixed</unpack>
        <unpack>createEpitopeFilesFromExcel --excelDir @dataDir@/2009-03-18 --outDir @dataDir@/IEDB_Epitopes_fixed </unpack>
        <pluginArgs>--seqFileDir @dataDir@/IEDB_Epitopes_fixed/fasta --externalDatabaseName %EXT_DB_NAME% --externalDatabaseVersion %EXT_DB_RLS_VER% --soTermName "protein_binding_site" --regexSourceId ">(\d+)|" --tableName "DoTS::MotifAASequence" --logFrequency 100 --regexTaxonName "\|.*\|(.*)" --regexName "\|(.*)\|.*"</pluginArgs>
        <description>
            <![CDATA[
   IEDB epitopes.
          ]]>
        </description>
    </resource>
    
