<resources>
  <resource resource="Pk_dbEST_RSRC" version="@Pk_dbEST_VER@"
      plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="common/empty"/>
    <unpack>getSubTaxa --ncbiTaxId 5850 --outputFile @dataDir@/taxonIdFile</unpack>
    <unpack>createPropFile --file @dataDir@/dbest.prop --propList "dbestLogin:@dbestLogin@,dbestPswd:@dbestPswd@"</unpack>
    <pluginArgs>--span 500 --taxonFile @dataDir@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @soVer@ --dbestLoginFile @dataDir@/dbest.prop --dbestConnect "dbi:Oracle:musbld" </pluginArgs>
    <info
        displayName="Plasmodium dbEST ESTs"
        project="PlasmoDB"
        organisms="@PkOrganismFullName@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Plasmodium ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Pknowlesi_chromosomes_RSRC" version="@Pknowlesi_chromosomes_VER@"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="PlasmoDB/pknowlesi/genome/chromosome/%RESOURCE_VERSION%/"/>
    <pluginArgs>--extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --mapFile isf/PlasmoDB/sangerFeatures2gus.xml --inputFileOrDir @dataDir@/%RESOURCE_VERSION%/ --inputFileExtension "embl" --fileFormat embl --soCvsVersion @soVer@ --defaultOrganism "Plasmodium knowlesi H"  --seqSoTerm "chromosome" --validationLog validation.log --bioperlTreeOutput bioperlTree.out</pluginArgs>
    <info
        displayName="P. knowlesi (H) chromosomes sequence and annotation"
        project="PlasmoDB"
        organisms="@PkOrganismFullName@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Chromosome sequences and annotation for <i>P. knowlesi</i> strain H obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute (February 2007)
        ]]>
      </description>
    </info>
  </resource>

