     <resource resource="dbEST" version="2009-02-23"
        plugin="GUS::Supported::Plugin::dbEST"
        displayName="Plasmodium dbEST ESTs"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/">
        <manualGet fileOrDir = "common/empty"
        />
        <unpack>getSubTaxa --ncbiTaxId 5850 --outputFile @dataDir@/taxonIdFile</unpack>
        <unpack>createPropFile --file @dataDir@/dbest.prop --propList "dbestLogin:@dbestLogin@,dbestPswd:@dbestPswd@"</unpack>
        <pluginArgs>--span 500 --taxonFile @dataDir@/taxonIdFile --extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --soVer @soVer@ --dbestLoginFile @dataDir@/dbest.prop --dbestConnect "dbi:Oracle:musbld" </pluginArgs>
        <description>
            <![CDATA[
                  Plasmodium ESTs from dbEST.
          ]]>
        </description>
    </resource>

    <resource resource="P.knowlesi_chromosomes" version="2010-02-01"
        plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
        organisms="P.knowlesi"
        displayName="P. knowlesi (H) chromosomes sequence and annotation" 
        category="Genome"> 
        <manualGet fileOrDir="PlasmoDB/pknowlesi/genome/chromosome/2010-02-01/"/>
        <pluginArgs>--extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --mapFile isf/PlasmoDB/sangerFeatures2gus.xml --inputFileOrDir @dataDir@/2010-02-01/ --inputFileExtension "embl" --fileFormat embl --soCvsVersion @soVer@ --defaultOrganism "Plasmodium knowlesi H" --handlerExternalDbs "enzyme:enzymeDB:2009-02-11" --seqSoTerm "chromosome" --validationLog validation.log --bioperlTreeOutput bioperlTree.out</pluginArgs>
        <description>
            <![CDATA[
                 Chromosome sequences and annotation for <i>P. knowlesi</i> strain H obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute (February 2007)
          ]]>
        </description>
    </resource> 


