    <resource resource="dbEST" version="2009-02-23"
        plugin="GUS::Supported::Plugin::dbEST"
        displayName="Plasmodium dbEST ESTs"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/">

        <manualGet fileOrDir = "/files/cbil/data/cbil/plasmoDB/manualDelivery/dbEST/dummy"
        />
        <unpack>getSubTaxa --ncbiTaxId 5854 --outputFile @downloadDir@/dbEST/taxonIdFile</unpack>
        <unpack>createPropFile --file dbest.prop --propList "dbestLogin:@dbestLogin@,dbestPswd:@dbestPswd@"</unpack>
        <pluginArgs>--span 500 --taxonFile @downloadDir@/dbEST/taxonIdFile --extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --soVer @soVer@ --dbestLoginFile dbest.prop --dbestConnect "dbi:Oracle:musbld" </pluginArgs>
        <description>
            <![CDATA[
                  Plasmodium ESTs from dbEST.
          ]]>
        </description>
    </resource>


    <resource resource="P.reichenowi.contigs" version="2004-03-11" 
        url="ftp://ftp.sanger.ac.uk/pub/pathogens/P_reichenowi/P_reichenowi.contigs.031104"
        plugin="GUS::Supported::Plugin::LoadFastaSequences"
        organisms="P.reichenowi"
        displayName="P. reichenowi contigs"
        category="Genome">
        <wgetArgs>--tries=5 --mirror --no-parent --no-directories --no-host-directories --cut-dirs=3 --output-document=P_reichenowi.contigs.031104</wgetArgs> 
        <unpack>mkdir @downloadDir@/P.reichenowi.contigs_fixed</unpack>
        <unpack>cat @downloadDir@/P.reichenowi.contigs/P_reichenowi.contigs.031104 | perl -pe 's/>/>Pr_/' > @downloadDir@/P.reichenowi.contigs_fixed/P_reichenowi.contigs.031104</unpack>
        <pluginArgs>--externalDatabaseName %EXT_DB_NAME% --ncbiTaxId 5854 --externalDatabaseVersion %EXT_DB_RLS_VER% --sequenceFile @downloadDir@/P.reichenowi.contigs_fixed/P_reichenowi.contigs.031104 --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
        <description>
            <![CDATA[ 
                 Contig sequences for <i>P. reichenowi</i> obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute
          ]]>      
        </description>  
    </resource>
    
