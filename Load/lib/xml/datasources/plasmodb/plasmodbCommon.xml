<resources>

    <resource resource="PlasmodiumIsolates" version="2009-04-13"
        project=PlasmoDB
        displayName="Plasmodium isolates from Genbank"
        category="Genome"
        organisms="Plasmodium"
        plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
           <manualGet fileOrDir="PlasmoDB/common/isolate/plasmoIsolatesFromGenbank/%EXT_DB_RLS_VER%/sequences_fixed_1.gb" 
            contact="GenBank"
            email="info@ncbi.nlm.nih.gov"
            institution="NCBI"
        />
        <pluginArgs>--extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --mapFile isf/plasmoIsolatesGenbank2gus.xml --inputFileOrDir @dataDir@/sequences_fixed_1.gb --fileFormat genbank --soCvsVersion @soVer@ --defaultOrganism "Plasmodium falciparum"  --seqSoTerm "region" --bioperlTreeOutput @dataDir@/bioperlTree.out</pluginArgs>
        <description>
            <![CDATA[
Plasmodium isolates from Genbank
          ]]>
        </description>
    </resource>

 <resource resource="isolateVocabularyMap_isolation_source" version="2009-06-14" 
        project=PlasmoDB
        displayName="update isolate vocabulary-isolation_source" 
        category="ignore" 
        plugin="ApiCommonData::Load::Plugin::LoadNothing"> 
        <manualGet fileOrDir = "PlasmoDB/common/isolate/isolate_ontology/%EXT_DB_RLS_VER%/isolationSource.xml" 
            contact = "Bindu Gajria" 
            email = "bindu@pcbi.upenn.edu" 
            institution = "University of Pennsylvania" />
        <unpack>isolateVocabulary.pl insert --gus_config_file @gusHome@/config/gus.config --type isolation_source --xml_file @dataDir@/isolationSource.xml</unpack>
        <pluginArgs>--dbRefExternalDatabaseSpec "%EXT_DB_NAME%|%EXT_DB_RLS_VER%"</pluginArgs>
    </resource> 

