    <resource resource="OrthoMCL_Phyletic" version="4"
        displayName="OrthoMCL phyletic profiles"
        organisms="P.falciparum"
        category="Proteome"
        plugin="ApiCommonData::Load::Plugin::InsertPhylogeneticProfile"
        publicUrl="http://orthomcl.cbil.upenn.edu">
           <manualGet fileOrDir="/files/cbil/data/cbil/plasmoDB/manualDelivery/orthoMCL-v3.0/orthomclGroups.txt"
            email="pinney@pcbi.upenn.edu"
            contact="Deborah Pinney"
            institution="PCBI"
        />
        <pluginArgs>--groupsFile @downloadDir@/OrthoMCL_Phyletic/orthomclGroups.txt --taxaToLoad "pber,pcha,pfal,pkno,pviv,pyoe"</pluginArgs>
        <description>
            <![CDATA[
OrthoMCL data in a phyletic profile format. Contact: Steve Fischer </tt>
          ]]>
        </description>
        <publication pmid="16381887"/>
    </resource>

<resource resource="OrthoMCL" version="4"
        displayName="Orthologous groups generated using OrthoMCL"
        organisms="P.falciparum"
        category="Proteome"
        publicUrl="http://orthomcl.cbil.upenn.edu"
        plugin="ApiCommonData::Load::Plugin::InsertOrthologousGroups">
        <manualGet fileOrDir="/files/cbil/data/cbil/plasmoDB/manualDelivery/orthoMCL-v3.0/orthomclGroups.txt"
            email="pinney@pcbi.upenn.edu"
            contact="Deborah Pinney"
            institution="PCBI"
        />
        <pluginArgs>--OrthologFile @downloadDir@/OrthoMCL/orthomclGroups.txt --AnalysisName 'OrthoMCL-based Orthology Predictions' --AnalysisDescription 'OrthoMCL-based Orthology Predictions'  --taxaToLoad "pber,pcha,pfal,pkno,pviv,pyoe" --ElementResultTable "DoTS::GeneFeature"</pluginArgs>
        <description>
            <![CDATA[
   OrthoMCL-based orthologous predictions for proteins were generated by Steve Fischer and Deborah Pinney (EuPathDB team) as described in Chen et al. Nucleic Acids Research 2006.
          ]]>
        </description>
        <publication pmid="12952885"/>
    </resource>

<resource resource="OrthoMCL" version="4"
        displayName="OrthoMCL phyletic pattern tree"
        organisms="P.falciparum"
        category="Proteome"
        plugin="ApiCommonData::Load::Plugin::InsertOrthomclTaxon"
        publicUrl="http://orthomcl.cbil.upenn.edu">
        <manualGet fileOrDir="PlasmoDB/common/phylogeny/OrthoMCLTaxon/4/"
            email="pinney@pcbi.upenn.edu"
            contact="Deborah Pinney"
            institution="PCBI"
        />
        <pluginArgs>--cladeFile @dataDir@/4/orthomclClades.txt --speciesFile @dataDir@/4/orthomclTaxons.txt </pluginArgs>
        <description>
            <![CDATA[
    Phyletic pattern tree for the organisms used in OrthoMCL.
          ]]>
        </description>
    </resource>



