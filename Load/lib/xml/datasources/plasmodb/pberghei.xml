<resources>
  <resource plugin="GUS::Supported::Plugin::LoadFastaSequences"
            resource="Pberghei_chromosomes_RSRC"
            version="@@Pberghei_chromosomes_VER@@">
    <manualGet fileOrDir="PlasmoDB/pberghei/genome/chromosome/%RESOURCE_VERSION%/gff" />
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 5823 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/P.berghei.chromosomes_fixed/Pberghei.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence" --regexChromosome '\|(\w+)'</pluginArgs>
    <unpack>mkdir @@dataDir@@/P.berghei.chromosomes_fixed</unpack>
    <unpack>makeCustomFastaAndGffFromGff3 --input_dir @@dataDir@@/gff --inputFileExt gff --output_fasta @@dataDir@@/P.berghei.chromosomes_fixed/Pberghei.fasta --output_gff  @@dataDir@@/P.berghei.chromosomes_fixed/Pberghei.gff3</unpack>
  </resource>
  <resource parentResource="Pberghei_chromosomes_RSRC"
            plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
            resource="Pberghei_annotations_RSRC"
            version="@@Pberghei_chromosomes_VER@@">
    <manualGet fileOrDir="PlasmoDB/pberghei/genome/chromosome/%RESOURCE_VERSION%/gff" />
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/sangerGFF2Gus.xml --inputFileOrDir @@dataDir@@/P.berghei.chromosomes_fixed/Pberghei.gff --inputFileExtension "gff"  --fileFormat gff3 --soCvsVersion @@SO_VER@@ --defaultOrganism "Plasmodium berghei strain ANKA"  --seqSoTerm "chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
    <unpack>mkdir @@dataDir@@/P.berghei.chromosomes_fixed</unpack>
    <unpack>makeCustomFastaAndGffFromGff3 --input_dir @@dataDir@@/gff --inputFileExt gff --output_fasta @@dataDir@@/P.berghei.chromosomes_fixed/Pberghei.fasta --output_gff  @@dataDir@@/P.berghei.chromosomes_fixed/Pberghei.gff3</unpack>
    <unpack>preprocessGFF3 --input_gff @@dataDir@@/P.berghei.chromosomes_fixed/Pberghei.gff3 --output_gff @@dataDir@@/P.berghei.chromosomes_fixed/Pberghei.gff</unpack>
  </resource>
  <resource plugin="GUS::Community::Plugin::InsertECMapping"
            resource="Pberghei_ECNumbers_RSRC"
            version="@@Pberghei_chromosomes_VER@@">
    <manualGet fileOrDir="PlasmoDB/pberghei/genome/chromosome/%RESOURCE_VERSION%/gff" />
    <pluginArgs>--ECMappingFile @@dataDir@@/Pberghei.ec --evidenceCode "GeneDB" --aaSeqLocusTagMappingSql "SELECT aa_sequence_id FROM dots.GeneFeature g, dots.Transcript t, dots.TranslatedAAFeature taf WHERE g.source_id = ? AND g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id"</pluginArgs>
    <unpack>makeCustomFastaAndGffFromGff3 --input_dir @@dataDir@@/gff --output_fasta @@dataDir@@/Pberghei.fasta --output_gff @@dataDir@@/Pberghei.gff3</unpack>
    <unpack>preprocessGFF3 --input_gff @@dataDir@@/Pberghei.gff3 --output_gff @@dataDir@@/Pberghei.gff</unpack>
    <unpack>makeECFile --format gff3 --fileOrDir @@dataDir@@/Pberghei.gff --id_qualifier ID --ec_qualifier eC_number --output_file @@dataDir@@/Pberghei.ec </unpack>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertGOAssociationsSimple"
            resource="GOAssociations_Pberghei_RSRC"
            version="@@Pberghei_chromosomes_VER@@">
    <manualGet fileOrDir="PlasmoDB/pberghei/function/@@Pberghei_chromosomes_VER@@/GOAssociations_Pberghei/Pberghei.GO" />
    <pluginArgs>--externalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%" --inputFile @@dataDir@@/Pberghei.GO --goExternalDatabaseSpec "GO_RSRC|@@GO_VER@@" --seqIdSql "SELECT g.source_id, taf.aa_sequence_id FROM dots.genefeature g, dots.transcript t, dots.translatedAAFeature taf WHERE g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id UNION SELECT taf.source_id, taf.aa_sequence_id FROM dots.translatedAAFeature taf" --targetTable "DoTS::TranslatedAASequence" --lineOfEvidence "GeneDB" --inputIdColumn id --tolerateMissingSeqs --skipBadGOTerms @@dataDir@@/badGOTerms.log</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile"
            resource="Pberghei_ProductName_RSRC"
            version="@@Pberghei_chromosomes_VER@@">
    <manualGet fileOrDir="PlasmoDB/pberghei/genome/chromosome/%RESOURCE_VERSION%/gff/" />
    <pluginArgs>--file @@dataDir@@/PbergheiProduct.tab --genomeDbName Pberghei_chromosomes_RSRC --genomeDbVer @@Pberghei_chromosomes_VER@@ </pluginArgs>
    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/*.gff --output_file @@dataDir@@/PbergheiProduct.tab --feat_with_geneid mRNA --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product</unpack>
    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/*.gff --output_file @@dataDir@@/PbergheiProduct.tab --feat_with_geneid pseudogenic_transcript --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product --append</unpack>
  </resource>
  <resource plugin="GUS::Supported::Plugin::LoadFastaSequences"
            resource="Pb_IEDB_Epitopes_RSRC"
            version="@@Pb_IEDB_Epitopes_VER@@">
    <getAndUnpackOutput dir="@@dataDir@@/IEDB_Epitopes_fixed/fasta" />
    <getAndUnpackOutput file="@@dataDir@@/IEDB_Epitopes_fixed/IEDBExportPber.txt" />
    <getAndUnpackOutput file="@@dataDir@@/IEDB_Epitopes_fixed/fasta/IEDBExportPber.fsa" />
    <manualGet fileOrDir="PlasmoDB/pberghei/epitope/IEDB/%RESOURCE_VERSION%/IEDBExportPber.txt" />
    <pluginArgs>--seqFileDir @@dataDir@@/IEDB_Epitopes_fixed/fasta --externalDatabaseName %RESOURCE_NAME% --externalDatabaseVersion %RESOURCE_VERSION% --soTermName "protein_binding_site" --regexSourceId ">(\d+)|" --tableName "DoTS::MotifAASequence" --logFrequency 100 --regexNcbiTaxId "\|.*\|(.*)" --regexName "\|(.*)\|.*"</pluginArgs>
    <unpack>mkdir -p @@dataDir@@/IEDB_Epitopes_fixed/fasta</unpack>
    <unpack>extractFromIEDB --inputFile @@dataDir@@/IEDBExportPber.txt --outputFile @@dataDir@@/IEDB_Epitopes_fixed/IEDBExportPber.txt </unpack>
    <unpack>generateEpitopeFastaFile --inputFile @@dataDir@@/IEDB_Epitopes_fixed/IEDBExportPber.txt --outputFile @@dataDir@@/IEDB_Epitopes_fixed/fasta/IEDBExportPber.fsa </unpack>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Pb_IEDB_Epitopes_DBxRefs_RSRC"
