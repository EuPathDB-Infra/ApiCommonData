<resources>
  <resource plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
            resource="PfalciparumIT_genome_RSRC"
            version="@@PfalciparumIT_genome_VER@@">
    <manualGet fileOrDir="PlasmoDB/pfalciparumIT/genome/%RESOURCE_VERSION%/final/genome.embl" />
    <pluginArgs>--organism "Plasmodium falciparum IT" --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --fileFormat embl --inputFileOrDir @@dataDir@@/genome.embl --mapFile @@gusHome@@/lib/xml/isf/PlasmoDB/PfalciparumITEmbl2gus.xml --soCvsVersion @@SO_VER@@ --bioperlTreeoutput @@dataDir@@/bioperlTree.out --validationLog @@dataDir@@/validation.log --seqSoTerm chromosome</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile"
            resource="PfalciparumIT_ProductName_RSRC"
            version="@@PfalciparumIT_genome_VER@@">
    <manualGet fileOrDir="PlasmoDB/pfalciparumIT/function/PfalciparumIT_genome_RSRC_product/%RESOURCE_VERSION%/product.txt" />
    <pluginArgs>--file @@dataDir@@/product.txt --productDbName PfalciparumIT_genome_RSRC --productDbVer @@PfalciparumIT_genome_VER@@</pluginArgs>
  </resource>
  <resource plugin="GUS::Supported::Plugin::dbEST"
            resource="PfalciparumIT_dbEST_RSRC"
            version="@@PfalciparumIT_dbEST_VER@@">
    <manualGet fileOrDir="global/empty" />
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
    <unpack>getSubTaxa --ncbiTaxId 5833 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
  </resource>
</resources>
