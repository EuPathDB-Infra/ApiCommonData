  <resource resource="TparvaMuguga_genomes_RSRC" version="@@TparvaMuguga_genomes_VER@@"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="PiroplasmaDB/tparvaMuguga/genome/tparvaMuguga/%RESOURCE_VERSION%/gb/"/>
    <pluginArgs>--organism "Theileria parva strain Muguga" --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --fileFormat genbank --inputFileOrDir @@dataDir@@/gb --mapFile @@gusHome@@/lib/xml/isf/PiroplasmaDB/piroGenbank2Gus.xml --soCvsVersion @@SO_VER@@ --bioperlTreeoutput @@dataDir@@/bioperlTree.out --validationLog @@dataDir@@/validation.log --seqSoTerm contig</pluginArgs>
    <info
        displayName="TparvaMuguga sequence and annotation"
        project="PiroplasmaDB"
        organisms="@@TparvaMugugaOrganismFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
genomes.
        ]]>
      </description>
    </info>
  </resource>


  <resource resource="TparvaMuguga_ECNumbers_RSRC" version="@@TparvaMuguga_genomes_VER@@"
      plugin="GUS::Community::Plugin::InsertECMapping">
    <manualGet fileOrDir="PiroplasmaDB/tparvaMuguga/function/Tparva_genome_RSRC_ecnumbers/%RESOURCE_VERSION%/ec.txt"/>
    <pluginArgs>--ECMappingFile @@dataDir@@/ec.txt --evidenceCode "GeneDB" --aaSeqLocusTagMappingSql "SELECT aa_sequence_id FROM dots.GeneFeature g, dots.Transcript t, dots.TranslatedAAFeature taf WHERE g.source_id = ? AND g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id"</pluginArgs>
    <info
        displayName="EC Number Mappings"
        project="PiroplasmaDB"
        organisms="@@TparvaMugugaOrganismFullName@@"
        category="Proteome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         EC Number mapping.
        ]]>
      </description>
    </info>
  </resource>

