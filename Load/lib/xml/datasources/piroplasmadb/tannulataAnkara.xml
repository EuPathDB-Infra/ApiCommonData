    <info
        displayName="TannulataAnkara sequence and annotation"
        project="PiroplasmaDB"
        organisms="@@TannulataAnkaraOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[

        ]]>
      </description>
    </info>
  </resource>


  <resource resource="TannulataAnkara_ECNumbers_RSRC" version="@@TannulataAnkara_genomes_VER@@"
