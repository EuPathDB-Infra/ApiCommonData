    <info
        displayName="EC Number Mappings"
        project="PiroplasmaDB"
        organisms="@@TannulataAnkaraOrganismFullName@@"
        category="Proteome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         EC Number mapping.
        ]]>
      </description>
    </info>
  </resource>

