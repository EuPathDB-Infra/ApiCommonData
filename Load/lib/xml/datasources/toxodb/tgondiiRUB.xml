<resources>

<resource resource="TgondiiRUB_genome_RSRC" version="@@TgondiiRUB_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgondiiRUB/genome/scaffolds/%RESOURCE_VERSION%/fromProvider/tgrub_scaffolds.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 935652 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/tgrub_scaffolds.fsa --SOTermName 'supercontig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii RUB scaffoldss"
        project="ToxoDB"
        organisms="@@TgondiiRUBFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Scaffold sequences for <i>Toxoplasma gondii RUB</i> obtained from Washington University in St. Louis 
        ]]>
      </description>
    </info>
  </resource>

<resource resource="TgondiiRUB_contigs_RSRC" version="@@TgondiiRUB_genome_VER@@" parentResource="TgondiiRUB_genome_RSRC"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgondiiRUB/genome/contigs/%RESOURCE_VERSION%/fromProvider/tgrub_degens.fsa"/>
    <pluginArgs>--externalDatabaseName %PARENT_RESOURCE_NAME% --ncbiTaxId 935652 --externalDatabaseVersion %PARENT_RESOURCE_VERSION% --sequenceFile @@dataDir@@/tgrub_degens.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii RUB contigs"
        project="ToxoDB"
        organisms="@@TgondiiRUBFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Toxoplasma gondii RUB</i> obtained from Washington University in St. Louis 
        ]]>
      </description>
    </info>
  </resource>


</resources>



