<resources>


<resource resource="TgondiiVAND_genome_RSRC" version="@@TgondiiVAND_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgondiiVAND/genome/scaffolds/%RESOURCE_VERSION%/fromProvider/tgvand_scaffolds.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 933077 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/tgvand_scaffolds.fsa --SOTermName 'supercontig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii VAND scaffolds"
        project="ToxoDB"
        organisms="@@TgondiiVANDFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Scaffold sequences for <i>Toxoplasma gondii VAND</i> obtained from Washington University in St. Louis 
        ]]>
      </description>
    </info>
  </resource>

<resource resource="TgondiiVAND_contigs_RSRC" version="@@TgondiiVAND_genome_VER@@" parentResource="TgondiiVAND_genome_RSRC"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgondiiVAND/genome/contigs/%RESOURCE_VERSION%/fromProvider/tgvand_orphans.fsa"/>
    <pluginArgs>--externalDatabaseName %PARENT_RESOURCE_NAME% --ncbiTaxId 933077 --externalDatabaseVersion %PARENT_RESOURCE_VERSION% --sequenceFile @@dataDir@@/tgvand_orphans.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii VAND contigs"
        project="ToxoDB"
        organisms="@@TgondiiVANDFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Toxoplasma gondii VAND</i> obtained from Washington University in St. Louis 
        ]]>
      </description>
    </info>
  </resource>

</resources>

