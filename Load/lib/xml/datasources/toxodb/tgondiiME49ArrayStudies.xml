    <info
        displayName="Toxoplasma gondii ME49 Boothroyd data"
        project="ToxoDB"
        organisms="Toxoplasma gondii ME49"
        category="Transcriptome"
        publicUrl=""
        contact="John Boothroyd"
        email=""
        institution="">
       <description>
        <![CDATA[
Expression profiling of Tgondii ME49 Boothroyd data
        ]]>
      </description>
<publication pmid=""/>
    </info>
  </resource>

