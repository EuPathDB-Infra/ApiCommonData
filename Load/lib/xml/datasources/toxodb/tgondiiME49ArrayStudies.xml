<resources>

<!--arrayStudies-->

  <resource resource="tgME49Boothroyd_RSRC" version="@@tgME49Boothroyd_VER@@"
      plugin="ApiCommonData::Load::Plugin::LoadNothing">
    <manualGet fileOrDir="ToxoDB/tgondiiME49/microarrayExpression/Boothroyd_DBP/1.0/final"/>
    <getAndUnpackOutput dir="@@dataDir@@/final"/>
    <getAndUnpackOutput file="@@dataDir@@/final/profiles.txt"/>
    <getAndUnpackOutput file="@@dataDir@@/final/profiles.txt.pct"/>
    <getAndUnpackOutput file="@@dataDir@@/final/Tgondiia520372.cdf"/>
    <getAndUnpackOutput file="@@dataDir@@/final/expression_profile_config.txt"/>
    <pluginArgs>--dbRefExternalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%"</pluginArgs>
    <info
        displayName="Toxoplasma gondii ME49 Boothroyd data"
        project="ToxoDB"
        organisms="Toxoplasma gondii ME49"
        category="Transcriptome"
        publicUrl=""
        contact="John Boothroyd"
        email=""
        institution="">
       <description>
        <![CDATA[
Expression profiling of Tgondii ME49 Boothroyd data
        ]]>
      </description>
<publication pmid=""/>
    </info>
  </resource>

  <resource resource="TgonME49_lorenzi_CosmidEnds_RSRC" version="2011-05-04"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgondiiME49/genome/cosmid_ends/%RESOURCE_VERSION%/final/jtc_fasta_TOXOPLASMA-F-01-40KB_1129290704345_1304110565918_1.WGS.fasta.seq"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --externalDatabaseVersion %RESOURCE_VERSION% --ncbiTaxId 508771 --sequenceFile @@dataDir@@/jtc_fasta_TOXOPLASMA-F-01-40KB_1129290704345_1304110565918_1.WGS.fasta.seq --soTermName "clone_insert_end"   --regexSourceId "^>(TOXOPLASMA-F-01-40KB_\\d+)\\|clear_start=\\d+\\|clear_end=\\d+\\|primer_id=\\d+\\|trace_id=\\d+\\|trace_file_id=\\d+\\|clone_insert_id=\\w+\\|"  --regexSecondaryId "^>TOXOPLASMA-F-01-40KB_\\d+\\|clear_start=\\d+\\|clear_end=\\d+\\|primer_id=\\d+\\|trace_id=\\d+\\|trace_file_id=\\d+\\|clone_insert_id=(\\w+)\\|" -tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="TgonME49 Cosmid Ends from Hernan Lorenzi"
        project="ToxoDB"
        organisms="Toxoplasma gondii ME49"
        category="Genome"
        publicUrl=""
        contact="Hernan Lorenzi"
        email="hlorenzi@jcvi.org"
        institution="jcvi">
      <description>
        <![CDATA[

        ]]>
      </description>
    </info>
  </resource>


  <resource resource="Toxoplasma_RFLP_Su_RSRC" version="2011-05-05"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgondiiME49/isolate/ChunleiSu_RFLP/%RESOURCE_VERSION%/final/140RFLPGenotypes.txt"/>
    <unpack>createRFLPGff --input_file @@dataDir@@/140RFLPGenotypes.txt --output_fasta  @@dataDir@@/RFLPs.fasta --output_gff @@dataDir@@/RFLPs.gff</unpack>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 5811 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/RFLPs.fasta --SOTermName 'region'  --regexSourceId "^>(.*)" --tableName "DoTS::ExternalNASequence" </pluginArgs>
    <info
        displayName="T. gondii RFLP data"
        project="ToxoDB"
        organisms="Toxoplasma"
        category="Genome"
        publicUrl=""
        contact="Chunlei Su"
        email="csu1@utk.edu"
        institution="UTK">
      <description>
        <![CDATA[
  <i>T. gondii</i> RFLP sequences (Chunlei Su lab)
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Toxoplasma_RFLP_feature_Su_RSRC" version="2011-05-05" parentResource="Toxoplasma_RFLP_Su_RSRC"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="global/empty"/>
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/ToxoDB/toxoIsolatesGenbank2gus.xml --inputFileOrDir @@dataDir@@/../Toxoplasma_RFLP_Su_RSRC/RFLPs.gff --inputFileExtension "gff"  --fileFormat gff2 --gff2GroupTag locus_tag --soCvsVersion @@SO_VER@@ --defaultOrganism "Toxoplasma gondii"  --seqSoTerm "chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
    <info
        displayName="T. gondii RFLP data"
        project="ToxoDB"
        organisms="Toxoplasma"
        category="Ignore"
        publicUrl=""
        contact="Chunlei Su"
        email="csu1@utk.edu"
        institution="UTK">
      <description>
        <![CDATA[
  <i>T. gondii</i> RFLP sequences (Chunlei Su lab).
        ]]>
      </description>
    </info>
  </resource>


</resources>
   