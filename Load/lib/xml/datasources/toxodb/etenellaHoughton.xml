<resources>

<!--ISFResource-->

  <resource resource="EtenellaHoughton_genome_RSRC" version="@@EtenellaHoughton_genome_VER@@"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="ToxoDB/etenellaHoughton/genome/%RESOURCE_VERSION%/final/genome.embl"/>
    <pluginArgs>--organism "Eimeria tenella str Houghton" --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --fileFormat embl --inputFileOrDir @@dataDir@@/genome.embl --mapFile @@gusHome@@/lib/xml/isf/ToxoDB/etenellaEmbl2Gus.xml --soCvsVersion @@SO_VER@@ --bioperlTreeoutput @@dataDir@@/bioperlTree.out --validationLog @@dataDir@@/validation.log --seqSoTerm supercontig</pluginArgs>
  </resource>



  <resource resource="EtenellaHoughton_ProductName_RSRC" version="@@EtenellaHoughton_genome_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile">
    <manualGet fileOrDir="ToxoDB/etenellaHoughton/function/EtenellaHoughton_genome_RSRC_product/%RESOURCE_VERSION%/product.txt"/>
    <pluginArgs>--file @@dataDir@@/product.txt --productDbName EtenellaHoughton_genome_RSRC --productDbVer @@EtenellaHoughton_genome_VER@@</pluginArgs>
  </resource>

  <resource resource="EtenellaHoughton_Comment_RSRC" version="@@EtenellaHoughton_genome_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertNaFeatCommentFromTabFile">
    <manualGet fileOrDir="ToxoDB/etenellaHoughton/genome/EtenellaHoughton_genome_RSRC_geneComment/%RESOURCE_VERSION%/comments.txt"/>
    <pluginArgs>--file @@dataDir@@/comments.txt --genomeDbName EtenellaHoughton_genome_RSRC --genomeDbVer @@EtenellaHoughton_genome_VER@@ </pluginArgs>
  </resource>

  <resource resource="EtenellaHoughton_previds_dbxref_RSRC" version="@@EtenellaHoughton_genome_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertDBxRefs">
    <manualGet fileOrDir="ToxoDB/etenellaHoughton/dbxref/EtenellaHoughton_genome_RSRC/%RESOURCE_VERSION%/EtenellaHoughton_previds.txt"/>
    <pluginArgs>--DbRefMappingFile @@dataDir@@/EtenellaHoughton_previds.txt --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'EtenellaHoughton_genome_RSRC|@@EtenellaHoughton_genome_VER@@' --idType 'previous id'</pluginArgs>
  </resource>

<!--ESTResource-->
  <resource resource="EtenellaHoughton_dbEST_RSRC" version="@@EtenellaHoughton_dbEST_VER@@"
      plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId 5802 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
  </resource>


<!--Proteome-->

  <resource resource="EtenellaHoughton_IEDB_Epitopes_RSRC" version="@@EtenellaHoughton_IEDB_Epitopes_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/common/epitope/IEDB/%RESOURCE_VERSION%/final/IEDBExportEtenellaHoughton.txt"/>
    <unpack>mkdir -p @@dataDir@@/IEDB_Epitopes_fixed/fasta</unpack>
    <unpack>extractFromIEDB --inputFile @@dataDir@@/IEDBExportEtenellaHoughton.txt --outputFile @@dataDir@@/IEDB_Epitopes_fixed/IEDBExportEtenellaHoughton.txt </unpack>
    <unpack>generateEpitopeFastaFile --inputFile @@dataDir@@/IEDB_Epitopes_fixed/IEDBExportEtenellaHoughton.txt --outputFile @@dataDir@@/IEDB_Epitopes_fixed/fasta/IEDBExportEtenellaHoughton.txt </unpack>
    <getAndUnpackOutput dir="@@dataDir@@/IEDB_Epitopes_fixed"/>
    <getAndUnpackOutput file="@@dataDir@@/IEDB_Epitopes_fixed/IEDBExportEtenellaHoughton.txt"/>
    <pluginArgs>--seqFileDir @@dataDir@@/IEDB_Epitopes_fixed/fasta --externalDatabaseName %RESOURCE_NAME% --externalDatabaseVersion %RESOURCE_VERSION% --soTermName "protein_binding_site" --regexSourceId ">(\d+)|" --tableName "DoTS::MotifAASequence" --logFrequency 100 --regexNcbiTaxId "\|.*\|(.*)" --regexName "\|(.*)\|.*"</pluginArgs>
  </resource>

  <resource resource="EtenellaHoughton_IEDB_Epitopes_DBxRefs_RSRC" version="@@EtenellaHoughton_IEDB_Epitopes_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertDBxRefs">
    <manualGet fileOrDir="global/empty"/>
    <unpack>createEpitopeDbRefFile --inputDir @@dataDir@@/../EtenellaHoughton_IEDB_Epitopes_RSRC/IEDB_Epitopes_fixed --outputFile @@dataDir@@/dbRefs.txt</unpack>
    <pluginArgs>--DbRefMappingFile @@dataDir@@/dbRefs.txt --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier, remark" --tableName "AASequenceDbRef"</pluginArgs>
  </resource>

</resources>
