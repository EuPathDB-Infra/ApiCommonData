<resources>

  <resource resource="TgondiiCatBR9
_genome_RSRC" version="@@TgondiiCatBR9_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgonCatBR9/genome/contig_NCBI_genbank/%RESOURCE_VERSION%/final/wgs.AFHV.contig.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 943120 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/wgs.AFHV.contig.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii CatBR9 genome contigs"
        project="ToxoDB"
        organisms="@@TgondiiCatBR9FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Genome contig sequences for <i>Toxoplasma gondii CatBR9</i> obtained from NCBI 
        ]]>
      </description>
    </info>
  </resource>

<resource resource="TgondiiCatBR9_apicoplast_RSRC" version="@@TgondiiCatBR9_genome_VER@@" parentResource="TgondiiCatBR9_genome_RSRC"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgonCatBR9/genome/apicoplast_NCBI_genbank/%RESOURCE_VERSION%/final/wgs.AFHV.apicoplast.fsa"/>
    <pluginArgs>--externalDatabaseName %PARENT_RESOURCE_NAME% --ncbiTaxId 943120 --externalDatabaseVersion %PARENT_RESOURCE_VERSION% --sequenceFile @@dataDir@@/wgs.AFHV.apicoplast.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii CatBR9 apicoplast contigs"
        project="ToxoDB"
        organisms="@@TgondiiCatBR9FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Apicoplast contig sequences for <i>Toxoplasma gondii CatBR9</i> obtained from NCBI 
        ]]>
      </description>
    </info>
  </resource>

</resources>



