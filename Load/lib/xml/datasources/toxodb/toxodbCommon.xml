<resources>

   <resource resource="ToxoIsolatesFromGenbank" version="2009-10-09"
        displayName="T. gondii and N. caninum isolates from Genbank"
        category="Genome"
        organisms="Toxoplasma"
        plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
        extDbName="Toxoplasma isolates From Genbank"
        extDbRlsVer="2009-10-09"
        extDbRlsDescrip="Toxoplasma isolates From Genbank|2009-10-09"
        dbCommit="@dbcommit@">
           <manualGet fileOrDir="/files/cbil/data/cbil/toxoDb/manualDelivery/ToxoIsolatesFromGenbank/10-09-2009/sequences.gb" 
            contact="GenBank"
            email="info@ncbi.nlm.nih.gov"
            institution="NCBI"
        />
        <unpack>fixIsolateSequences --seq_file @downloadDir@/ToxoIsolatesFromGenbank/sequences.gb --fixed_seq_file @downloadDir@/ToxoIsolatesFromGenbank/sequences_fixed.gb --removed_seq_file @downloadDir@/ToxoIsolatesFromGenbank/sequences_excluded.gb</unpack>
        <unpack>sed "s/plastid Toxoplasma gondii/Toxoplasma gondii/g" @downloadDir@/ToxoIsolatesFromGenbank/sequences_fixed.gb | sed "s/apicoplast Neospora caninum/Neospora caninum/g" | sed "s/mitochondrion Toxoplasma gondii/Toxoplasma gondii/g" | sed "s/plastid Neospora caninum/Neospora caninum/g" > @downloadDir@/ToxoIsolatesFromGenbank/sequences_fixed.gb.1</unpack>
        <pluginArgs>--extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --mapFile @gusHome@/lib/xml/isf/GenbankIsolates2gus.xml --inputFileOrDir @downloadDir@/ToxoIsolatesFromGenbank/sequences_fixed.gb.1 --fileFormat genbank --soCvsVersion @soVer@ --defaultOrganism "Toxoplasma gondii" --handlerExternalDbs "enzyme:enzymeDB:2009-09-24"  --seqSoTerm "region" --bioperlTreeOutput @pipelineDir@/logs/Toxoisolates.bioperl</pluginArgs>
        <description>
            <![CDATA[
<i>Toxoplasma gondii</i> and <i>Neospora caninum</i> isolate data was obtained from Genbank.  
          ]]>
        </description>
    </resource>


   <resource resource="ToxoIsolatesFromDardre" version="2009-05-04"
        displayName="T. gondii isolates"
        category="Ignore"
        organisms="Toxoplasma"
        plugin="ApiCommonData::Load::Plugin::UpdateIsolates"
        extDbName="Toxoplasma isolates From Genbank"
        extDbRlsVer="2009-05-04"
        extDbRlsDescrip="Toxoplasma isolates From Genbank|2009-05-04"
        dbCommit="@dbcommit@">
           <manualGet fileOrDir="/files/cbil/data/cbil/toxoDb/manualDelivery/Isolates/Marie-Laure/ToxoDB_Microsatellites.txt" 
            contact="GenBank"
            email="info@ncbi.nlm.nih.gov"
            institution="NCBI"
        />
          <pluginArgs>--extDbRlsSpec '%EXT_DB_NAME%|%EXT_DB_RLS_VER%' --inputFile @downloadDir@/ToxoIsolatesFromDardre/ToxoDB_Microsatellites.txt</pluginArgs>
        <description>
            <![CDATA[
Several isolate records were obtained from GenBank and updated with additional information provided by the authors.  See references for additional information.Toxoplasma isolates from Marie-Laure Dardre.
          ]]>
        </description>
    </resource>


   <resource resource="SibleyToxoIsolateSequences" version="2009-05-04"
        displayName="T. gondii isolates (Sibley)"
        category="Genome"
        organisms="Toxoplasma"
        plugin="GUS::Supported::Plugin::LoadFastaSequences"
        extDbName="Toxoplasma isolates From David Sibley"
        extDbRlsVer="2009-05-04"
        extDbRlsDescrip="Toxoplasma isolates From David Sibley|2009-05-04"
        dbCommit="@dbcommit@">
           <manualGet fileOrDir="/files/cbil/data/cbil/toxoDb/manualDelivery/Isolates/Sibley/3-23-09/" 
            contact="David Sibley"
            email="khan@borcim.wustl.edu"
            institution="WUSTL"
        />
        <unpack>createSibleyIsolateGff --strain_file  @downloadDir@/SibleyToxoIsolateSequences/3-23-09/Strains_list.tab --input_dir @downloadDir@/SibleyToxoIsolates/3-23-09/ --output_gff @downloadDir@/SibleyToxoIsolateSequences/sibleyIsolates.gff </unpack>
        <pluginArgs>--externalDatabaseName %EXT_DB_NAME% --externalDatabaseVersion %EXT_DB_RLS_VER% --ncbiTaxId 5811 --sequenceFile @downloadDir@/SibleyToxoIsolateSequences/3-23-09/isolates_fixed.fasta --soTermName "region" --regexSourceId "^>(\S+)" --tableName "DoTS::ExternalNASequence"</pluginArgs>

        <description>
            <![CDATA[
   Toxoplasma isolates and sequences obtained from David Sibley (Contact: L. David Sibley sibley&#64;borcim&#46;wustl&#46;edu)..
          ]]>
        </description>
    </resource>

   <resource resource="SibleyToxoIsolates" version="2009-05-04"
        displayName="Toxoplasma isolates from David Sibley"
        category="ignore"
        organisms="Toxoplasma"
        plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
        extDbName="Toxoplasma isolates From David Sibley"
        extDbRlsVer="2009-05-04"
        extDbRlsDescrip="Toxoplasma isolates From David Sibley|2009-05-04"
        dbCommit="@dbcommit@">
           <manualGet fileOrDir="@downloadDir@/SibleyToxoIsolateSequences/sibleyIsolates.gff" 
            contact="David Sibley"
            email="khan@borcim.wustl.edu"
            institution="WUSTL"
        />

<pluginArgs> --extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --mapFile @gusHome@/lib/xml/isf/ToxoDB/toxoIsolatesGenbank2gus.xml --inputFileOrDir @downloadDir@/SibleyToxoIsolates/sibleyIsolates.gff --fileFormat gff2 --gff2GroupTag locus_tag --naSequenceSubclass ExternalNASequence --seqIdColumn source_id  --soCvsVersion @soVer@ --defaultOrganism "Toxoplasma gondii" --handlerExternalDbs "enzyme:enzymeDB:2008-07-01"  --bioperlTreeOutput @pipelineDir@/logs/SibleyToxoisolates.bioperl</pluginArgs>
        <description>
            <![CDATA[
Toxoplasma isolates from David Sibley Lab.
          ]]>
        </description>
    </resource>


</resources>
