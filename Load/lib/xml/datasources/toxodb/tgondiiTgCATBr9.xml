<resources>

  <resource resource="TgondiiTgCATBr9_genome_RSRC" version="@@TgondiiTgCATBr9_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgonTgCATBr9/genome/contig_NCBI_genbank/%RESOURCE_VERSION%/final/wgs.AFHV.contig.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 943120 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/wgs.AFHV.contig.fsa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii TgCATBr9 genome contigs"
        project="ToxoDB"
        organisms="@@TgondiiTgCATBr9FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Genome contig sequences for <i>Toxoplasma gondii TgCATBr9</i> obtained from NCBI 
        ]]>
      </description>
    </info>
  </resource>

<resource resource="TgondiiTgCATBr9_apicoplast_RSRC" version="@@TgondiiTgCATBr9_genome_VER@@" parentResource="TgondiiTgCATBr9_genome_RSRC"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgonTgCATBr9/genome/apicoplast_NCBI_genbank/%RESOURCE_VERSION%/final/wgs.AFHV.apicoplast.fsa"/>
    <pluginArgs>--externalDatabaseName %PARENT_RESOURCE_NAME% --ncbiTaxId 943120 --externalDatabaseVersion %PARENT_RESOURCE_VERSION% --sequenceFile @@dataDir@@/wgs.AFHV.apicoplast.fsa --SOTermName 'apicoplast_chromosome'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii TgCATBr9 apicoplast contigs"
        project="ToxoDB"
        organisms="@@TgondiiTgCATBr9FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Apicoplast contig sequences for <i>Toxoplasma gondii TgCATBr9</i> obtained from NCBI 
        ]]>
      </description>
    </info>
  </resource>

</resources>



