<resources>

  <resource resource="Nc_RNASeq_Tachyzoite_Reid_RSRC" version="@@Nc_RNASeq_Tachyzoite_Reid_VER@@"
      plugin="ApiCommonData::Load::Plugin::LoadNothing">
    <manualGet fileOrDir="ToxoDB/ncaninum/rnaSeq/tachy/Reid/%RESOURCE_VERSION%"/> 
    <getAndUnpackOutput file="@@dataDir@@/config.txt"/>
    <getAndUnpackOutput file="@@dataDir@@/3250_5_1.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/3250_5_2.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/3250_6_1.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/3250_6_2.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/3563_2_1.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/3563_2_2.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/3563_3_1.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/3563_3_2.fastq"/>
    <pluginArgs>--dbRefExternalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%"</pluginArgs>
    <info
        displayName="Ncaninum tachyzoite RNA-Seqs from Adam Reid"
        project="ToxoDB"
        organisms="@@NcaninumFullName@@"
        category="Genome"
        publicUrl=""
        contact="Adam Reid"
        email="ap2@sanger.ac.uk"
        institution="Sanger">
      <description>
        <![CDATA[
        ]]>
      </description>
    </info>
  </resource>

