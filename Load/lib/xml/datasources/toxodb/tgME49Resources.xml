

  <resource resource="TgME49_RNASeq_Tachyzoite_Reid_RSRC" version="@@TgME49_RNASeq_Tachyzoite_Reid_VER@@"
      plugin="ApiCommonData::Load::Plugin::LoadNothing">
    <manualGet fileOrDir="ToxoDB/tgondiiVEG/rnaSeq/tachy/Reid/%RESOURCE_VERSION%/fromProvider"/>
    <getAndUnpackOutput file="@@dataDir@@/fromProvider/tgME49config.txt"/>
    <getAndUnpackOutput file="@@dataDir@@/fromProvider/3152_3_1.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/fromProvider/3152_3_2.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/fromProvider/3152_5_1.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/fromProvider/3152_5_2.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/fromProvider/4137_7_1.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/fromProvider/4137_7_2.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/fromProvider/4137_8_1.fastq"/>
    <getAndUnpackOutput file="@@dataDir@@/fromProvider/4137_8_2.fastq"/>
    <pluginArgs>--dbRefExternalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%"</pluginArgs>
    <info
        displayName="Tgondii VEG tachyzoite RNA-Seqs from Adam Reid"
        project="ToxoDB"
        organisms="@@TgME49OrganismFullName@@"
        category="Genome"
        publicUrl=""
        contact="Adam Reid"
        email="ap2@sanger.ac.uk"
        institution="Sanger">
      <description>
        <![CDATA[
				Transcriptome analysis of T. gondii VEG and N. caninum Liverpool tachyzoites using RNAseq RNA sequencing data provided by Adam Reid, Mandy Sanders, Mike Quail, Matthew Berriman, Arnab Pain (Wellcome Trust Sanger Institute) Rebecca Norton, Dong Xia, Sarah Vermont and Jonathan Wastling (University of Liverpool). A paper describing our analysis of this data (Reid et al.) is currently in preparation.  
        ]]>
      </description>
    </info>
  </resource>

