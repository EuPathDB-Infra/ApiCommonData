<resources>


<resource resource="Tgondiip89_genome_RSRC" version="@@Tgondiip89_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgondiip89/genome/scaffolds/%RESOURCE_VERSION%/fromProvider/tgp89Scaffolds.fasta"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 943119 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/tgp89Scaffolds.fasta --SOTermName 'supercontig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii p89 scaffoldss"
        project="ToxoDB"
        organisms="@@Tgondiip89FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Scaffold sequences for <i>Toxoplasma gondii p89</i> obtained from Washington University in St. Louis 
        ]]>
      </description>
    </info>
  </resource>

<resource resource="Tgondiip89_contigs_RSRC" version="@@Tgondiip89_genome_VER@@" parentResource="Tgondiip89_genome_RSRC"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgondiip89/genome/contigs/%RESOURCE_VERSION%/fromProvider/tgp89OrphanContigs.fasta"/>
    <pluginArgs>--externalDatabaseName %PARENT_RESOURCE_NAME% --ncbiTaxId 943119 --externalDatabaseVersion %PARENT_RESOURCE_VERSION% --sequenceFile @@dataDir@@/tgp89OrphanContigs.fasta --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii p89 contigs"
        project="ToxoDB"
        organisms="@@Tgondiip89FullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Toxoplasma gondii p89</i> obtained from Washington University in St. Louis 
        ]]>
      </description>
    </info>
  </resource>

</resources>



