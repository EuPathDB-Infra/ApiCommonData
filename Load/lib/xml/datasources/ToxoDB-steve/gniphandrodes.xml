<resources>

  <constant name="orgAbbrev" value="gnip???"/>
  <constant name="project" value="ToxoDB"/>
  <constant name="ncbiTaxId" value="110365"/>
  <constant name="genomeRSRC" value="$$orgAbbrev$$_genome_WashU_contigs_RSRC"/>

  <!-- OldName: Gregarina_niphandrodes_genome_RSRC    OldVersion:  -->
  <resourceInstance class="fastaSequence">
    <prop name="name">WashU_contigs</prop>
    <prop name="version"></prop>
    <prop name="category">genome</prop>
    <prop name="table">DoTS::ExternalNaSequence</prop>
    <prop name="soTermName">contig</prop>
    <prop name="regexSourceId">(\S+)</prop>
  </resourceInstance>
  <resource resource="$$orgAbbrev$$_genome_WashU_sequence_RSRC" version="@@Gregarina_niphandrodes_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/gniphandrodes/genome/contigs/%RESOURCE_VERSION%/final/Gniphandrodes_asmblAndDegen.fsa"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 110365 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Gniphandrodes_asmblAndDegen.fsa --SOTermName 'contig'  --regexSourceId  '(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Gregarina niphandrodes contigs"
        project="ToxoDB"
        organisms="@@GniphandrodesFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Gregarina niphandrodes</i> obtained from Washington University in St. Louis 
        ]]>
      </description>
    </info>
  </resource>

</resources>




