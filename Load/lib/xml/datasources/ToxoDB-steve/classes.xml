      <manualGet fileOrDir="$$project$$/$$orgAbbrev$$/isolate/$$name$$/%RESOURCE_VERSION%/final/isolates.gff"/>

      <pluginArgs> --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --mapFile @gusHome@/lib/xml/isf/$$mapFile$$ --inputFileOrDir @@dataDir@@/isolates.gff --fileFormat gff2 --gff2GroupTag locus_tag --naSequenceSubclass ExternalNASequence --seqIdColumn source_id  --soCvsVersion @SO_VER@ --defaultOrganism $$ncbiTaxId --bioperlTreeOutput @@dataDir@@/bioperlTree.out</pluginArgs>
    </resource>
  </resourceClass>

  <resourceClass class="massSpecPreprocessed">
    <prop name="project"/>
    <prop name="orgAbbrev"/>
    <prop name="name"/>
    <prop name="version"/>
    <prop name="flag"/>
    <resource resource="$$orgAbbrev$$_massSpec_Wastling-Rhoptry_RSRC" version="2007-05-11"
	      plugin="ApiCommonData::Load::Plugin::InsertMassSpecPreprocessed"
	      parentResource="$$genomeRSRC$$"
	      internalDescrip="">

      <manualGet fileOrDir="$$project$$/$$orgAbbrev$$/massSpec/$$name$$/%RESOURCE_VERSION%/final"/>

      <pluginArgs>--inputDir @@dataDir@@/final --externalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%" --geneExternalDatabaseSpec '%PARENT_RESOURCE_NAME%|%PARENT_RESOURCE_VERSION%' $$flag$$ </pluginArgs>
    </resource>
  </resourceClass>


</resourceClasses>
