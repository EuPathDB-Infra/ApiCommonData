    <prop name="loadProductName">true</prop>
    <prop name="distanceBetweenStartsForAlignmentsInSameCluster">30000</prop>
    <prop name="minOrfLength">50</prop>
    <prop name="deprecated">false</prop>
    <prop name="soTermIdsOrNames">chromosome</prop>
    <prop name="makeESTDownloadFile">true</prop>
    <prop name="makeProteomeDownloadFile">true</prop>
    <prop name="makeGenomicDownloadFile">true</prop>
    <prop name="organelleInitDownloadDirSubgraph">initMitoAndApicoplastDownloadDirs.xml</prop>
