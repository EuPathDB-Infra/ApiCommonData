  <resource resource="$$orgAbbrev_epitope_IEDB_RSRC"
	    version="@@$$orgAbbrev$$_epitope_IEDB_VER@@"
	    plugin="GUS::Supported::Plugin::LoadFastaSequences">

    <manualGet fileOrDir="global/empty"/>

    <!-- validate that IEDB knows about this taxon id -->
    <unpack><![CDATA[unzip -p @@manualDelivery@@/common/epitope/IEDB/@@$$orgAbbrev$$_epitope_IEDB_VER@@/fromProvider/OrganismList.zip | grep "<OrganismId>$$speciesNcbiTaxId$$<"]]></unpack>

