<resources>

  <constant name="orgAbbrev" value="tgonVEG"/>
  <constant name="project" value="ToxoDB"/>
  <constant name="ncbiTaxId" value="432359"/>
  <constant name="genomeRSRC" value="$$orgAbbrev$$_genome_JCVI_genbank_RSRC"/>

 
  <!-- OldName: T.gondiiVEG_contigsGB   OldVersion: 2008-07-23 -->
  <resourceInstance class="genbank_genome">
    <prop name="name">JCVI</prop>
    <prop name="version">2008-07-23</prop>
    <prop name="soTerm">supercontig</prop>
    <prop name="mapFile">ToxoDB/toxoGenbank2gus.xml</prop>
  </resourceInstance>
  <resource resource="T.gondiiVEG_contigsGB" version="@@_VER@@"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
      internalDescrip="T.gondii VEG contig sequence and annotation from Genbank on 2008-07-23">
    <manualGet fileOrDir="/files/cbil/data/cbil/toxoDb/manualDelivery/AnnotationFromJCVI/new_snapshot_07-17-2008/tgac1.20080717.gbf"/>
    <pluginArgs>--extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --mapFile @gusHome@/lib/xml/isf/ToxoDB/toxoGenbank2gus.xml --inputFileOrDir @downloadDir@/T.gondiiVEG_contigsGB/tgac1.20080717.gbf --fileFormat genbank --soCvsVersion @soVer@ --organism "Toxoplasma gondii VEG" --handlerExternalDbs "enzyme:enzymeDB:2008-07-01" --seqSoTerm "supercontig"</pluginArgs>
    <info
        displayName="T. gondii VEG contig sequence and annotation from Genbank"
        project=""
        organism=""
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
Toxoplasma gondii VEG sequence and annotation from Lis Caler at the J. Craig Venter Institute (<a href="http://msc.jcvi.org/t_gondii/index.shtml"Target="_blank">JCVI</a>).
        ]]>
      </description>
    </info>
  </resource>


  <!-- OldName: VEGChromosomeMap   OldVersion: 2008-08-28 -->
  <resourceInstance class="genome_chromosome_assembly_agp">
    <prop name="version">2008-08-28</prop>
    <prop name="virtualSeqSOTerm">chromosome</prop>
    <prop name="sourceIdPrefix">TGVEG_chr</prop>
    <prop name="parentResource">$$genomeRSRC$$</prop>
  </resourceInstance>
  <resource resource="VEGChromosomeMap" version="@@_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile"
      internalDescrip="">
    <manualGet fileOrDir="/files/cbil/data/cbil/toxoDb/manualDelivery/ChromosomeMap/agp/veg.agp"/>
    <pluginArgs>--agpFile @downloadDir@/VEGChromosomeMap/veg.agp --seqPieceExtDbName "T.gondii VEG contigs from Genbank" --seqPieceExtDbRlsVer "2008-07-23" --virSeqExtDbName %EXT_DB_NAME% --virSeqExtDbRlsVer %EXT_DB_RLS_VER% --virtualSeqSOTerm "chromosome" --ncbiTaxId "432359" --soVer @soVer@ --sourceIdPrefix 'TGVEG_chr'</pluginArgs>
    <info
        displayName="T. gondii VEG Chromosome Map"
        project=""
        organism=""
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
Chromosome map generated by David Sibley for <i>T. gondii</i> ME49 strain was used as a template to creat <i>T. gondii</i> VEG chromosomes.  Chromosomes we assembled by the EuPathDB team using <a href="http://www.biostat.wisc.edu/~cdewey/mercator/" TARGET="_blank">Mercator</a> and <a href="http://genome.cshlp.org/content/14/4/693.abstract" TARGET="_blank">MAVID</a>.
        ]]>
      </description>
    </info>
  </resource>

</resources>

  <!-- OldName: dbEST   OldVersion: 2009-09-25 -->
  <resourceInstance class="dbEST"/>
  <resource resource="dbEST" version="@@_VER@@"
      plugin="GUS::Supported::Plugin::dbEST"
      internalDescrip="ESTs from the mirrored dbEST">
    <manualGet fileOrDir="/files/cbil/data/cbil/toxoDb/manualDelivery/dbEST/dummy"/>
    <unpack>getSubTaxa --ncbiTaxId 5810 --outputFile @downloadDir@/dbEST/taxonIdFile</unpack>
    <unpack>createPropFile --file @pipelineDir@/config/dbestProp --propList "dbestLogin:@dbestLogin@,dbestPswd:@dbestPswd@"</unpack>
    <pluginArgs>--span 500 --taxonFile @downloadDir@/dbEST/taxonIdFile --extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --soVer @soVer@ --dbestLoginFile @pipelineDir@/config/dbestProp --dbestConnect "dbi:Oracle:musbld" </pluginArgs>
    <info
        displayName="dbEST ESTs"
        project=""
        organism=""
        category="Transcriptome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
ESTs from the mirrored <a href="http://www.ncbi.nlm.nih.gov/dbEST/"TARGET="_blank">dbEST</a> site.
        ]]>
      </description>
    </info>
  </resource>

