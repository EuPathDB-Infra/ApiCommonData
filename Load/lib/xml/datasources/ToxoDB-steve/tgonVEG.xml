<resources>

  <constant name="orgAbbrev" value="tgonVEG"/>
  <constant name="project" value="ToxoDB"/>
  <constant name="ncbiTaxId" value="432359"/>
  <constant name="genomeRSRC" value="$$orgAbbrev$$_genome_JCVI_genbank_RSRC"/>

 
  <!-- OldName: T.gondiiVEG_contigsGB   OldVersion: 2008-07-23 -->
  <resourceInstance class="genbank_genome">
    <prop name="name">JCVI</prop>
    <prop name="version">2008-07-23</prop>
    <prop name="soTerm">supercontig</prop>
    <prop name="mapFile">ToxoDB/toxoGenbank2gus.xml</prop>
  </resourceInstance>


  <!-- OldName: VEGChromosomeMap   OldVersion: 2008-08-28 -->
  <resourceInstance class="genome_chromosome_assembly_agp">
    <prop name="version">2008-08-28</prop>
    <prop name="virtualSeqSOTerm">chromosome</prop>
    <prop name="sourceIdPrefix">TGVEG_chr</prop>
    <prop name="parentResource">$$genomeRSRC$$</prop>
  </resourceInstance>

  <!-- OldName: dbEST   OldVersion: 2009-09-25 -->
  <resourceInstance class="dbEST"/>

</resources>


