<resources>

  <constant name="orgAbbrev" value="tgonFOU"/>
  <constant name="project" value="ToxoDB"/>
  <constant name="ncbiTaxId" value="943167"/>
  <constant name="genomeRSRC" value="$$orgAbbrev$$_genome_WashU_scaffolds_RSRC"/>

  <!-- OldName: Toxoplasma_gondii_FOU_genome_RSRC    OldVersion:  -->
  <resourceInstance class="fastaSequence">
    <prop name="name">WashU_scaffolds</prop>
    <prop name="version"></prop>
    <prop name="category">genome</prop>
    <prop name="table">DoTS::ExternalNaSequence</prop>
    <prop name="soTermName">scaffold</prop>
    <prop name="regexSourceId">(\S+)</prop>
  </resourceInstance>
  <resource resource="Toxoplasma_gondii_FOU_genome_RSRC" version="@@Toxoplasma_gondii_FOU_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgondiiFOU/genome/scaffolds/%RESOURCE_VERSION%/from_provider/tgfouScaffolds.fasta"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 943167 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/tgfouScaffolds.fasta --SOTermName 'scaffold'  --regexSourceId  '(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii FOU scaffoldss"
        project="ToxoDB"
        organisms="@@TgondiiFOUFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Scaffold sequences for <i>Toxoplasma gondii FOU</i> obtained from Washington University in St. Louis 
        ]]>
      </description>
    </info>
  </resource>

  <!-- OldName: Toxoplasma_gondii_FOU_contigs_RSRC    OldVersion:  -->
  <resourceInstance class="fastaSequence">
    <prop name="name">WashU_congtigs</prop>
    <prop name="version"></prop>
    <prop name="category">genome</prop>
    <prop name="table">DoTS::ExternalNaSequence</prop>
    <prop name="soTermName">contig</prop>
    <prop name="regexSourceId">(\S+)</prop>
  </resourceInstance>
<resource resource="Toxoplasma_gondii_FOU_contigs_RSRC" version="@@Toxoplasma_gondii_FOU_genome_VER@@" parentResource="Toxoplasma_gondii_FOU_genome_RSRC"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="ToxoDB/tgondiiFOU/genome/contigs/%RESOURCE_VERSION%/from_provider/tgfouOrphanContigs.fasta"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 943167 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/tgfouOrphanContigs.fasta --SOTermName 'contig'  --regexSourceId  '(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Toxoplasma gondii FOU contigs"
        project="ToxoDB"
        organisms="@@TgondiiFOUFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Toxoplasma gondii FOU</i> obtained from Washington University in St. Louis 
        ]]>
      </description>
    </info>
  </resource>

</resources>



