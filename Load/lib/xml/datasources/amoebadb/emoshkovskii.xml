<resources>

<resource resource="Emoshkovskii_genome_RSRC" version="@@Emoshkovskii_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="AmoebaDB/emoshkovskii/genome/contigs/%RESOURCE_VERSION%/fromProvider/454AllContigs.fna"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 41668 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/454AllContigs.fna --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Entamoeba moshkovskii contigs"
        project="AmoebaDB"
        organisms="@@EmoshkovskiiFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Entamoeba moshkovskii</i> obtained from the University of Liverpool 
        ]]>
      </description>
    </info>
  </resource>

<resource resource="EmoshkovskiiScaffoldAGP_RSRC" version="@@EmoshkovskiiScaffoldAGP_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile">
    <manualGet fileOrDir="AmoebaDB/emoshkovskii/genome/agp/%RESOURCE_VERSION%/fromProvider/454Scaffolds.txt"/>
    <pluginArgs>--agpFile @@dataDir@@/454Scaffolds.txt --seqPieceExtDbName "Emoshkovskii_genome_RSRC" --seqPieceExtDbRlsVer @@Emoshkovskii_genome_VER@@ --virSeqExtDbName %RESOURCE_NAME% --virSeqExtDbRlsVer %RESOURCE_VERSION% --virtualSeqSOTerm "supercontig" --ncbiTaxId "41668" --soVer @@SO_VER@@ --notChromosomes</pluginArgs>
    <info
        displayName="Entamoeba moshkovskii AGP map to scaffolds"
        project="AmoebaDB"
        organisms="@@EmoshkovskiiFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig to scaffold map for <i>Entamoeba moshkovskii</i> obtained from the University of Liverpool 
        ]]>
      </description>
    </info>
  </resource>

</resources>



