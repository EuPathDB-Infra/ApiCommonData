<resources>

<resource resource="EhistolyticaRahman_genome_RSRC" version="@@EhistolyticaRahman_genome_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="AmoebaDB/ehistolyticaRahman/genome/contigs/%RESOURCE_VERSION%/fromProvider/454AllContigs.fna"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 10000001 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/454AllContigs.fna --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="Entamoeba histolytica Rahman contigs"
        project="AmoebaDB"
        organisms="@@EhistolyticaRahmanFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig sequences for <i>Entamoeba histolytica Rahman</i> obtained from the University of Liverpool
        ]]>
      </description>
    </info>
  </resource>

<resource resource="EhistolyticaRahmanScaffoldAGP_RSRC" version="@@EhistolyticaRahmanScaffoldAGP_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile">
    <manualGet fileOrDir="AmoebaDB/ehistolyticaRahman/genome/agp/%RESOURCE_VERSION%/fromProvider/454Scaffolds.txt"/>
    <pluginArgs>--agpFile @@dataDir@@/454Scaffolds.txt --seqPieceExtDbName "EhistolyticaRahman_genome_RSRC" --seqPieceExtDbRlsVer @@EhistolyticaRahman_genome_VER@@ --virSeqExtDbName %RESOURCE_NAME% --virSeqExtDbRlsVer %RESOURCE_VERSION% --virtualSeqSOTerm "supercontig" --ncbiTaxId "10000001" --soVer @@SO_VER@@ --notChromosomes</pluginArgs>
    <info
        displayName="Entamoeba histolytica Rahman contig map to scaffolds"
        project="AmoebaDB"
        organisms="@@EhistolyticaRahmanFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Contig to scaffold map for <i>Entamoeba histolytica Rahman</i> obtained from the University of Liverpool 
        ]]>
      </description>
    </info>
  </resource>

</resources>



