<resources>
<!--ISFResource-->

  <resource resource="Tvivax_chromosomes_RSRC" version="@@Tvivax_chromosomes_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="TriTrypDB/tvivax/genome/chromosome/%RESOURCE_VERSION%/Tvivax.fasta"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 5699 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Tvivax.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence" --regexChromosome '\|(\d+)'</pluginArgs>
    <info
        displayName="T. vivax chromosomes sequence and annotation"
        project="TriTrypDB"
        organisms="@@TvivaxOrganismFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
                 Chromosome sequences for <i>T.vivax</i> obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute. Please visit <a href="http://www.genedb.org/Homepage/Tvivax">GeneDB</a> for project details and data release policies.
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Tvivax_annotations_RSRC" version="@@Tvivax_chromosomes_VER@@" parentResource="Tvivax_chromosomes_RSRC"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="TriTrypDB/tvivax/genome/chromosome/%RESOURCE_VERSION%/Tvivax.gff3"/>
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/sangerGFF32Gus.xml --inputFileOrDir @@dataDir@@/Tvivax.gff3 --inputFileExtension "gff"  --fileFormat gff3 --soCvsVersion @@SO_VER@@ --defaultOrganism "Trypanosoma vivax"  --seqSoTerm "chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
    <info
        displayName="T. vivax chromosomes sequence and annotation"
        project="TriTrypDB"
        organisms="@@TvivaxOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
                  This resource includes chromosome and unassigned contig sequences and annotations for <i>Trypanosoma vivax</i> obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute. Please visit <a href="http://www.genedb.org/Homepage/Tvivax">GeneDB</a> for project details and data release policies.
        ]]>
      </description>
    </info>
  </resource>

   <resource resource="GOAssociations_Tvivax_RSRC" version="@@Tvivax_chromosomes_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGOAssociationsSimple">
    <manualGet fileOrDir="TriTrypDB/tvivax/function/GOAssociations_Tvivax/%RESOURCE_VERSION%/Tvivax.gas"/>
    <pluginArgs>--externalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%" --inputFile @@dataDir@@/Tvivax.gas --goExternalDatabaseSpec "GO_RSRC|@@GO_VER@@" --seqIdSql "SELECT g.source_id, taf.aa_sequence_id FROM dots.genefeature g, dots.transcript t, dots.translatedAAFeature taf WHERE g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id UNION SELECT taf.source_id, taf.aa_sequence_id FROM dots.translatedAAFeature taf" --targetTable "DoTS::TranslatedAASequence" --lineOfEvidence "GeneDB" --inputIdColumn id --tolerateMissingSeqs --skipBadGOTerms @@dataDir@@/badGOTerms.log</pluginArgs>
    <info
        displayName="GO Association for T.vivax genes"
        project="TriTrypDB"
        organisms="@@TvivaxOrganismFullName@@"
        category="Proteome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         GO Associations for <i>T. vivax</i> genes.
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Tvivax_ProductName_RSRC" version="@@Tvivax_chromosomes_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile">
    <manualGet fileOrDir="TriTrypDB/tvivax/genome/chromosome/%RESOURCE_VERSION%/Tvivax.gff3"/>
    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/Tvivax.gff3 --output_file @@dataDir@@/TvivaxProduct.tab --feat_with_geneid mRNA --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product</unpack>
    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/Tvivax.gff3 --output_file @@dataDir@@/TvivaxProduct.tab --feat_with_geneid pseudogenic_transcript --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product --append</unpack>
    <pluginArgs>--file @@dataDir@@/TvivaxProduct.tab --genomeDbName Tvivax_chromosomes_RSRC --genomeDbVer @@Tvivax_chromosomes_VER@@ </pluginArgs>
    <info
        displayName="Tvivax Product Name"
        project="TriTrypDB"
        organisms="@@TvivaxOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Tvivax_Comment_RSRC" version="@@Tvivax_chromosomes_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertNaFeatCommentFromTabFile">
    <manualGet fileOrDir="TriTrypDB/tvivax/genome/chromosome/%RESOURCE_VERSION%/Tvivax.gff3"/>
    <unpack>makeCommentFile --format gff3 --fileOrDir @@dataDir@@/Tvivax.gff3 --output_file @@dataDir@@/TvivaxComment.tab --feat_with_geneid mRNA --geneid_qualifier Parent --feat_with_comment polypeptide --commentfeatid_qualifier ID --comment_qualifier comment</unpack>
    <unpack>makeCommentFile --format gff3 --fileOrDir @@dataDir@@/Tvivax.gff3 --output_file @@dataDir@@/TvivaxComment.tab --feat_with_geneid pseudogenic_transcript --geneid_qualifier Parent --feat_with_comment polypeptide --commentfeatid_qualifier ID --comment_qualifier comment --append</unpack>
    <pluginArgs>--file @@dataDir@@/TvivaxComment.tab --genomeDbName Tvivax_chromosomes_RSRC --genomeDbVer @@Tvivax_chromosomes_VER@@ </pluginArgs>
    <info
        displayName="Tvivax Comments"
        project="TriTrypDB"
        organisms="@@TvivaxOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         
        ]]>
      </description>
    </info>
  </resource>

<!--ESTResource-->

  <resource resource="Tvivax_dbEST_RSRC" version="@@Tvivax_dbEST_VER@@"
      plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId 5699 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
    <info
        displayName="TriTryp dbEST ESTs"
        project="TriTrypDB"
        organisms="@@TvivaxFullName@@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         TriTryp ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>

<!--DbxRefsResource-->
