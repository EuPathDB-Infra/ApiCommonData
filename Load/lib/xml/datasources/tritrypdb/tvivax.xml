    <info
        displayName="dbxref from entrez gene ids to eupathdb gene identifiers"
        project="TriTrypDB"
        organisms="@@TvivaxOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[         
        ]]>
      </description>
    </info>
  </resource>


