<!--ISFResource-->

  <resource resource="Tvivax_chromosomes_RSRC" version="@@Tvivax_chromosomes_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="TriTrypDB/tvivax/genome/chromosome/%RESOURCE_VERSION%/gff"/>
    <unpack>mkdir @@dataDir@@/Tvivax_chromosomes_fixed</unpack>
    <unpack>makeCustomFastaAndGffFromGff3 --input_dir @@dataDir@@/gff --inputFileExt gff --output_fasta @@dataDir@@/Tvivax_chromosomes_fixed/Tvivax.fasta --output_gff  @@dataDir@@/Tvivax_chromosomes_fixed/Tvivax.gff3</unpack>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 5699 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Tvivax_chromosomes_fixed/Tvivax.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)\|' --tableName "DoTS::ExternalNASequence" --regexChromosome '\|(\d+)'</pluginArgs>
    <info
        displayName="T. vivax chromosomes sequence and annotation"
        project="TriTrypDB"
        organisms="@@TvivaxOrganismFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
                 Chromosome sequences for <i>T.vivax</i> obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute. Please visit <a href="http://www.genedb.org/Homepage/Tvivax">GeneDB</a> for project details and data release policies.
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Tvivax_annotations_RSRC" version="@@Tvivax_chromosomes_VER@@" parentResource="Tvivax_chromosomes_RSRC"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="TriTrypDB/tvivax/genome/chromosome/%RESOURCE_VERSION%/gff"/>
    <unpack>mkdir @@dataDir@@/Tvivax_chromosomes_fixed</unpack>
    <unpack>makeCustomFastaAndGffFromGff3 --input_dir @@dataDir@@/gff --inputFileExt gff --output_fasta @@dataDir@@/Tvivax_chromosomes_fixed/Tvivax.fasta --output_gff  @@dataDir@@/Tvivax_chromosomes_fixed/Tvivax.gff3</unpack>
    <unpack>preprocessGFF3 --input_gff @@dataDir@@/Tvivax_chromosomes_fixed/Tvivax.gff3 --output_gff @@dataDir@@/Tvivax_chromosomes_fixed/Tvivax.gff</unpack>  
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/geneDBGFF2Gus.xml --inputFileOrDir @@dataDir@@/Tvivax_chromosomes_fixed/Tvivax.gff --inputFileExtension "gff"  --fileFormat gff3 --soCvsVersion @@SO_VER@@ --defaultOrganism "Trypanosoma vivax"  --seqSoTerm "chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
    <info
        displayName="T. vivax chromosomes sequence and annotation"
        project="TriTrypDB"
        organisms="@@TvivaxOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
                  This resource includes chromosome and unassigned contig sequences and annotations for <i>Trypanosoma vivax</i> obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute. Please visit <a href="http://www.genedb.org/Homepage/Tvivax">GeneDB</a> for project details and data release policies.
        ]]>
      </description>
    </info>
  </resource>

   <resource resource="GOAssociations_Tvivax_RSRC" version="@@Tvivax_chromosomes_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGOAssociationsSimple">
    <manualGet fileOrDir="TriTrypDB/tvivax/function/GOAssociations_Tvivax/%RESOURCE_VERSION%/Tvivax.gas"/>
    <pluginArgs>--externalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%" --inputFile @@dataDir@@/Tvivax.gas --goExternalDatabaseSpec "GO_RSRC|@@GO_VER@@" --seqIdSql "SELECT g.source_id, taf.aa_sequence_id FROM dots.genefeature g, dots.transcript t, dots.translatedAAFeature taf WHERE g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id UNION SELECT taf.source_id, taf.aa_sequence_id FROM dots.translatedAAFeature taf" --targetTable "DoTS::TranslatedAASequence" --lineOfEvidence "GeneDB" --inputIdColumn id --tolerateMissingSeqs --skipBadGOTerms @@dataDir@@/badGOTerms.log</pluginArgs>
    <info
        displayName="GO Association for T.vivax genes"
        project="TriTrypDB"
        organisms="@@TvivaxOrganismFullName@@"
        category="Proteome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         GO Associations for <i>T. vivax</i> genes.
        ]]>
      </description>
    </info>
  </resource>

