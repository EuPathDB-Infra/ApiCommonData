<resources>

  <resource plugin="GUS::Supported::Plugin::LoadFastaSequences"
            resource="ltarParrotTraII_primary_genome_RSRC"
            version="@@ltarParrotTraII_genome_VER@@">
    <manualGet fileOrDir="TriTrypDB/ltarParrotTarII/genome/chromosome_Oullette_gff3/2011-06-22/final/genome.fasta" />
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 56890001 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/genome.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence" --regexChromosome '.+?(\d+)'</pluginArgs>
  </resource>

  <resource parentResource="ltarParrotTraII_primary_genome_RSRC"
	    plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
            resource="ltarParrotTraII_genome_annotation_RSRC"
            version="@@ltarParrotTraII_genome_VER@@">
    <manualGet fileOrDir="TriTrypDB/ltarParrotTarII/genome/chromosome_Oullette_gff3/2011-06-22/final/genome.gff" />
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/TriTrypDB/ltarParrotTraIIGFF2Gus.xml --inputFileOrDir @@dataDir@@/genome.gff --inputFileExtension "gff" --fileFormat gff3 --soCvsVersion @@SO_VER@@  --organism "Leishmania tarentolae Parrot-TarII" --seqSoTerm 'chromosome' --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
  </resource>

  <resource parentResource="ltarParrotTraII_primary_genome_RSRC"
	    plugin="GUS::Supported::Plugin::LoadFastaSequences"
            resource="ltarParrotTraII_supercontig_RSRC"
            version="@@ltarParrotTraII_genome_VER@@">
