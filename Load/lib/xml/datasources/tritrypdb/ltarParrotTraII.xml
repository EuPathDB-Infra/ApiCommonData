  </resource>

  <resource plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile"
            resource="ltarParrotTraII_function_Oullette_product_name_RSRC"
            version="@@ltarParrotTraII_genome_VER@@">
    <manualGet fileOrDir="TriTrypDB/ltarParrotTarII/function/Oullette_product_names/2011-06-22/final/products.txt" />
    <pluginArgs>--file @@dataDir@@/products.txt --productDbName %PARENT_RESOURCE_NAME% --productDbVer %PARENT_RESOURCE_VERSION%  </pluginArgs>
  </resource>

</resources>
