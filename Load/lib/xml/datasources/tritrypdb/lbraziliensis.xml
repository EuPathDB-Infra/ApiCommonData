        displayName="GO Association for L. braziliensis genes"
        project="TriTrypDB"
        organisms="@@LbraziliensisOrganismFullName@@"
        category="Proteome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         GO Associations for <i>L. braziliensis</i> genes.
        ]]>
      </description>
