<resources>
  <resource resource="Linfantum_dbEST_RSRC" version="@@Linfantum_dbEST_VER@@"
      plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId 5671 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
    <info
        displayName="TriTryp dbEST ESTs"
        project="TriTrypDB"
        organisms="@@LinfantumFullName@@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         TriTryp ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>


    <resource resource="L.infantum_chromosomes" version="2009-09-07" 
        plugin="GUS::Supported::Plugin::LoadFastaSequences"
        organisms="L.infantum"
        displayName="L.infantum chromosomes"
        category="ignore"
        extDbName="Sanger L. infantum chromosomes"
        extDbRlsVer="2009-09-07"
        extDbRlsDescrip="Sanger L. infantum|2009-09-07"
        dbCommit="@dbcommit@">
        <manualGet
           fileOrDir="/files/cbil/data/cbil/TrypDB/manualDelivery/GenomesFromSanger/export_2009-09-07/Linfantum"/>
        <unpack>mkdir @downloadDir@/L.infantum_chromosomes_fixed</unpack>
        <unpack>makeFastaFile --input_dir @downloadDir@/L.infantum_chromosomes/Linfantum --output_fasta @downloadDir@/L.infantum_chromosomes_fixed/Linfantum.fasta --output_gff @downloadDir@/L.infantum_chromosomes_fixed/Linfantum.gff</unpack>
        <pluginArgs>--externalDatabaseName %EXT_DB_NAME% --ncbiTaxId 5671 --externalDatabaseVersion %EXT_DB_RLS_VER% --sequenceFile @downloadDir@/L.infantum_chromosomes_fixed/Linfantum.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
        <description>
            <![CDATA[ 
   Sequence data for <i>Leishmania infantum</i> clone JPCM5 (MCAN/ES/98/LLM-877) were downloaded from <a href="http://www.genedb.org/genedb/linfantum/">GeneDB</a> (sequence and annotated features).<br> 
Sequencing of <i>L. infantum</i> was conducted by <a href="http://www.sanger.ac.uk/Projects/L_infantum/">The Sanger Institute pathogen sequencing unit</a>. 
          ]]>
        </description>
<publication pmid="17572675"/>
   </resource>

    <resource resource="L.infantum_Annotation" version="2009-09-10"
        displayName="L.infantum Genome Annotations"
        category="Genome"
        organisms="L.infantum"
        plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
        extDbName="Sanger L. infantum chromosomes"
        extDbRlsVer="2009-09-07"
        extDbRlsDescrip="Sanger L. infantum chromosomes|2009-09-10"
        dbCommit="@dbcommit@">
      <manualGet fileOrDir="@downloadDir@/L.infantum_chromosomes_fixed/Linfantum.gff" />
        <unpack>preprocessGFF3 --input_gff @downloadDir@/L.infantum_chromosomes_fixed/Linfantum.gff --output_gff @downloadDir@/L.infantum_Annotation/Linfantum_fixed.gff</unpack>
        <pluginArgs>--extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --mapFile @gusHome@/lib/xml/isf/TriTrypDB/sangerGFF32Gus.xml --inputFileOrDir @downloadDir@/L.infantum_Annotation/Linfantum_fixed.gff --fileFormat gff3 --soCvsVersion @soVer@ --defaultOrganism "Leishmania infantum" --naSequenceSubclass ExternalNASequence --seqIdColumn source_id --validationLog @pipelineDir@/logs/Linfantum.validation.log --bioperlTreeOutput @pipelineDir@/logs/Linfantum.bioperl --handlerExternalDbs "enzyme:enzymeDB:2009-08-12"</pluginArgs>
        <description>
            <![CDATA[
   This resource includes chromosome sequences and annotations for <i>Leishmania infantum</i>.
   Sequence data for <i>Leishmania infantum</i> clone JPCM5 (MCAN/ES/98/LLM-877) were downloaded from <a href="http://www.genedb.org/genedb/linfantum/">GeneDB</a> (sequence and annotated features).<br> 
Sequencing of <i>L. infantum</i> was conducted by <a href="http://www.sanger.ac.uk/Projects/L_infantum/">The Sanger Institute pathogen sequencing unit</a>. 
          ]]>
        </description>
<publication pmid="17572675"/>
    </resource>

    <resource resource="GOAssociations_Linfantum" version="2009-10-06"
        displayName="L. infantum protein GO Associations"        
        category="Proteome"
        plugin="ApiCommonData::Load::Plugin::InsertGOAssociationsSimple"
        extDbName="L.infantum GO Associations from GeneDB"
        extDbRlsVer="2009-10-06"
        extDbRlsDescrip="L.infantum GO Associations from GeneDB|2009-10-06"
        dbCommit="@dbcommit@">
        <manualGet fileOrDir="/files/cbil/data/cbil/TrypDB/manualDelivery/GenomesFromSanger/nfs/pathdata2/GFF/eupath-export-2009-10-06/Linfantum.gas"/>


        <pluginArgs>--externalDatabaseSpec "%EXT_DB_NAME%|%EXT_DB_RLS_VER%" --inputFile @downloadDir@/GOAssociations_Linfantum/Linfantum.gas --goExternalDatabaseSpec "Gene Ontology|1.748" --seqIdSql "SELECT g.source_id, taf.aa_sequence_id FROM dots.genefeature g, dots.transcript t, dots.translatedAAFeature taf WHERE g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id" --targetTable "DoTS::TranslatedAASequence" --lineOfEvidence "GeneDB" --inputIdColumn id --tolerateMissingSeqs --skipBadGOTerms @pipelineDir@/logs/badGOTerms.log</pluginArgs>
        <description>
          <![CDATA[
          <i>L. infantum</i> Gene Ontology associations from <a href="http://www.genedb.org/genedb/leish/">GeneDB</a>.
          ]]>
        </description>
    </resource>

