<resources>
  <resource plugin="GUS::Supported::Plugin::LoadFastaSequences"
            resource="Lmexicana_chromosomes_RSRC"
            version="@@Lmexicana_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/lmexicana/genome/chromosome/%RESOURCE_VERSION%/gff" />
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 5665 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Lmexicana_chromosomes_fixed/Lmexicana.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)\|' --tableName "DoTS::ExternalNASequence" --regexChromosome '\|(\d+)'</pluginArgs>
    <unpack>mkdir @@dataDir@@/Lmexicana_chromosomes_fixed</unpack>
    <unpack>splitGff3FilesIntoFastaAndCustomGff3 --input_dir @@dataDir@@/gff --inputFileExt gff --output_fasta @@dataDir@@/Lmexicana_chromosomes_fixed/Lmexicana.fasta --output_gff  @@dataDir@@/Lmexicana_chromosomes_fixed/Lmexicana.gff3</unpack>
  </resource>
  <resource parentResource="Lmexicana_chromosomes_RSRC"
            plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
            resource="Lmexicana_annotations_RSRC"
            version="@@Lmexicana_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/lmexicana/genome/chromosome/%RESOURCE_VERSION%/gff" />
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/geneDBGFF2Gus.xml --inputFileOrDir @@dataDir@@/Lmexicana_chromosomes_fixed/Lmexicana.gff --inputFileExtension "gff"  --fileFormat gff3 --soCvsVersion @@SO_VER@@ --defaultOrganism "Leishmania mexicana"  --seqSoTerm "chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
    <unpack>mkdir @@dataDir@@/Lmexicana_chromosomes_fixed</unpack>
    <unpack>splitGff3FilesIntoFastaAndCustomGff3 --input_dir @@dataDir@@/gff --inputFileExt gff --output_fasta @@dataDir@@/Lmexicana_chromosomes_fixed/Lmexicana.fasta --output_gff  @@dataDir@@/Lmexicana_chromosomes_fixed/Lmexicana.gff3</unpack>
    <unpack>preprocessGFF3 --input_gff @@dataDir@@/Lmexicana_chromosomes_fixed/Lmexicana.gff3 --output_gff @@dataDir@@/Lmexicana_chromosomes_fixed/Lmexicana.gff</unpack>
  </resource>
  <resource plugin="GUS::Community::Plugin::InsertECMapping"
            resource="Lmexicana_ECNumbers_RSRC"
            version="@@Lmexicana_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/lmexicana/function/Lmexicana_chromosomes_RSRC_ecnumbers/%RESOURCE_VERSION%/ec.txt" />
    <pluginArgs>--ECMappingFile @@dataDir@@/ec.txt --evidenceCode "GeneDB" --aaSeqLocusTagMappingSql "SELECT aa_sequence_id FROM dots.GeneFeature g, dots.Transcript t, dots.TranslatedAAFeature taf WHERE g.source_id = ? AND g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id"</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertGOAssociationsSimple"
            resource="GOAssociations_Lmexicana_RSRC"
            version="@@Lmexicana_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/lmexicana/function/GOAssociations_Lmexicana/%RESOURCE_VERSION%/Lmexicana.gas" />
    <pluginArgs>--externalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%" --inputFile @@dataDir@@/Lmexicana.gas --goExternalDatabaseSpec "GO_RSRC|@@GO_VER@@" --seqIdSql "SELECT g.source_id, taf.aa_sequence_id FROM dots.genefeature g, dots.transcript t, dots.translatedAAFeature taf WHERE g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id UNION SELECT taf.source_id, taf.aa_sequence_id FROM dots.translatedAAFeature taf" --targetTable "DoTS::TranslatedAASequence" --lineOfEvidence "GeneDB" --inputIdColumn id --tolerateMissingSeqs --skipBadGOTerms @@dataDir@@/badGOTerms.log</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile"
            resource="Lmexicana_ProductName_RSRC"
            version="@@Lmexicana_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/lmexicana/function/Lmexicana_chromosomes_RSRC_product/%RESOURCE_VERSION%/product.txt" />
    <pluginArgs>--file @@dataDir@@/product.txt --productDbName Lmexicana_chromosomes_RSRC --productDbVer @@Lmexicana_chromosomes_VER@@</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertNaFeatCommentFromTabFile"
            resource="Lmexicana_Comment_RSRC"
            version="@@Lmexicana_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/lmexicana/genome/Lmexicana_chromosomes_RSRC_geneComment/%RESOURCE_VERSION%/comments.txt" />
    <pluginArgs>--file @@dataDir@@/comments.txt --genomeDbName Lmexicana_chromosomes_RSRC --genomeDbVer @@Lmexicana_chromosomes_VER@@ </pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertGeneNamesFromTabFile"
            resource="Lmexicana_GeneName_RSRC"
            version="@@Lmexicana_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/lmexicana/genome/Lmexicana_chromosomes_RSRC_geneName/%RESOURCE_VERSION%/genename.txt" />
    <pluginArgs>--file @@dataDir@@/genename.txt --geneNameDbName Lmexicana_chromosomes_RSRC --geneNameDbVer @@Lmexicana_chromosomes_VER@@ </pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Lmexicana_previds_dbxref_RSRC"
            version="@@Lmexicana_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/lmexicana/dbxref/Lmexicana_chromosomes_RSRC/%RESOURCE_VERSION%/Lmexicana_previds.txt" />
    <pluginArgs>--DbRefMappingFile @@dataDir@@/Lmexicana_previds.txt --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'Lmexicana_chromosomes_RSRC|@@Lmexicana_chromosomes_VER@@' --idType 'previous id'</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Lmexicana_pmid_dbxref_RSRC"
            version="@@Lmexicana_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/lmexicana/dbxref/Lmexicana_chromosomes_RSRC/%RESOURCE_VERSION%/Lmexicana_pmid.txt" />
    <pluginArgs>--DbRefMappingFile @@dataDir@@/Lmexicana_pmid.txt --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'Lmexicana_chromosomes_RSRC|@@Lmexicana_chromosomes_VER@@' --idType ''</pluginArgs>
  </resource>
  <resource plugin="GUS::Supported::Plugin::dbEST"
            resource="Lmexicana_dbEST_RSRC"
            version="@@Lmexicana_dbEST_VER@@">
    <manualGet fileOrDir="global/empty" />
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
    <unpack>getSubTaxa --ncbiTaxId 5665 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Lmexicana_Entrez_Gene_dbxref_RSRC"
            version="2010-10-13">
    <manualGet fileOrDir="TriTrypDB/common/dbxref/Gene2Entrez/%RESOURCE_VERSION%/gene2entrez.tab" />
    <pluginArgs>--DbRefMappingFile @@dataDir@@/gene2entrez.tab --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'Lmexicana_chromosomes_RSRC|@@Lmexicana_chromosomes_VER@@'  --idType 'alternate id'</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Lmexicana_Uniprot_Gene_dbxref_RSRC"
            version="@@Lmexicana_Uniprot_Gene_dbxref_VER@@">
    <manualGet fileOrDir="TriTrypDB/common/dbxref/Gene2Uniprot/%RESOURCE_VERSION%/gene2uniprot.tab" />
    <pluginArgs>--DbRefMappingFile @@dataDir@@/gene2uniprot.tab --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'Lmexicana_chromosomes_RSRC|@@Lmexicana_chromosomes_VER@@' --idType 'alternate id'</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertMassSpecFeaturesAndSummaries"
            resource="Lmexicana_Proteomics_Aebischer_GelFree_RSRC"
            version="2009-07">
    <manualGet fileOrDir="TriTrypDB/lmexicana/massSpec/Lmexicana_Aebischer_GelFree/%RESOURCE_VERSION%/final/Leishmania_Protein_Peptides1.tab" />
    <pluginArgs>--inputFile @@dataDir@@/Leishmania_Protein_Peptides1.tab --externalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%" --geneExternalDatabaseSpec 'Lmexicana_chromosomes_RSRC|@@Lmexicana_chromosomes_VER@@' --developmentalStage ''</pluginArgs>
  </resource>
</resources>
