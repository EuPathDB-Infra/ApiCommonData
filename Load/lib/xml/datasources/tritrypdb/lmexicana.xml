  <resource resource="Lmexicana_GeneName_RSRC" version="@@Lmexicana_chromosomes_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGeneNamesFromTabFile">
    <manualGet fileOrDir="PlasmoDB/TriTrypDB/lmexicana/genome/chromosome/%RESOURCE_VERSION%/Lmex.gff3"/>
    <unpack>makeGeneNameFile --format gff3 --fileOrDir @@dataDir@@/Lmex.gff3 --output_file @@dataDir@@/LmexicanaGeneName.tab --feat_with_genename gene --genename_qualifier Name --geneid_qualifier ID</unpack>
    <unpack>makeGeneNameFile --format gff3 --fileOrDir @@dataDir@@/Lmex.gff3 --output_file @@dataDir@@/LmexicanaGeneName.tab --feat_with_genename gene --genename_qualifier synonym --geneid_qualifier ID --append</unpack>
    <pluginArgs>--file @@dataDir@@/LmexicanaGeneName.tab --genomeDbName Lmexicana_chromosomes_RSRC --genomeDbVer @@Lmexicana_chromosomes_VER@@ </pluginArgs>
