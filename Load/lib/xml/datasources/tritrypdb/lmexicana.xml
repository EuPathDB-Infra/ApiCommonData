    <unpack>makeCommentFile --format gff3 --fileOrDir @@dataDir@@/Lmex.gff3 --output_file @@dataDir@@/LmexicanaComment.tab --feat_with_geneid pseudogenic_transcript --geneid_qualifier Parent --feat_with_comment polypeptide --commentfeatid_qualifier ID --comment_qualifier comment --append</unpack>
    <pluginArgs>--file @@dataDir@@/LmexicanaComment.tab --genomeDbName Lmexicana_chromosomes_RSRC --genomeDbVer @@Lmexicana_chromosomes_VER@@ </pluginArgs>
