<resources>

<!--ISFResource-->

  <resource resource="Lmexicana_chromosomes_RSRC" version="@@Lmexicana_chromosomes_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="TriTrypDB/lmexicana/genome/chromosome/%RESOURCE_VERSION%/Lmex.fasta/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 5665 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Lmex.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence" --regexChromosome '\|(\d+)'</pluginArgs>
    <info
        displayName="L. mexicana chromosomes sequence and annotation"
        project="TriTrypDB"
        organisms="@@LmexicanaOrganismFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
                 This resource includes chromosome and unassigned contig sequences for <i>L.mexicana</i> obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute. Please visit <a href="http://www.genedb.org/Homepage/Lmexicana">GeneDB</a> for project details and data release policies.
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Lmexicana_annotations_RSRC" version="@@Lmexicana_chromosomes_VER@@" parentResource="Lmexicana_chromosomes_RSRC"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="TriTrypDB/lmexicana/genome/chromosome/%RESOURCE_VERSION%/Lmex.gff3"/>
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/sangerGFF32Gus.xml --inputFileOrDir @@dataDir@@/Lmex.gff3 --inputFileExtension "gff"  --fileFormat gff3 --soCvsVersion @@SO_VER@@ --defaultOrganism "Leishmania mexicana"  --seqSoTerm "chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
    <info
        displayName="L. mexicana sequence and annotation"
        project="TriTrypDB"
        organisms="@@LmexicanaOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
                  Genome and annotations for <i>L. mexicana</i> obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="GOAssociations_Lmexicana_RSRC" version="@@Lmexicana_chromosomes_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGOAssociationsSimple">
    <manualGet fileOrDir="TriTrypDB/lmexicana/function/GOAssociations_Lmexicana/%RESOURCE_VERSION%/Lmexicana.gas"/>
    <pluginArgs>--externalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%" --inputFile @@dataDir@@/Lmexicana.gas --goExternalDatabaseSpec "GO_RSRC|@@GO_VER@@" --seqIdSql "SELECT g.source_id, taf.aa_sequence_id FROM dots.genefeature g, dots.transcript t, dots.translatedAAFeature taf WHERE g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id UNION SELECT taf.source_id, taf.aa_sequence_id FROM dots.translatedAAFeature taf" --targetTable "DoTS::TranslatedAASequence" --lineOfEvidence "GeneDB" --inputIdColumn id --tolerateMissingSeqs --skipBadGOTerms @@dataDir@@/badGOTerms.log</pluginArgs>
    <info
        displayName="GO Association for L. mexicana genes"
        project="TriTrypDB"
        organisms="@@LmexicanaOrganismFullName@@"
        category="Proteome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         GO Associations for <i>L. mexicana</i> genes.
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Lmexicana_ProductName_RSRC" version="@@Lmexicana_chromosomes_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile">
    <manualGet fileOrDir="TriTrypDB/lmexicana/genome/chromosome/%RESOURCE_VERSION%/Lmex.gff3"/>
    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/Lmex.gff3 --output_file @@dataDir@@/LmexicanaProduct.tab --feat_with_geneid mRNA --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product</unpack>
    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/Lmex.gff3 --output_file @@dataDir@@/LmexicanaProduct.tab --feat_with_geneid pseudogenic_transcript --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product --append</unpack>
    <pluginArgs>--file @@dataDir@@/LmexicanaProduct.tab --genomeDbName Lmexicana_chromosomes_RSRC --genomeDbVer @@Lmexicana_chromosomes_VER@@ --productDbName Lmexicana_chromosomes_RSRC --productDbVer @@Lmexicana_chromosomes_VER@@</pluginArgs>
    <info
        displayName="Lmexicana Product Name"
        project="TriTrypDB"
        organisms="@@LmexicanaOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Lmexicana_Comment_RSRC" version="@@Lmexicana_chromosomes_VER@@"
