    <getAndUnpackOutput file="@@dataDir@@/final/s_8_2_CGATGTsequence.txt" />
    <manualGet fileOrDir="TriTrypDB/lmajor/rnaSeq/washu_lmajor_friedlin/%RESOURCE_VERSION%/final" />
