        extDbRlsDescrip="L.major_seidman_75.1 genome from WUSTL|07-20-2010"
        dbCommit="@dbcommit@">
        <manualGet
           fileOrDir="/files/cbil/data/cbil/TrypDB/manualDelivery/Genomes/WhitePaper_preliminary_genomes/Lmajor_seidman_75.1/FTP/454AllContigs.fna"/>

        <pluginArgs>--externalDatabaseName %EXT_DB_NAME% --ncbiTaxId 5656 --externalDatabaseVersion %EXT_DB_RLS_VER% --sequenceFile @downloadDir@/Lmajor_seidman_75.1/454AllContigs.fna --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
