        <pluginArgs>--externalDatabaseName %EXT_DB_NAME% --ncbiTaxonName "Trypanosoma cruzi strain NonEsmeraldo" --externalDatabaseVersion %EXT_DB_RLS_VER% --sequenceFile @downloadDir@/T.cruzi_chromosomes_fixed/Tcruzi_NonEsmeraldo.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
