  <resource resource="Linfantum_IEDB_Epitopes_RSRC" version="@@Linfantum_IEDB_Epitopes_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="TriTrypDB/common/epitope/IEDB/%RESOURCE_VERSION%/final/IEDBExportLi.txt"/>
    <unpack>mkdir -p @@dataDir@@/IEDB_Epitopes_fixed/fasta</unpack>
    <unpack>extractFromIEDB --inputFile @@dataDir@@/IEDBExportLi.txt --outputFile @@dataDir@@/IEDB_Epitopes_fixed/IEDBExportLi.txt </unpack>
    <unpack>generateEpitopeFastaFile --inputFile @@dataDir@@/IEDB_Epitopes_fixed/IEDBExportLi.txt --outputFile @@dataDir@@/IEDB_Epitopes_fixed/fasta/IEDBExportLi.fsa </unpack>
    <getAndUnpackOutput dir="@@dataDir@@/IEDB_Epitopes_fixed"/>
    <getAndUnpackOutput file="@@dataDir@@/IEDB_Epitopes_fixed/IEDBExportLi.txt"/>
    <pluginArgs>--seqFileDir @@dataDir@@/IEDB_Epitopes_fixed/fasta --externalDatabaseName %RESOURCE_NAME% --externalDatabaseVersion %RESOURCE_VERSION% --soTermName "protein_binding_site" --regexSourceId ">(\d+)|" --tableName "DoTS::MotifAASequence" --logFrequency 100 --regexNcbiTaxId "\|.*\|(.*)" --regexName "\|(.*)\|.*"</pluginArgs>
    <info
        displayName="IEDB Epitopes"
        project="TriTrypDB"
        organisms="@@LinfantumOrganismFullName@@"
        category="Proteome"
        publicUrl="http://www.immuneepitope.org"
