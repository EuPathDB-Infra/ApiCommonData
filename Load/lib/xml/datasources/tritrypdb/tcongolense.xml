<resources>
  <resource plugin="GUS::Supported::Plugin::LoadFastaSequences"
            resource="Tcongolense_chromosomes_RSRC"
            version="@@Tcongolense_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcongolense/genome/chromosome/%RESOURCE_VERSION%/gff" />
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 5692 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Tcongolense_chromosomes_fixed/Tcongolense.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)\|' --tableName "DoTS::ExternalNASequence" --regexChromosome '\|(\d+)'</pluginArgs>
    <unpack>mkdir @@dataDir@@/Tcongolense_chromosomes_fixed</unpack>
    <unpack>makeCustomFastaAndGffFromGff3 --input_dir @@dataDir@@/gff --inputFileExt gff --output_fasta @@dataDir@@/Tcongolense_chromosomes_fixed/Tcongolense.fasta --output_gff  @@dataDir@@/Tcongolense_chromosomes_fixed/Tcongolense.gff3</unpack>
  </resource>
  <resource parentResource="Tcongolense_chromosomes_RSRC"
            plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
            resource="Tcongolense_annotations_RSRC"
            version="@@Tcongolense_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcongolense/genome/chromosome/%RESOURCE_VERSION%/gff" />
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/geneDBGFF2Gus.xml --inputFileOrDir @@dataDir@@/Tcongolense_chromosomes_fixed/Tcongolense.gff --inputFileExtension "gff"  --fileFormat gff3 --soCvsVersion @@SO_VER@@ --defaultOrganism "Trypanosoma congolense"  --seqSoTerm "chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
    <unpack>mkdir @@dataDir@@/Tcongolense_chromosomes_fixed</unpack>
    <unpack>makeCustomFastaAndGffFromGff3 --input_dir @@dataDir@@/gff --inputFileExt gff --output_fasta @@dataDir@@/Tcongolense_chromosomes_fixed/Tcongolense.fasta --output_gff  @@dataDir@@/Tcongolense_chromosomes_fixed/Tcongolense.gff3</unpack>
    <unpack>preprocessGFF3 --input_gff @@dataDir@@/Tcongolense_chromosomes_fixed/Tcongolense.gff3 --output_gff @@dataDir@@/Tcongolense_chromosomes_fixed/Tcongolense.gff</unpack>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertGOAssociationsSimple"
            resource="GOAssociations_Tcongolense_RSRC"
            version="@@Tcongolense_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcongolense/function/GOAssociations_Tcongolense/%RESOURCE_VERSION%/Tcongolense.gas" />
    <pluginArgs>--externalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%" --inputFile @@dataDir@@/Tcongolense.gas --goExternalDatabaseSpec "GO_RSRC|@@GO_VER@@" --seqIdSql "SELECT g.source_id, taf.aa_sequence_id FROM dots.genefeature g, dots.transcript t, dots.translatedAAFeature taf WHERE g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id UNION SELECT taf.source_id, taf.aa_sequence_id FROM dots.translatedAAFeature taf" --targetTable "DoTS::TranslatedAASequence" --lineOfEvidence "GeneDB" --inputIdColumn id --tolerateMissingSeqs --skipBadGOTerms @@dataDir@@/badGOTerms.log</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile"
            resource="Tcongolense_ProductName_RSRC"
            version="@@Tcongolense_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcongolense/function/Tcongolense_chromosomes_RSRC_product/%RESOURCE_VERSION%/product.txt" />
    <pluginArgs>--file @@dataDir@@/product.txt --productDbName Tcongolense_chromosomes_RSRC --productDbVer @@Tcongolense_chromosomes_VER@@</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertGeneNamesFromTabFile"
            resource="Tcongolense_GeneName_RSRC"
            version="@@Tcongolense_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcongolense/genome/Tcongolense_chromosomes_RSRC_geneName/%RESOURCE_VERSION%/genename.txt" />
    <pluginArgs>--file @@dataDir@@/genename.txt --geneNameDbName Tcongolense_chromosomes_RSRC --geneNameDbVer @@Tcongolense_chromosomes_VER@@ </pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertNaFeatCommentFromTabFile"
            resource="Tcongolense_Comment_RSRC"
            version="@@Tcongolense_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcongolense/genome/Tcongolense_chromosomes_RSRC_geneComment/%RESOURCE_VERSION%/comments.txt" />
    <pluginArgs>--file @@dataDir@@/comments.txt --genomeDbName Tcongolense_chromosomes_RSRC --genomeDbVer @@Tcongolense_chromosomes_VER@@ </pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Tcongolense_previds_dbxref_RSRC"
            version="@@Tcongolense_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcongolense/dbxref/Tcongolense_chromosomes_RSRC/%RESOURCE_VERSION%/Tcongolense_previds.txt" />
    <pluginArgs>--DbRefMappingFile @@dataDir@@/Tcongolense_previds.txt --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'Tcongolense_chromosomes_RSRC|@@Tcongolense_chromosomes_VER@@' --idType 'previous id'</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Tcongolense_pmid_dbxref_RSRC"
            version="@@Tcongolense_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcongolense/dbxref/Tcongolense_chromosomes_RSRC/%RESOURCE_VERSION%/Tcongolense_pmid.txt" />
    <pluginArgs>--DbRefMappingFile @@dataDir@@/Tcongolense_pmid.txt --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'Tcongolense_chromosomes_RSRC|@@Tcongolense_chromosomes_VER@@' --idType ''</pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Tcongolense_interpro_dbxref_RSRC"
            version="@@Tcongolense_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcongolense/dbxref/Tcongolense_chromosomes_RSRC/%RESOURCE_VERSION%/Tcongolense_interpro.txt" />
    <pluginArgs>--DbRefMappingFile @@dataDir@@/Tcongolense_interpro.txt --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'Tcongolense_chromosomes_RSRC|@@Tcongolense_chromosomes_VER@@' --idType ''</pluginArgs>
  </resource>
  <resource plugin="GUS::Community::Plugin::InsertECMapping"
            resource="Tcongolense_ECNumbers_RSRC"
            version="@@Tcongolense_chromosomes_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcongolense/function/Tcongolense_chromosomes_RSRC_ecnumbers/%RESOURCE_VERSION%/ec.txt" />
    <pluginArgs>--ECMappingFile @@dataDir@@/ec.txt --evidenceCode "GeneDB" --aaSeqLocusTagMappingSql "SELECT aa_sequence_id FROM dots.GeneFeature g, dots.Transcript t, dots.TranslatedAAFeature taf WHERE g.source_id = ? AND g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id"</pluginArgs>
  </resource>
  <resource plugin="GUS::Supported::Plugin::dbEST"
            resource="Tcongolense_dbEST_RSRC"
            version="@@Tcongolense_dbEST_VER@@">
    <manualGet fileOrDir="global/empty" />
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
    <unpack>getSubTaxa --ncbiTaxId 5692 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Tcongolense_Entrez_Gene_dbxref_RSRC"
            version="2010-10-13">
    <manualGet fileOrDir="TriTrypDB/common/dbxref/Gene2Entrez/%RESOURCE_VERSION%/gene2entrez.tab" />
    <pluginArgs>--DbRefMappingFile @@dataDir@@/gene2entrez.tab --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'Tcongolense_chromosomes_RSRC|@@Tcongolense_chromosomes_VER@@' --idType 'alternate id' </pluginArgs>
  </resource>
  <resource plugin="ApiCommonData::Load::Plugin::InsertDBxRefs"
            resource="Tcongolense_Uniprot_Gene_dbxref_RSRC"
            version="@@Tcongolense_Uniprot_Gene_dbxref_VER@@">
    <manualGet fileOrDir="TriTrypDB/common/dbxref/Gene2Uniprot/%RESOURCE_VERSION%/gene2uniprot.tab" />
    <pluginArgs>--DbRefMappingFile @@dataDir@@/gene2uniprot.tab --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec 'Tcongolense_chromosomes_RSRC|@@Tcongolense_chromosomes_VER@@'  --idType 'alternate id'</pluginArgs>
  </resource>
</resources>
