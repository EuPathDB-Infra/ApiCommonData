<resources>

<!--ISFResource-->

  <resource resource="Tcongolense_chromosomes_RSRC" version="@@Tcongolense_chromosomes_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="TriTrypDB/tcongolense/genome/chromosome/%RESOURCE_VERSION%/Tcongolense.fasta"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 5692 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/Tcongolense.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence" --regexChromosome '\|(\d+)'</pluginArgs>
    <info
        displayName="T. congolense chromosomes sequence and annotation"
        project="TriTrypDB"
        organisms="@@TcongolenseOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
                 Chromosome sequences for <i>T.congolense</i> obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute. Please visit <a href="http://www.genedb.org/Homepage/Tcongolense">GeneDB</a> for project details and data release policies.
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Tcongolense_annotations_RSRC" version="@@Tcongolense_chromosomes_VER@@" parentResource="Tcongolense_chromosomes_RSRC"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="TriTrypDB/tcongolense/genome/chromosome/%RESOURCE_VERSION%/Tcongolense.gff3"/>
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/sangerGFF32Gus.xml --inputFileOrDir @@dataDir@@/Tcongolense.gff3 --inputFileExtension "gff"  --fileFormat gff3 --soCvsVersion @@SO_VER@@ --defaultOrganism "Trypanosoma congolense"  --seqSoTerm "chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
    <info
        displayName="T. congolense chromosomes sequence and annotation"
        project="TriTrypDB"
        organisms="@@TcongolenseOrganismFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
                  This resource includes chromosome and unassigned contig sequences and annotations for <i>Trypanosoma congolense</i> obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute. Please visit <a href="http://www.genedb.org/Homepage/Tcongolense">GeneDB</a> for project details and data release policies.
        ]]>
      </description>
    </info>
  </resource>

   <resource resource="GOAssociations_Tcongolense_RSRC" version="@@Tcongolense_chromosomes_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGOAssociationsSimple">
    <manualGet fileOrDir="TriTrypDB/tcongolense/function/GOAssociations_Tcongolense/%RESOURCE_VERSION%/Tcongolense.gas"/>
    <pluginArgs>--externalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%" --inputFile @@dataDir@@/Tcongolense.gas --goExternalDatabaseSpec "GO_RSRC|@@GO_VER@@" --seqIdSql "SELECT g.source_id, taf.aa_sequence_id FROM dots.genefeature g, dots.transcript t, dots.translatedAAFeature taf WHERE g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id UNION SELECT taf.source_id, taf.aa_sequence_id FROM dots.translatedAAFeature taf" --targetTable "DoTS::TranslatedAASequence" --lineOfEvidence "GeneDB" --inputIdColumn id --tolerateMissingSeqs --skipBadGOTerms @@dataDir@@/badGOTerms.log</pluginArgs>
    <info
        displayName="GO Association for T.Tcongolense genes"
        project="TriTrypDB"
        organisms="@@TcongolenseOrganismFullName@@"
        category="Proteome"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         GO Associations for <i>T. congolense</i> genes.
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Tcongolense_ProductName_RSRC" version="@@Tcongolense_chromosomes_VER@@"
      plugin="ApiCommonData::Load::Plugin::InsertGeneFeatProductFromTabFile">
    <manualGet fileOrDir="TriTrypDB/tcongolense/genome/chromosome/%RESOURCE_VERSION%/Tcongolense.gff3"/>
    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/Tcongolense.gff3 --output_file @@dataDir@@/TcongolenseProduct.tab --feat_with_geneid mRNA --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product</unpack>
    <unpack>makeProductFile --format gff3 --fileOrDir @@dataDir@@/Tcongolense.gff3 --output_file @@dataDir@@/TcongolenseProduct.tab --feat_with_geneid pseudogenic_transcript --geneid_qualifier Parent --feat_with_product polypeptide --prodfeatid_qualifier ID --product_qualifier product --append</unpack>
    <pluginArgs>--file @@dataDir@@/TcongolenseProduct.tab --genomeDbName Tcongolense_chromosomes_RSRC --genomeDbVer @@Tcongolense_chromosomes_VER@@ --productDbName Tcongolense_chromosomes_RSRC --productDbVer @@Tcongolense_chromosomes_VER@@</pluginArgs>
    <info
        displayName="Tcongolense Product Name"
        project="TriTrypDB"
        organisms="@@TcongolenseOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         
        ]]>
      </description>
    </info>
  </resource>

  <resource resource="Tcongolense_GeneName_RSRC" version="@@Tcongolense_chromosomes_VER@@"
