    <resource resource="Tcongolense_Annotation_chromosomes_RSRC" version="2009-09-14"
        displayName="T.congolense Genome Annotations"
        category="Genome"
        organisms="T.congolense"
        plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
        extDbName="Sanger T. congolense contigs and chromosomes"
        extDbRlsVer="2009-09-14"
        extDbRlsDescrip="Sanger T. congolense contigs and chromosomes|2009-09-14"
        dbCommit="@dbcommit@">
      <manualGet fileOrDir="@downloadDir@/T.congolense_chromosomes_fixed/Tcongolense.gff" />
        <unpack>preprocessGFF3 --input_gff @downloadDir@/T.congolense_chromosomes_fixed/Tcongolense.gff --output_gff @downloadDir@/T.congolense_Annotation_chromosomes/Tcongolense_fixed.gff</unpack>
        <pluginArgs>--extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --mapFile @gusHome@/lib/xml/isf/TriTrypDB/sangerGFF32Gus.xml --inputFileOrDir @downloadDir@/T.congolense_Annotation_chromosomes/Tcongolense_fixed.gff --fileFormat gff3 --soCvsVersion @soVer@ --defaultOrganism "Trypanosoma congolense" --naSequenceSubclass ExternalNASequence --seqIdColumn source_id --validationLog @pipelineDir@/logs/Tcongolense.validation.log.2 --bioperlTreeOutput @pipelineDir@/logs/Tcongolense.bioperl.2 --handlerExternalDbs "enzyme:enzymeDB:2009-08-12"</pluginArgs>
        <description>
            <![CDATA[
                  This resource includes chromosome and unassigned contig sequences and annotations for <i>Trypanosoma congolense</i> obtained from the Pathogen Sequencing Unit at the Wellcome Trust Sanger Institute. Please visit <a href="http://www.genedb.org/Homepage/Tcongolense">GeneDB</a> for project details and data release policies.
           ]]>
        </description>
    </resource>
 

    <resource resource="GOAssociations_Tcongolense_RSRC" version="2009-10-06"
        displayName="T. congolense protein GO Associations"        
        category="Proteome"
        plugin="ApiCommonData::Load::Plugin::InsertGOAssociationsSimple"
        extDbName="T.congolense GO Associations from GeneDB"
        extDbRlsVer="2009-10-06"
        extDbRlsDescrip="T.congolense GO Associations from GeneDB|2009-10-06"
        dbCommit="@dbcommit@">
        <manualGet fileOrDir="/files/cbil/data/cbil/TrypDB/manualDelivery/GenomesFromSanger/nfs/pathdata2/GFF/eupath-export-2009-10-06/Tcongolense.gas"/>


        <pluginArgs>--externalDatabaseSpec "%EXT_DB_NAME%|%EXT_DB_RLS_VER%" --inputFile @downloadDir@/GOAssociations_Tcongolense/Tcongolense.gas --goExternalDatabaseSpec "Gene Ontology|1.748" --seqIdSql "SELECT g.source_id, taf.aa_sequence_id FROM dots.genefeature g, dots.transcript t, dots.translatedAAFeature taf WHERE g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id" --targetTable "DoTS::TranslatedAASequence" --lineOfEvidence "GeneDB" --inputIdColumn id --tolerateMissingSeqs --skipBadGOTerms @pipelineDir@/logs/badGOTerms.log</pluginArgs>
        <description>
          <![CDATA[
          <i>T. congolense</i> Gene Ontology associations from <a href="http://www.genedb.org/genedb/tryp/">GeneDB</a>.
          ]]>
        </description>
    </resource>

<!--ESTResource-->
  <resource resource="Tcongolense_dbEST_RSRC" version="@@Tcongolense_dbEST_VER@@"
      plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId 5692 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
    <info
        displayName="TriTryp dbEST ESTs"
        project="TriTrypDB"
        organisms="@@TcongolenseFullName@@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         TriTryp ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>

</resources>
