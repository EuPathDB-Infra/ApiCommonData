    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 914063 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/supercontigs.fasta --SOTermName 'supercontig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
  </resource>
</resources>
