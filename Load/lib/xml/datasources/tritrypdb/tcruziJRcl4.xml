<resources>
<!--ISFResource-->

  <resource resource="TcruziJRcl4_contigs_RSRC" version="@@TcruziJRcl4_contigs_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="TriTrypDB/tcruzi/genome/tcruziJRcl4/%RESOURCE_VERSION%/final/supercontigs.fasta"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 914063 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/supercontigs.fasta --SOTermName 'supercontig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    <info
        displayName="T. cruzi cruzi JR cl. 4 genome sequence."
        project="TriTrypDB"
        organisms="@@TcruziJRcl4OrganismFullName@@"
        category="Genome"
        publicUrl=""
        contact="Wes Warren"
        email="wwarren@genome.wustl.edu"
        institution="wustl">
      <description>
        <![CDATA[
T. cruzi cruzi JR cl. 4 genome sequence. 
        ]]>
      </description>
    </info>
  </resource>

</resources>
