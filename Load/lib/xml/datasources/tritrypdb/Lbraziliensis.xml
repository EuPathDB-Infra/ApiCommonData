<resources>

 <!--ESTResource-->
 <resource resource="Lbraziliensis_dbEST_RSRC" version="@@Lbraziliensis_dbEST_VER@@"
      plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId 5660 --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 --taxonFile @@dataDir@@/taxonIdFile --extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --soVer @@SO_VER@@ --dbestLoginFile @@dataDir@@/dbest.prop --dbestConnect @@dbestConnect@@ </pluginArgs>
    <info
        displayName="TriTryp dbEST ESTs"
        project="TriTrypDB"
        organisms="@@LbraziliensisFullName@@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         TriTryp ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>

<!--ISFResource-->
    <resource resource="Lbraziliensis_chromosomes_RSRC" version="@@Lbraziliensis_chromosomes_VER@@" 
        plugin="GUS::Supported::Plugin::LoadFastaSequences"
        organisms="L.braziliensis"
        displayName="L.braziliensis chromosomes"
        category="ignore"
        extDbName="Sanger L. braziliensis chromosomes"
        extDbRlsVer="2009-10-06"
        extDbRlsDescrip="Sanger L. braziliensis chromosomes|2009-10-06"
        dbCommit="@dbcommit@">
        <manualGet
           fileOrDir="/files/cbil/data/cbil/TrypDB/manualDelivery/GenomesFromSanger/nfs/pathdata2/GFF/eupath-export-2009-10-06/Lbraziliensis"/>
        <unpack>mkdir @downloadDir@/L.braziliensis_chromosomes_fixed</unpack>
        <unpack>makeFastaFile --input_dir @downloadDir@/L.braziliensis_chromosomes/Lbraziliensis --output_fasta @downloadDir@/L.braziliensis_chromosomes_fixed/Lbraziliensis.fasta --output_gff @downloadDir@/L.braziliensis_chromosomes_fixed/Lbraziliensis.gff</unpack>
        <pluginArgs>--externalDatabaseName %EXT_DB_NAME% --ncbiTaxId 5560 --externalDatabaseVersion %EXT_DB_RLS_VER% --sequenceFile @downloadDir@/L.braziliensis_chromosomes_fixed/Lbraziliensis.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
        <description>
            <![CDATA[ 
   Sequence data for <i>Leishmania braziliensis</i> clone M2904 (MHOM/BR/75M2904) were downloaded from <a href="http://www.genedb.org/genedb/lbraziliensis/">GeneDB</a> (sequence and annotated features).<br>
Sequencing of <i>L. braziliensis</i> was conducted by <a href="http://www.sanger.ac.uk/Projects/L_braziliensis/">The Sanger Institute pathogen sequencing unit</a>.

          ]]>      
        </description>  
<publication pmid="17572675"/>
    </resource>

    <resource resource="Lbraziliensis_Annotation_RSRC" version="2009-10-06"
        displayName="L.braziliensis Genome Annotations"
        category="Genome"
        organisms="L.braziliensis"
        plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
        extDbName="Sanger L. braziliensis chromosomes"
        extDbRlsVer="2009-10-06"
        extDbRlsDescrip="Sanger L. braziliensis chromosomes|2009-10-06"
        dbCommit="@dbcommit@">
      <manualGet fileOrDir="@downloadDir@/L.braziliensis_chromosomes_fixed/Lbraziliensis.gff" />
        <unpack>cp @downloadDir@/L.braziliensis_chromosomes/Lbraziliensis/Lbrerrs.txt @downloadDir@/L.braziliensis_Annotation/</unpack>
        <unpack>preprocessGFF3 --input_gff @downloadDir@/L.braziliensis_chromosomes_fixed/Lbraziliensis.gff --output_gff @downloadDir@/L.braziliensis_Annotation/Lbraziliensis_fixed.gff</unpack>
        <pluginArgs>--extDbName %EXT_DB_NAME% --extDbRlsVer %EXT_DB_RLS_VER% --mapFile @gusHome@/lib/xml/isf/TriTrypDB/sangerGFF32Gus.xml --inputFileOrDir @downloadDir@/L.braziliensis_Annotation/Lbraziliensis_fixed.gff --fileFormat gff3 --soCvsVersion @soVer@ --defaultOrganism "Leishmania braziliensis" --naSequenceSubclass ExternalNASequence --seqIdColumn source_id --validationLog @pipelineDir@/logs/Lbraziliensis.validation.log --bioperlTreeOutput @pipelineDir@/logs/Lbraziliensis.bioperl --handlerExternalDbs "enzyme:enzymeDB:2009-08-12"</pluginArgs>
        <description>
            <![CDATA[
   This resource includes chromosome sequences and annotations for <i>Leishmania braziliensis</i>.
   Sequence data for <i>Leishmania braziliensis</i> clone M2904 (MHOM/BR/75M2904) were downloaded from <a href="http://www.genedb.org/genedb/lbraziliensis/">GeneDB</a> (sequence and annotated features).<br>
Sequencing of <i>L. braziliensis</i> was conducted by <a href="http://www.sanger.ac.uk/Projects/L_braziliensis/">The Sanger Institute pathogen sequencing unit</a>.

           ]]>
        </description>
<publication pmid="17572675"/>
    </resource>

    <resource resource="GOAssociations_Lbraziliensis_RSRC" version="2009-10-06"
        displayName="L. braziliensis protein GO Associations"        
        category="Proteome"
        plugin="ApiCommonData::Load::Plugin::InsertGOAssociationsSimple"
        extDbName="L.braziliensis GO Associations from GeneDB"
        extDbRlsVer="2009-10-06"
        extDbRlsDescrip="L.braziliensis GO Associations from GeneDB|2009-10-06"
        dbCommit="@dbcommit@">
        <manualGet fileOrDir="/files/cbil/data/cbil/TrypDB/manualDelivery/GenomesFromSanger/nfs/pathdata2/GFF/eupath-export-2009-10-06/Lbraziliensis.gas"/>


        <pluginArgs>--externalDatabaseSpec "%EXT_DB_NAME%|%EXT_DB_RLS_VER%" --inputFile @downloadDir@/GOAssociations_Lbraziliensis/Lbraziliensis.gas --goExternalDatabaseSpec "Gene Ontology|1.748" --seqIdSql "SELECT g.source_id, taf.aa_sequence_id FROM dots.genefeature g, dots.transcript t, dots.translatedAAFeature taf WHERE g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id" --targetTable "DoTS::TranslatedAASequence" --lineOfEvidence "GeneDB" --inputIdColumn id --tolerateMissingSeqs --skipBadGOTerms @pipelineDir@/logs/badGOTerms.log</pluginArgs>
        <description>
          <![CDATA[
          <i>L. braziliensis</i> Gene Ontology associations from <a href="http://www.genedb.org/genedb/leish/">GeneDB</a>.
          ]]>
        </description>
    </resource>


<!--MassSpecResource-->

  <resource resource="Lbraziliensis_Proteomics_Promastigotes_RSRC" version="2009-08-22"
      plugin="ApiCommonData::Load::Plugin::InsertMassSpecFeaturesAndSummaries">
    <manualGet fileOrDir="TriTrypDB/lbraziliensis/massSpec/promastigotes_secreted/%RESOURCE_VERSION%/final/Lbraziliensis_proteomics.tab"/>
    <pluginArgs>--inputFile @@dataDir@@/Lbraziliensis_proteomics.tab --externalDatabaseSpec "%RESOURCE_NAME%|%RESOURCE_VERSION%" --geneExternalDatabaseSpec 'Lbraziliensis_chromosomes_RSRC|@@Lbraziliensis_chromosomes_VER@@' --developmentalStage 'promastigotes'</pluginArgs>
    <info
        displayName="L. braziliensis Proteomics-Promastigotes"
        project="TriTrypDB"
        organisms="@@LbraziliensisOrganismFullName@@"
        category="Proteome"
        publicUrl=""
        contact="Patricia Cuervo"
        email="patyce@ioc.fiocruz.br"
        institution="Instituto Oswaldo Cruz, Rio de Janeiro, Brazil">
      <description>
        <![CDATA[
          Proteomic characterization of the released/secreted proteins of <i>Leishmania (Viannia) braziliensis</i> promastigotes <a href="http://www.ncbi.nlm.nih.gov/pubmed/19703603">Cuervo et al.</a>
        ]]>
      </description>
    </info>
  </resource>

<!--DbxRefsResource-->
  <resource resource="Lbraziliensis_Entrez_Gene_dbxref_RSRC" version="2010-10-13"
      plugin="ApiCommonData::Load::Plugin::InsertDBxRefs">
    <manualGet fileOrDir="TriTrypDB/common/dbxref/Gene2Entrez/%RESOURCE_VERSION%/gene2entrez.tab"/>
    <pluginArgs>--DbRefMappingFile @@dataDir@@/gene2entrez.tab --extDbName %RESOURCE_NAME% --extDbReleaseNumber %RESOURCE_VERSION% --columnSpec "primary_identifier,secondary_identifier,remark" --tableName "DbRefNAFeature"  --geneExternalDatabaseSpec '_chromosomes_RSRC|@@P_chromosomes_VER@@'</pluginArgs>
    <info
        displayName="dbxref from entrez gene ids to eupathdb gene identifiers"
        project="TriTrypDB"
        organisms="@@LbraziliensisOrganismFullName@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[         
        ]]>
      </description>
    </info>
  </resource>

</resources>

