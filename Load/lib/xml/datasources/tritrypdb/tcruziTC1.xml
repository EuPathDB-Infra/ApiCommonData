<resources>
  <resource plugin="GUS::Supported::Plugin::LoadFastaSequences"
            resource="TcruziTC1_contigs_RSRC"
            version="@@TcruziTC1_contigs_VER@@">
    <manualGet fileOrDir="TriTrypDB/tcruzi/genome/tcruziTC1/%RESOURCE_VERSION%/final/silvio.fa" />
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 5693000 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/silvio.fa --SOTermName 'contig'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence"</pluginArgs>
  </resource>
</resources>
