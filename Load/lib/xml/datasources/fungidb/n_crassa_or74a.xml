<resources>
<!--n_crassa_or74aISFResource-->
  <resource resource="n_crassa_or74a_chromosomes_RSRC" version="@@n_crassa_or74a_chromosomes_VER@@"
      plugin="GUS::Supported::Plugin::LoadFastaSequences">
    <manualGet fileOrDir="FungiDB/n_crassa_or74a/genome/chromosome/%RESOURCE_VERSION%/neurospora_crassa_or74a_supercontigs.fasta"/>
    <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId 367110 --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/neurospora_crassa_or74a_supercontigs.fasta --SOTermName 'chromosome'  --regexSourceId  '>(\S+)' --tableName "DoTS::ExternalNASequence" --regexChromosome '\d+\.(\d+)' </pluginArgs>
    <info
        displayName="Neurospora crassa OR74A chromosomes sequence and annotation"
        project="FungiDB"
        organisms="@@n_crassa_or74a_OrganismFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="Broad">
      <description>
        <![CDATA[
         Sequence data from Broad.
        ]]>
      </description>
    </info>
  </resource>

<resource resource="n_crassa_or74a_annotations_RSRC" version="@@n_crassa_or74a_chromosomes_VER@@" parentResource="n_crassa_or74a_chromosomes_RSRC"
      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">
    <manualGet fileOrDir="FungiDB/n_crassa_or74a/genome/chromosome/%RESOURCE_VERSION%/neurospora_crassa_or74a_transcripts.gff3"/> 
    <unpack>preprocessGFF3 --input_gff @@dataDir@@/neurospora_crassa_or74a_transcripts.gff3 --output_gff @@dataDir@@/neurospora_crassa_or74a.transform.gff</unpack>
    <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/FungiDB/broadGFF32Gus.xml --inputFileOrDir @@dataDir@@/neurospora_crassa_or74a.transform.gff --inputFileExtension "gff"  --fileFormat gff3 --soCvsVersion @@SO_VER@@ --defaultOrganism "Neurospora crassa OR74A"  --seqSoTerm "chromosome" --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
    <info
        displayName="Neurospora crassa OR74A chromosomes sequence and annotation"
        project="FungiDB"
        organisms="@@n_crassa_or74a_OrganismFullName@@"
        category="Genome"
        publicUrl=""
        contact=""
        email=""
        institution="Broad">
      <description>
        <![CDATA[
         ]]>
      </description>
    </info>
  </resource>

</resources>
