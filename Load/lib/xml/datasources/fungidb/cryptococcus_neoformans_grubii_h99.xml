<!-- Cryptococcus neoformans var. grubii H99 -->
<resources>

     <!-- ISF FASTA resource -->
     <resource                       
            resource="cryptococcus_neoformans_grubii_h99_genome_sequence_RSRC"
            version="@@cryptococcus_neoformans_grubii_h99_GENOME_SEQUENCE_VER@@"
            plugin="GUS::Supported::Plugin::LoadFastaSequences">

            <!-- Transforming upstream of ISF/workflow... -->
            <manualGet fileOrDir="FungiDB/cryptococcus_neoformans_grubii_h99/genome/%RESOURCE_VERSION%/cryptococcus_neoformans_grubii_h99_supercontigs.uniquified.fasta" />

            <!-- or within the workflow iteself... -->
            <!--
            <manualGet fileOrDir="FungiDB/cryptococcus_neoformans_grubii_h99/genome/%RESOURCE_VERSION%/cryptococcus_neoformans_grubii_h99_supercontigs.fasta" />
             <unpack>
                   fungidb_uniquify_and_standardize_seqids.pl
                   --input  @@dataDir@@/cryptococcus_neoformans_grubii_h99_supercontigs.fasta
                   --output @@dataDir@@/cryptococcus_neoformans_grubii_h99_supercontigs.uniquified.fasta
                   --prefix @@cryptococcus_neoformans_grubii_h99_FUNGIDB_ABBREVIATION@@
             </unpack>
             -->

            <pluginArgs>
                   --externalDatabaseName %RESOURCE_NAME%
                   --ncbiTaxId @@cryptococcus_neoformans_grubii_h99_NCBI_TAXONOMY_ID@@
                   --externalDatabaseVersion %RESOURCE_VERSION% 
                   --sequenceFile @@dataDir@@/cryptococcus_neoformans_grubii_h99_supercontigs.uniquified.fasta 
                   --SOTermName 'chromosome'  
                   --regexSourceId  '>(\S+)' 
                   --tableName "DoTS::ExternalNASequence" 
                   --regexChromosome '\d+\.(\d+)' 
	    </pluginArgs>

             <info
                   displayName="Cryptococcus neoformans var. grubii (strain H99, serotype A) genomic sequence"
                   project="FungiDB"
                   organisms="@@cryptococcus_neoformans_grubii_h99_FUNGIDB_DISPLAY_NAME@@"
                   category="Genome"
                   publicUrl="http://www.broadinstitute.org/annotation/genome/cryptococcus_neoformans"
		   contact="Broad Instute Annotation Webmaster"
		   email="annotation-webmaster@broadinstitute.org"
		   institution="Broad Institute">	          
                   <description>
		     <![CDATA[
                              Cryptococcus neoformans var. grubii (H99) genomic sequence. Source: Broad Institute
		     ]]>
                   </description>
          </info>
  </resource>

  <!-- ISF genome annotations -->
  <resource 
         resource="cryptococcus_neoformans_grubii_h99_genome_annotations_RSRC"
         version="@@cryptococcus_neoformans_grubii_h99_GENOME_SEQUENCE_VER@@"
         parentResource="cryptococcus_neoformans_grubii_h99_genome_sequence_RSRC"
         plugin="GUS::Supported::Plugin::InsertSequenceFeatures">

         <!-- Transforming upstream of ISF/workflow... -->
         <manualGet fileOrDir="FungiDB/cryptococcus_neoformans_grubii_h99/genome/%RESOURCE_VERSION%/cryptococcus_neoformans_grubii_h99_transcripts.transformed.gff3"/> 

         <!-- or within the workflow iteself... -->
         <!--
         <manualGet fileOrDir="FungiDB/cryptococcus_neoformans_grubii_h99/genome/%RESOURCE_VERSION%/cryptococcus_neoformans_grubii_h99_transcripts.gff3"/> 
          <unpack>
                fungidb_uniquify_and_standardize_seqids.pl
                --input  @@dataDir@@/cryptococcus_neoformans_grubii_h99_transcripts.gff3
                --output @@dataDir@@/cryptococcus_neoformans_grubii_h99_transcripts.uniquified.gff3
                --prefix @@cryptococcus_neoformans_grubii_h99_FUNGIDB_ABBREVIATION@@
          </unpack>

         <unpack>
             preprocessGFF3 
                --input_gff @@dataDir@@/cryptococcus_neoformans_grubii_h99_transcripts.uniquified.gff3
                --output_gff @@dataDir@@/cryptococcus_neoformans_grubii_h99.transformed.gff3
         </unpack>
         -->

         <pluginArgs>
                --extDbName %PARENT_RESOURCE_NAME%
                --extDbRlsVer %PARENT_RESOURCE_VERSION%
                --mapFile @@gusHome@@/lib/xml/isf/FungiDB/broadGFF32Gus.xml
                --inputFileOrDir @@dataDir@@/cryptococcus_neoformans_grubii_h99_transcripts_transformed.gff3
                --inputFileExtension "gff3"
                --fileFormat gff3
                --soCvsVersion @@SO_VER@@
                --defaultOrganism "@@cryptococcus_neoformans_grubii_h99_NCBI_DISPLAY_NAME@@"
                --seqSoTerm "chromosome"
                --validationLog @@dataDir@@/validation.log
                --bioperlTreeOutput @@dataDir@@/bioperlTree.out
                --seqIdColumn source_id
                --naSequenceSubclass ExternalNASequence
            </pluginArgs>

            <info
                displayName="Cryptococcus neoformans var. grubii (H99) genomic annotations"
                project="FungiDB"
                organisms="@@cryptococcus_neoformans_grubii_h99_FUNGIDB_DISPLAY_NAME@@"
                category="Genome"
		publicUrl="http://www.broadinstitute.org/annotation/genome/cryptococcus_neoformans"
		contact="Broad Instute Annotation Webmaster"
		email="annotation-webmaster@broadinstitute.org"
		institution="Broad Institute">	          

                <description>
                     <![CDATA[
                      Cryptococcus neoformans var. grubii (H99) genomic annotations. Source: The Broad Institute and Duke University.
                      ]]>
                </description>
            </info>
  </resource>



