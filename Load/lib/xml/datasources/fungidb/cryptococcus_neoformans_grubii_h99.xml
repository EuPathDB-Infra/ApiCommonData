  <resource resource="cryptococcus_neoformans_grubii_h99_genedb_ids_RSRC"
            version="@@cryptococcus_neoformans_grubii_h99_VER@@"
            plugin="ApiCommonData::Load::Plugin::InsertNaFeatureNaGene">


    <manualGet fileOrDir="FungiDB/cryptococcus_neoformans_grubii_h99/genome/%RESOURCE_VERSION%/primary_gene_aliases.tab"/>
    <pluginArgs>--MappingFile @@dataDir@@/primary_gene_aliases.tab</pluginArgs>

    <info
        displayName="Mapping of FungiDB IDs to original gene names."
        project="FungiDB"
        organisms="@@cryptococcus_neoformans_grubii_h99_FUNGIDB_DISPLAY_NAME@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[

        ]]>
      </description>
    </info>
  </resource>


  <!-- dbEST -->
  <resource resource="cryptococcus_neoformans_grubii_h99_dbest_RSRC" version="@@cryptococcus_neoformans_grubii_h99_DBEST_VER@@"
                                                          plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId @@cryptococcus_neoformans_grubii_h99_NCBI_TAXONOMY_ID@@ --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file  @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 
                --taxonFile @@dataDir@@/taxonIdFile 
                --extDbName %RESOURCE_NAME% 
                --extDbRlsVer %RESOURCE_VERSION%
                --soVer @@SO_VER@@
                --dbestLoginFile @@dataDir@@/dbest.prop 
                --dbestConnect @@dbestConnect@@
    </pluginArgs>

    <info
        displayName="Cryptococcus neoformans var. grubii (H99) dbEST ESTs"
        project="FungiDB"
        organisms="@@cryptococcus_neoformans_grubii_h99_FUNGIDB_DISPLAY_NAME@@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact="NCBI HelpDesk"
        email="info@ncbi.nlm.nih.gov"
        institution="NCBI">
      <description>
        <![CDATA[
         Cryptococcus neoformans var. grubii (H99) ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>


</resources>
