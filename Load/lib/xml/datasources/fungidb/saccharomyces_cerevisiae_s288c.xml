          plugin="GUS::Community::Plugin::InsertECMapping">

          <!-- Manual delivery: see git:script/mirror_sgd_data.sh -->   
          <manualGet fileOrDir="FungiDB/saccharomyces_cerevisiae_s288c/@@saccharomyces_cerevisiae_s288c_VER@@/xrefs/%RESOURCE_VERSION%" />

          <unpack>
             fungidb_unpack_sgd_ids.pl
                 --input @@dataDir@@/dbxref.tab
                 --output @@dataDir@@/ec_numbers.tab
                 --db IUBMB
                 --type "EC number"
         </unpack>

        <unpack>
                fungidb_prepend_fungidb_identifier.pl
                     --input @@dataDir@@/ec_numbers.tab
                     --output @@dataDir@@/ec_numbers.uniquified.tab
                     --prefix @@saccharomyces_cerevisiae_s288c_FUNGIDB_ABBREVIATION@@
                     --col 0
         </unpack>

         <pluginArgs>
              --ECMappingFile @@dataDir@@/ec_numbers.uniquified.tab
              --evidenceCode "SGD"
              --aaSeqLocusTagMappingSql "SELECT aa_sequence_id FROM dots.GeneFeature g, dots.Transcript t, dots.TranslatedAAFeature taf WHERE g.source_id = ? AND g.na_feature_id = t.parent_id AND t.na_feature_id = taf.na_feature_id"
              --verbose
         </pluginArgs>
    
         <info
            displayName="Saccharomyces cerevisiae (S288C) EC IDs"
            project="FungiDB"
            organisms="@@saccharomyces_cerevisiae_s288c_FUNGIDB_DISPLAY_NAME@@"
            category="Proteome"
            publicUrl="http://downloads.yeastgenome.org/chromosomal_feature/"
            contact=""
            email=""
            institution="">
            <description>
               <![CDATA[
                 Saccharomyces cerevisiae (S288C) EC IDs. Description: EC number of the reaction catalyzed by the S. cerevisiae protein.  EC numbers are assigned to reactions by the Internal Union of Biochemistry and Molecular Biology. Source: SGD.
               ]]>
            </description>
         </info>
     </resource>


