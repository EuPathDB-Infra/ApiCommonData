<!-- Fusarium verticillioidies (unknown) Resources -->
<resources>

     <!-- ISF FASTA resource -->
     <resource 
            resource="fusarium_verticillioides_na_genome_sequence_RSRC"
            version="@@fusarium_verticillioides_na_GENOME_SEQUENCE_VER@@"
            plugin="GUS::Supported::Plugin::LoadFastaSequences">

            <!-- Transforming upstream of ISF/workflow... -->            
            <manualGet 
                   fileOrDir="FungiDB/fusarium_verticillioides_na/genome/%RESOURCE_VERSION%/fusarium_verticillioides_na_supercontigs.uniquified.fasta"/>

            <!-- or within the workflow iteself... -->
            <!--
            <manualGet 
              fileOrDir="FungiDB/fusarium_verticillioides_na/genome/%RESOURCE_VERSION%/fusarium_verticillioides_na_supercontigs.fasta"/>
            <unpack>
                  fungidb_uniquify_and_standardize_seqids.pl
                      --input  @@dataDir@@/fusarium_verticillioides_na_supercontigs.fasta
                      --output @@dataDir@@/fusarium_verticillioides_na_supercontigs_uniquified.fasta
                      --prefix @@fusarium_verticillioides_na_FUNGIDB_ABBREVIATION@@
            </unpack>
            -->

            <pluginArgs>
                   --externalDatabaseName %RESOURCE_NAME%
                   --ncbiTaxId @@fusarium_verticillioides_na_NCBI_TAXONOMY_ID@@
                   --externalDatabaseVersion %RESOURCE_VERSION%
                   --sequenceFile @@dataDir@@/fusarium_verticillioides_na_supercontigs_uniquified.fasta
                   --SOTermName 'chromosome'
                   --regexSourceId  '>(\S+)' 
                   --tableName "DoTS::ExternalNASequence" 
                   --regexChromosome '\d+\.(\d+)'
             </pluginArgs>

             <info
                   displayName="Fusarium verticillioides (7600, FRC M3125, NRRL 20956) genomic sequence"
                   project="FungiDB"
                   organisms="@@fusarium_verticillioides_na_FUNGIDB_DISPLAY_NAME@@"
                   category="Genome"
                   publicUrl="http://www.broadinstitute.org/annotation/genome/fusarium_group/"
                   contact="Broad Instute Annotation Webmaster"
                   email="annotation-webmaster@broadinstitute.org"
                   institution="Broad Institute">          
                   <description>
                          <![CDATA[
                              Fusarium verticillioides (7600, FRC M3125, NRRL 20956) genomic sequence. Source: The Broad Institute
                          ]]>
                   </description>
          </info>
  </resource>

  <!-- ISF genome annotations -->
  <resource 
         resource="fusarium_verticillioides_na_genome_annotations_RSRC"
         version="@@fusarium_verticillioides_na_GENOME_SEQUENCE_VER@@" 
         parentResource="fusarium_verticillioides_na_genome_sequence_RSRC"
         plugin="GUS::Supported::Plugin::InsertSequenceFeatures">


        <!-- Transforming upstream of ISF/workflow... -->
        <manualGet fileOrDir="FungiDB/fusarium_verticillioides_na/genome/%RESOURCE_VERSION%/fusarium_verticillioides_na_transcripts.transformed.gff3" />

        <!-- or within the workflow iteself... -->
        <!--
        <manualGet fileOrDir="FungiDB/fusarium_verticillioides_na/genome/%RESOURCE_VERSION%/fusarium_verticillioides_na_transcripts.gff3" />
        <unpack>
               fungidb_uniquify_and_standardize_seqids.pl
                   --input  @@dataDir@@/fusarium_verticillioides_na_transcripts.gff3
                   --output @@dataDir@@/fusarium_verticillioides_na_transcripts.uniquified.gff3
                   --prefix @@fusarium_verticillioides_na_FUNGIDB_ABBREVIATION@@
        </unpack>

         <unpack>
               preprocessGFF3 
                   --input_gff  @@dataDir@@/fusarium_verticillioides_na_transcripts.uniquified.gff3
                   --output_gff @@dataDir@@/fusarium_verticillioides_na_transcripts.transformed.gff3
         </unpack>
         -->

         <pluginArgs>
                --extDbName %PARENT_RESOURCE_NAME%
                --extDbRlsVer %PARENT_RESOURCE_VERSION%
                --mapFile @@gusHome@@/lib/xml/isf/FungiDB/broadGFF32Gus.xml
                --inputFileOrDir @@dataDir@@/fusarium_verticillioides_na_transcripts.transformed.gff3
                --inputFileExtension "gff3"
                --fileFormat gff3
                --soCvsVersion @@SO_VER@@
                --defaultOrganism "@@fusarium_verticillioides_na_NCBI_DISPLAY_NAME@@"
                --seqSoTerm "chromosome"
                --validationLog @@dataDir@@/validation.log
                --bioperlTreeOutput @@dataDir@@/bioperlTree.out
                --seqIdColumn source_id
                --naSequenceSubclass ExternalNASequence
            </pluginArgs>

            <info
                displayName="Fusarium verticillioides (7600, FRC M3125, NRRL 20956) genomic annotations"
                project="FungiDB"
                organisms="@@fusarium_verticillioides_na_FUNGIDB_DISPLAY_NAME@@"
                category="Genome"
		publicUrl="http://www.broadinstitute.org/annotation/genome/fusarium_group/"
		contact="Broad Instute Annotation Webmaster"
		email="annotation-webmaster@broadinstitute.org"
		institution="Broad Institute">          
	      
                <description>
                     <![CDATA[
                      Fusarium verticillioides (7600, FRC M3125, NRRL 20956) genomic annotations. Source: Broad Institute.
                      ]]>
                </description>
            </info>
  </resource>


 <!-- //////////////////////
           ALIASES
       ////////////////////// -->

  <resource resource="fusarium_verticillioides_na_primary_gene_aliases_RSRC" 
            version="@@fusarium_verticillioides_na_GENOME_SEQUENCE_VER@@"
            plugin="ApiCommonData::Load::Plugin::InsertNaFeatureNaGene">
 
    <manualGet fileOrDir="FungiDB/fusarium_verticillioides_na/genome/%RESOURCE_VERSION%/primary_gene_aliases.tab"/>
    <pluginArgs>--MappingFile @@dataDir@@/primary_gene_aliases.tab</pluginArgs>
    <info
        displayName="Mapping of FungiDB IDs to original gene names."
        project="FungiDB"
        organisms="@@fusarium_verticillioides_na_FUNGIDB_DISPLAY_NAME@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         
        ]]>
      </description>
    </info>
  </resource>

 
  <!-- dbEST -->
  <resource resource="fusarium_verticillioides_na_dbest_RSRC" version="@@fusarium_verticillioides_na_DBEST_VER@@"
                                                          plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId @@fusarium_verticillioides_na_NCBI_TAXONOMY_ID@@ --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file  @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 
                --taxonFile @@dataDir@@/taxonIdFile 
                --extDbName %RESOURCE_NAME% 
                --extDbRlsVer %RESOURCE_VERSION%
                --soVer @@SO_VER@@
                --dbestLoginFile @@dataDir@@/dbest.prop 
                --dbestConnect @@dbestConnect@@
    </pluginArgs>

    <info
        displayName="Fusarium verticillioides dbEST ESTs"
        project="FungiDB"
        organisms="@@fusarium_verticillioides_na_FUNGIDB_DISPLAY_NAME@@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact="NCBI HelpDesk"
        email="info@ncbi.nlm.nih.gov"
        institution="NCBI">
      <description>
        <![CDATA[
         Fusarium verticillioides ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>


</resources>
