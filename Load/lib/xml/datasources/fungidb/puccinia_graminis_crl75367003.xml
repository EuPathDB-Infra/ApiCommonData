<!-- Puccinia graminis (CRL 75-36-700-3) Resources -->
<resources>

     <!-- ISF FASTA resource -->
     <resource 
            resource="puccinia_graminis_tritici_crl75367003_genome_sequence_RSRC"
            version="@@puccinia_graminis_tritici_crl75367003_GENOME_SEQUENCE_VER@@"
            plugin="GUS::Supported::Plugin::LoadFastaSequences">

            <!-- Transforming upstream of ISF/workflow... -->            
            <manualGet 
                   fileOrDir="FungiDB/puccinia_graminis_tritici_crl75367003/genome/%RESOURCE_VERSION%/puccinia_graminis_tritici_crl75367003_supercontigs.uniquified.fasta"/>

            <!-- or within the workflow iteself... -->
            <manualGet 
              fileOrDir="FungiDB/puccinia_graminis_tritici_crl75367003/genome/%RESOURCE_VERSION%/puccinia_graminis_tritici_crl75367003_supercontigs.fasta"/>
            <unpack>
                  fungidb_uniquify_and_standardize_seqids.pl
                      --input  @@dataDir@@/puccinia_graminis_tritici_crl75367003_supercontigs.fasta
                      --output @@dataDir@@/puccinia_graminis_tritici_crl75367003_supercontigs_uniquified.fasta
                      --prefix @@puccinia_graminis_tritici_crl75367003_FUNGIDB_ABBREVIATION@@
            </unpack>

            <pluginArgs>
                   --externalDatabaseName %RESOURCE_NAME%
                   --ncbiTaxId @@puccinia_graminis_tritici_crl75367003_NCBI_TAXONOMY_ID@@
                   --externalDatabaseVersion %RESOURCE_VERSION%
                   --sequenceFile @@dataDir@@/puccinia_graminis_tritici_crl75367003_supercontigs_uniquified.fasta
                   --SOTermName 'chromosome'  
                   --regexSourceId  '>(\S+)' 
                   --tableName "DoTS::ExternalNASequence" 
                   --regexChromosome '\d+\.(\d+)'
             </pluginArgs>

             <info
                   displayName="Puccinia graminis (CRL 75-36-700-3, race SCCL) genomic sequence"
                   project="FungiDB"
                   organisms="@@puccinia_graminis_tritici_crl75367003_FUNGIDB_DISPLAY_NAME@@"
                   category="Genome"
                   publicUrl="http://www.broadinstitute.org/annotation/genome/puccinia_group/"
                   contact="Broad Instute Annotation Webmaster"
                   email="annotation-webmaster@broadinstitute.org"
                   institution="Broad Institute">          
                   <description>
                          <![CDATA[
                              Puccinia graminis (CRL 75-36-700-3, race SCCL) genomic sequence. Source: Broad Institute
                          ]]>
                   </description>
          </info>
  </resource>

  <!-- ISF genome annotations -->
  <resource 
         resource="puccinia_graminis_tritici_crl75367003_genome_annotations_RSRC"
         version="@@puccinia_graminis_tritici_crl75367003_GENOME_SEQUENCE_VER@@" 
         parentResource="puccinia_graminis_tritici_crl75367003_genome_sequence_RSRC"
         plugin="GUS::Supported::Plugin::InsertSequenceFeatures">


        <!-- Transforming upstream of ISF/workflow... -->
        <manualGet fileOrDir="FungiDB/puccinia_graminis_tritici_crl75367003/genome/%RESOURCE_VERSION%/puccinia_graminis_tritici_crl75367003_transcripts.transformed.gff3" />

        <!-- or within the workflow iteself... -->
        <manualGet fileOrDir="FungiDB/puccinia_graminis_tritici_crl75367003/genome/%RESOURCE_VERSION%/puccinia_graminis_tritici_crl75367003_transcripts.gff3" />
        <unpack>
               fungidb_uniquify_and_standardize_seqids.pl
                   --input  @@dataDir@@/puccinia_graminis_tritici_crl75367003_transcripts.gff3
                   --output @@dataDir@@/puccinia_graminis_tritici_crl75367003_transcripts.uniquified.gff3
                   --prefix @@puccinia_graminis_tritici_crl75367003_FUNGIDB_ABBREVIATION@@
        </unpack>

         <unpack>
               preprocessGFF3 
                   --input_gff  @@dataDir@@/puccinia_graminis_tritici_crl75367003_transcripts.uniquified.gff3
                   --output_gff @@dataDir@@/puccinia_graminis_tritici_crl75367003_transcripts.transformed.gff3
         </unpack>


         <pluginArgs>
                --extDbName %PARENT_RESOURCE_NAME%
                --extDbRlsVer %PARENT_RESOURCE_VERSION%
                --mapFile @@gusHome@@/lib/xml/isf/FungiDB/broadGFF32Gus.xml
                --inputFileOrDir @@dataDir@@/puccinia_graminis_tritici_crl75367003_transcripts.transformed.gff3
                --inputFileExtension "gff3"
                --fileFormat gff3
                --soCvsVersion @@SO_VER@@
                --defaultOrganism "@@puccinia_graminis_tritici_crl75367003_NCBI_DISPLAY_NAME@@"
                --seqSoTerm "chromosome"
                --validationLog @@dataDir@@/validation.log
                --bioperlTreeOutput @@dataDir@@/bioperlTree.out
                --seqIdColumn source_id
                --naSequenceSubclass ExternalNASequence
            </pluginArgs>

            <info
		displayName="Puccinia graminis (CRL 75-36-700-3, race SCCL) genomic annotations"
                project="FungiDB"
                organisms="@@puccinia_graminis_tritici_crl75367003_FUNGIDB_DISPLAY_NAME@@"
                category="Genome"
		publicUrl="http://www.broadinstitute.org/annotation/genome/puccinia_group/"
		contact="Broad Instute Annotation Webmaster"
		email="annotation-webmaster@broadinstitute.org"
		institution="Broad Institute">          

                <description>
                     <![CDATA[
                      Puccinia graminis (CRL 75-36-700-3, race SCCL) genomic annotations. Source: Broad Institute
                      ]]>
                </description>
            </info>
  </resource>


 <!-- //////////////////////
           ALIASES
       ////////////////////// -->

  <resource resource="puccinia_graminis_tritici_crl75367003_primary_gene_aliases_RSRC" 
            version="@@puccinia_graminis_tritici_crl75367003_GENOME_SEQUENCE_VER@@"
            plugin="ApiCommonData::Load::Plugin::InsertNaFeatureNaGene">
 
    <manualGet fileOrDir="FungiDB/puccinia_graminis_tritici_crl75367003/genome/%RESOURCE_VERSION%/primary_gene_aliases.tab"/>
    <pluginArgs>--MappingFile @@dataDir@@/primary_gene_aliases.tab</pluginArgs>
    <info
        displayName="Mapping of FungiDB IDs to original gene names."
        project="FungiDB"
        organisms="@@puccinia_graminis_tritici_crl75367003_FUNGIDB_DISPLAY_NAME@@"
        category="ignore"
	publicUrl="http://www.broadinstitute.org/annotation/genome/puccinia_group/"
	contact="Broad Instute Annotation Webmaster"
	email="annotation-webmaster@broadinstitute.org"
	institution="Broad Institute">          
      <description>
        <![CDATA[
         
        ]]>
      </description>
    </info>
  </resource>

 
  <!-- dbEST -->
  <resource resource="puccinia_graminis_tritici_crl75367003_dbEST_RSRC" version="@@puccinia_graminis_tritici_crl75367003_DBEST_VER@@"
                                                          plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId @@puccinia_graminis_tritici_crl75367003_NCBI_TAXONOMY_ID@@ --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file  @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 
                --taxonFile @@dataDir@@/taxonIdFile 
                --extDbName %RESOURCE_NAME% 
                --extDbRlsVer %RESOURCE_VERSION%
                --soVer @@SO_VER@@
                --dbestLoginFile @@dataDir@@/dbest.prop 
                --dbestConnect @@dbestConnect@@
    </pluginArgs>

    <info
        displayName="Puccinia graminis dbEST ESTs"
        project="FungiDB"
        organisms="@@puccinia_graminis_tritici_crl75367003_FUNGIDB_DISPLAY_NAME@@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact="NCBI HelpDesk"
        email="info@ncbi.nlm.nih.gov"
        institution="NCBI">
      <description>
        <![CDATA[
         Puccinia graminis ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>


</resources>
