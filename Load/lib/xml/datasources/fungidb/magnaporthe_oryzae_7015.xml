<!-- Magnaporthe oryzae (70-15) Resources -->
<resources>

     <!-- ISF FASTA resource -->
     <resource 
            resource="magnaporthe_oryzae_7015_genome_sequence_RSRC"
            version="@@magnaporthe_oryzae_7015_GENOME_SEQUENCE_VER@@"
            plugin="GUS::Supported::Plugin::LoadFastaSequences">

            <!-- Transforming upstream of ISF/workflow... -->            
            <manualGet 
                   fileOrDir="FungiDB/magnaporthe_oryzae_7015/genome/%RESOURCE_VERSION%/magnaporthe_oryzae_7015_supercontigs.uniquified.fasta"/>

            <!-- or within the workflow iteself... -->
            <!--
            <manualGet 
              fileOrDir="FungiDB/magnaporthe_oryzae_7015/genome/%RESOURCE_VERSION%/magnaporthe_oryzae_7015_supercontigs.fasta"/>
            <unpack>
                  fungidb_uniquify_and_standardize_seqids.pl
                      --input  @@dataDir@@/magnaporthe_oryzae_7015_supercontigs.fasta
                      --output @@dataDir@@/magnaporthe_oryzae_7015_supercontigs_uniquified.fasta
                      --prefix @@magnaporthe_oryzae_7015_FUNGIDB_ABBREVIATION@@
            </unpack>
            -->

            <pluginArgs>
                   --externalDatabaseName %RESOURCE_NAME%
                   --ncbiTaxId @@magnaporthe_oryzae_7015_NCBI_TAXONOMY_ID@@
                   --externalDatabaseVersion %RESOURCE_VERSION%
                   --sequenceFile @@dataDir@@/magnaporthe_oryzae_7015_supercontigs_uniquified.fasta
                   --SOTermName 'chromosome'  
                   --regexSourceId  '>(\S+)' 
                   --tableName "DoTS::ExternalNASequence" 
                   --regexChromosome '\d+\.(\d+)'
             </pluginArgs>

             <info
                   displayName="Magnaporthe oryzae (70-15) genomic sequence"
                   project="FungiDB"
                   organisms="@@magnaporthe_oryzae_7015_FUNGIDB_DISPLAY_NAME@@"
                   category="Genome"
                   publicUrl=""
                   contact=""
                   email=""
                   institution="The Broad Institute">
          
                   <description>
                          <![CDATA[
                              Magnaporthe oryzae (70-15) genomic sequence. Source: The Broad Institute
                          ]]>
                   </description>
          </info>
  </resource>

  <!-- ISF genome annotations -->
  <resource 
         resource="magnaporthe_oryzae_7015_genome_annotations_RSRC"
         version="@@magnaporthe_oryzae_7015_GENOME_SEQUENCE_VER@@" 
         parentResource="magnaporthe_oryzae_7015_genome_sequence_RSRC"
         plugin="GUS::Supported::Plugin::InsertSequenceFeatures">


        <!-- Transforming upstream of ISF/workflow... -->
        <manualGet fileOrDir="FungiDB/magnaporthe_oryzae_7015/genome/%RESOURCE_VERSION%/magnaporthe_oryzae_7015_transcripts.transformed.gff3" />

        <!-- or within the workflow iteself... -->
        <!--
        <manualGet fileOrDir="FungiDB/magnaporthe_oryzae_7015/genome/%RESOURCE_VERSION%/magnaporthe_oryzae_7015_transcripts.gff3" />
        <unpack>
               fungidb_uniquify_and_standardize_seqids.pl
                   --input  @@dataDir@@/magnaporthe_oryzae_7015_transcripts.gff3
                   --output @@dataDir@@/magnaporthe_oryzae_7015_transcripts.uniquified.gff3
                   --prefix @@magnaporthe_oryzae_7015_FUNGIDB_ABBREVIATION@@
        </unpack>

         <unpack>
               preprocessGFF3 
                   --input_gff  @@dataDir@@/magnaporthe_oryzae_7015_transcripts.uniquified.gff3
                   --output_gff @@dataDir@@/magnaporthe_oryzae_7015_transcripts.transformed.gff3
         </unpack>
         -->

         <pluginArgs>
                --extDbName %PARENT_RESOURCE_NAME%
                --extDbRlsVer %PARENT_RESOURCE_VERSION%
                --mapFile @@gusHome@@/lib/xml/isf/FungiDB/broadGFF32Gus.xml
                --inputFileOrDir @@dataDir@@/magnaporthe_oryzae_7015_transcripts.transformed.gff3
                --inputFileExtension "gff3"
                --fileFormat gff3
                --soCvsVersion @@SO_VER@@
                --defaultOrganism "@@magnaporthe_oryzae_7015_NCBI_DISPLAY_NAME@@"
                --seqSoTerm "chromosome"
                --validationLog @@dataDir@@/validation.log
                --bioperlTreeOutput @@dataDir@@/bioperlTree.out
                --seqIdColumn source_id
                --naSequenceSubclass ExternalNASequence
            </pluginArgs>

            <info
                displayName="Magnaporthe oryzae genomic annotations"
                project="FungiDB"
                organisms="@@magnaporthe_oryzae_7015_FUNGIDB_DISPLAY_NAME@@"
                category="Genome"
                publicUrl=""
                contact=""
                email=""
                institution="The Broad Institute">

                <description>
                     <![CDATA[
                      Magnaporthe oryzae genomic annotations. Source: The Broad Institute.
                      ]]>
                </description>
            </info>
  </resource>


 <!-- //////////////////////
           ALIASES
       ////////////////////// -->

  <resource resource="magnaporthe_oryzae_7015_primary_gene_aliases_RSRC" 
            version="@@magnaporthe_oryzae_7015_GENOME_SEQUENCE_VER@@"
            plugin="ApiCommonData::Load::Plugin::InsertNaFeatureNaGene">
 
    <manualGet fileOrDir="FungiDB/magnaporthe_oryzae_7015/genome/%RESOURCE_VERSION%/primary_gene_aliases.tab"/>
    <pluginArgs>--MappingFile @@dataDir@@/primary_gene_aliases.tab</pluginArgs>
    <info
        displayName="Mapping of FungiDB IDs to original gene names."
        project="FungiDB"
        organisms="@@magnaporthe_oryzae_7015_FUNGIDB_DISPLAY_NAME@@"
        category="ignore"
        publicUrl=""
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         
        ]]>
      </description>
    </info>
  </resource>

 
  <!-- dbEST -->
  <resource resource="magnaporthe_oryzae_7015_dbEST_RSRC" version="@@magnaporthe_oryzae_7015_DBEST_VER@@"
                                                          plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId @@magnaporthe_oryzae_7015_NCBI_TAXONOMY_ID@@ --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file  @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 
                --taxonFile @@dataDir@@/taxonIdFile 
                --extDbName %RESOURCE_NAME% 
                --extDbRlsVer %RESOURCE_VERSION%
                --soVer @@SO_VER@@
                --dbestLoginFile @@dataDir@@/dbest.prop 
                --dbestConnect @@dbestConnect@@
    </pluginArgs>

    <info
        displayName="Magnaporthe oryzae dbEST ESTs"
        project="FungiDB"
        organisms="@@magnaporthe_oryzae_7015_FUNGIDB_DISPLAY_NAME@@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact=""
        email=""
        institution="">
      <description>
        <![CDATA[
         Magnaporthe oryzae ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>


</resources>
