<!-- Neosartorya fischeri (NRRL 181) Resources -->
<resources>

     <!-- ISF FASTA resource -->
     <resource 
            resource="neosartorya_fischeri_nrrl181_genome_sequence_RSRC"
            version="@@neosartorya_fischeri_nrrl181_GENOME_SEQUENCE_VER@@"
            plugin="GUS::Supported::Plugin::LoadFastaSequences">

            <!-- Transforming upstream of ISF/workflow... -->            
            <manualGet 
                   fileOrDir="FungiDB/neosartorya_fischeri_nrrl181/genome/%RESOURCE_VERSION%/neosartorya_fischeri_nrrl181_supercontigs.uniquified.fasta"/>

            <!-- or within the workflow iteself... -->
            <!--
            <manualGet 
              fileOrDir="FungiDB/neosartorya_fischeri_nrrl181/genome/%RESOURCE_VERSION%/neosartorya_fischeri_nrrl181_supercontigs.fasta"/>
            <unpack>
                  fungidb_uniquify_and_standardize_seqids.pl
                      --input  @@dataDir@@/neosartorya_fischeri_nrrl181_supercontigs.fasta
                      --output @@dataDir@@/neosartorya_fischeri_nrrl181_supercontigs_uniquified.fasta
                      --prefix @@neosartorya_fischeri_nrrl181_FUNGIDB_ABBREVIATION@@
            </unpack>
            -->

            <pluginArgs>
                   --externalDatabaseName %RESOURCE_NAME%
                   --ncbiTaxId @@neosartorya_fischeri_nrrl181_NCBI_TAXONOMY_ID@@
                   --externalDatabaseVersion %RESOURCE_VERSION%
                   --sequenceFile @@dataDir@@/neosartorya_fischeri_nrrl181_supercontigs_uniquified.fasta
                   --SOTermName 'chromosome'  
                   --regexSourceId  '>(\S+)' 
                   --tableName "DoTS::ExternalNASequence" 
                   --regexChromosome '\d+\.(\d+)'
             </pluginArgs>

             <info
                   displayName="Neosartorya fischeri (NRRL 181) genomic sequence"
                   project="FungiDB"
                   organisms="@@neosartorya_fischeri_nrrl181_FUNGIDB_DISPLAY_NAME@@"
                   category="Genome"
		   publicUrl="http://www.broadinstitute.org/annotation/genome/aspergillus_group/"
                   contact="Broad Instute Annotation Webmaster"
                   email="annotation-webmaster@broadinstitute.org"
                   institution="@@neosartorya_fischeri_nrrl181_GENOME_SOURCE@@">          
                   <description>
                          <![CDATA[
                              Neosartorya fischeri (NRRL 181) genomic sequence. Source: J.Craig Venter Institute via Broad Institute Aspergillus Comparative Site
                          ]]>
                   </description>
          </info>
  </resource>

  <!-- ISF genome annotations -->
  <resource 
         resource="neosartorya_fischeri_nrrl181_genome_annotations_RSRC"
         version="@@neosartorya_fischeri_nrrl181_GENOME_SEQUENCE_VER@@" 
         parentResource="neosartorya_fischeri_nrrl181_genome_sequence_RSRC"
         plugin="GUS::Supported::Plugin::InsertSequenceFeatures">


        <!-- Transforming upstream of ISF/workflow... -->
        <manualGet fileOrDir="FungiDB/neosartorya_fischeri_nrrl181/genome/%RESOURCE_VERSION%/neosartorya_fischeri_nrrl181_transcripts.transformed.gff3" />

        <!-- or within the workflow iteself... -->
        <!--
        <manualGet fileOrDir="FungiDB/neosartorya_fischeri_nrrl181/genome/%RESOURCE_VERSION%/neosartorya_fischeri_nrrl181_transcripts.gff3" />
        <unpack>
               fungidb_uniquify_and_standardize_seqids.pl
                   --input  @@dataDir@@/neosartorya_fischeri_nrrl181_transcripts.gff3
                   --output @@dataDir@@/neosartorya_fischeri_nrrl181_transcripts.uniquified.gff3
                   --prefix @@neosartorya_fischeri_nrrl181_FUNGIDB_ABBREVIATION@@
        </unpack>

         <unpack>
               preprocessGFF3 
                   --input_gff  @@dataDir@@/neosartorya_fischeri_nrrl181_transcripts.uniquified.gff3
                   --output_gff @@dataDir@@/neosartorya_fischeri_nrrl181_transcripts.transformed.gff3
         </unpack>
         -->

         <pluginArgs>
                --extDbName %PARENT_RESOURCE_NAME%
                --extDbRlsVer %PARENT_RESOURCE_VERSION%
                --mapFile @@gusHome@@/lib/xml/isf/FungiDB/broadGFF32Gus.xml
                --inputFileOrDir @@dataDir@@/neosartorya_fischeri_nrrl181_transcripts.transformed.gff3
                --inputFileExtension "gff3"
                --fileFormat gff3
                --soCvsVersion @@SO_VER@@
                --defaultOrganism "@@neosartorya_fischeri_nrrl181_NCBI_DISPLAY_NAME@@"
                --seqSoTerm "chromosome"
                --validationLog @@dataDir@@/validation.log
                --bioperlTreeOutput @@dataDir@@/bioperlTree.out
                --seqIdColumn source_id
                --naSequenceSubclass ExternalNASequence
            </pluginArgs>

            <info
                displayName="Neosartorya fischeri genomic annotations"
                project="FungiDB"
                organisms="@@neosartorya_fischeri_nrrl181_FUNGIDB_DISPLAY_NAME@@"
                category="Genome"
		publicUrl="http://www.broadinstitute.org/annotation/genome/aspergillus_group/"
		contact="Broad Instute Annotation Webmaster"
		email="annotation-webmaster@broadinstitute.org"
		institution="@@neosartorya_fischeri_nrrl181_GENOME_SOURCE@@">
	      <description>
                     <![CDATA[
                      Neosartorya fischeri genomic annotations. Source: J. Craig Venter Institute via Broad Institute Aspergillus Comparative Site
                      ]]>
                </description>
            </info>
  </resource>


 <!-- //////////////////////
           ALIASES
       ////////////////////// -->

  <resource resource="neosartorya_fischeri_nrrl181_primary_gene_aliases_RSRC" 
            version="@@neosartorya_fischeri_nrrl181_GENOME_SEQUENCE_VER@@"
            plugin="ApiCommonData::Load::Plugin::InsertNaFeatureNaGene">
 
    <manualGet fileOrDir="FungiDB/neosartorya_fischeri_nrrl181/genome/%RESOURCE_VERSION%/primary_gene_aliases.tab"/>
    <pluginArgs>--MappingFile @@dataDir@@/primary_gene_aliases.tab</pluginArgs>
    <info
        displayName="Mapping of FungiDB IDs to original gene names."
        project="FungiDB"
        organisms="@@neosartorya_fischeri_nrrl181_FUNGIDB_DISPLAY_NAME@@"
        category="ignore"
	publicUrl="http://www.broadinstitute.org/annotation/genome/aspergillus_group/"
	contact="Broad Instute Annotation Webmaster"
	email="annotation-webmaster@broadinstitute.org"
	institution="@@neosartorya_fischeri_nrrl181_GENOME_SOURCE@@">
      <description>
        <![CDATA[
         
        ]]>
      </description>
    </info>
  </resource>

 
  <!-- dbEST -->
  <resource resource="neosartorya_fischeri_nrrl181_dbEST_RSRC" version="@@neosartorya_fischeri_nrrl181_DBEST_VER@@"
                                                          plugin="GUS::Supported::Plugin::dbEST">
    <manualGet fileOrDir="global/empty"/>
    <unpack>getSubTaxa --ncbiTaxId @@neosartorya_fischeri_nrrl181_NCBI_TAXONOMY_ID@@ --outputFile @@dataDir@@/taxonIdFile</unpack>
    <unpack>createPropFile --file  @@dataDir@@/dbest.prop --propList "dbestLogin:@@dbestLogin@@,dbestPswd:@@dbestPswd@@"</unpack>
    <pluginArgs>--span 500 
                --taxonFile @@dataDir@@/taxonIdFile 
                --extDbName %RESOURCE_NAME% 
                --extDbRlsVer %RESOURCE_VERSION%
                --soVer @@SO_VER@@
                --dbestLoginFile @@dataDir@@/dbest.prop 
                --dbestConnect @@dbestConnect@@
    </pluginArgs>

    <info
        displayName="Neosartorya fischeri dbEST ESTs"
        project="FungiDB"
        organisms="@@neosartorya_fischeri_nrrl181_FUNGIDB_DISPLAY_NAME@@"
        category="Transcriptome"
        publicUrl="http://www.ncbi.nlm.nih.gov/dbEST/"
        contact="NCBI HelpDesk"
        email="info@ncbi.nlm.nih.gov"
        institution="NCBI">
      <description>
        <![CDATA[
         Neosartorya fischeri ESTs from dbEST.
        ]]>
      </description>
    </info>
  </resource>


</resources>
