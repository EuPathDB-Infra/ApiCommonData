            extDbRlsDescrip="E. histolytica Intestinal Colonization/Invasion and Stage Conversion Array Profiles"
            dbCommit="@dbcommit@">
     <manualGet fileOrDir="/files/cbil/data/cbil/AmoebaDB/manualDelivery/Microarray/Gilchrist_Singh/workspace/analysis/" 
            contact="John Brestelli"
