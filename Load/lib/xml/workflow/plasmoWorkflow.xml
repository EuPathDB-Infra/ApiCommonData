<workflowGraph name="trypWorkflow">
  <param name="PfalciparumExtDbRlsSpec"/>
  <param name="PfalciparumVirtualSeqsExtDbRlsSpec"/>
  <param name="PfalciparumGenomeSource"/>
  <param name="PvivaxExtDbRlsSpec"/>
  <param name="PvivaxGenomeSource"/>
  <param name="PvivaxVirtualSeqsExtDbRlsSpec"/>
  <param name="PyoeliiExtDbRlsSpec"/>
  <param name="PyoeliiVirtualSeqsExtDbRlsSpec"/>
  <param name="PyoeliiGenomeSource"/>
  <param name="PyoeliiExtDbRlsSpec"/>
  <param name="PyoeliiVirtualSeqsExtDbRlsSpec"/>
  <param name="PyoeliiGenomeSource"/>
  <param name="PchabaudiExtDbRlsSpec"/>
  <param name="PchabaudiVirtualSeqsExtDbRlsSpec"/>
  <param name="PchabaudiGenomeSource"/>
  <param name="PknowlesiExtDbRlsSpec"/>
  <param name="PknowlesiVirtualSeqsExtDbRlsSpec"/>
  <param name="PknowlesiGenomeSource"/>
  <param name="PfalciparumPlastidExtDbRlsSpec"/>
  <param name="PfalciparumPlastidVirtualSeqsExtDbRlsSpec"/>
  <param name="PfalciparumPlastidGenomeSource"/>
  <param name="PfalciparumMitoExtDbRlsSpec"/>
  <param name="PfalciparumMitoVirtualSeqsExtDbRlsSpec"/>
  <param name="PfalciparumMitoGenomeSource"/>
  <param name="PvivaxMitoExtDbRlsSpec"/>
  <param name="PvivaxMitoVirtualSeqsExtDbRlsSpec"/>
  <param name="PvivaxMitoGenomeSource"/>
  <param name="PreichenowiExtDbRlsSpec"/>
  <param name="PreichenowiVirtualSeqsExtDbRlsSpec"/>
  <param name="PreichenowiGenomeSource"/>
  <param name="PgallinaceumExtDbRlsSpec"/>
  <param name="PgallinaceumVirtualSeqsExtDbRlsSpec"/>
  <param name="PgallinaceumGenomeSource"/>
  <param name="interproVersion"/>
  <param name="iedbVersion"/>
  <param name="nrdbVersion"/>
  <param name="dbESTVersion"/>
  <param name="soVersion"/>
  <param name="goVersion"/>
  <param name="ncbiTaxonVersion"/>
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="pdbExtDbRlsSpec"/>


  <constant name="pdbFile">common/pdb.fsa</constant>
  <constant name="nrdbFile">common/nrdb.fsa</constant>
  <constant name="nrdbShortDeflineFile">common/nrdb-short-defline.fsa</constant>
  <constant name="mercatorDir">common/mercator/</constant>

  <subgraph name="common" xmlFile="commonAnalysis.xml">
    <paramValue name="nrdbExtDbRlsSpec">NRDB|$$nrdbVersion$$</paramValue>
    <paramValue name="nrdbOutputFile">$$nrdbFile$$</paramValue>
    <paramValue name="nrdbShortDeflineOutputFile">$$nrdbShortDeflineFile$$</paramValue>
    <paramValue name="pdbOutputFile">$$pdbFile$$</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
  </subgraph>

  <subgraph name="PfalciparumAnalysis" xmlFile="organismAnalysis.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Pfalciparum</paramValue>
    <paramValue name="organismFullName">Plasmodium falciparum</paramValue>
    <paramValue name="organismTwoLetterAbbrev">Pf</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$PfalciparumExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec">$$PfalciparumVirtualSeqsExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeSource">$$PfalciparumGenomeSource$$</paramValue>
    <paramValue name="interproVersion">$$interproVersion$$</paramValue>
    <paramValue name="iedbVersion">$$iedbVersion$$</paramValue>
    <paramValue name="soVersion">$$soVersion$$</paramValue>
    <paramValue name="goVersion">$$goVersion$$</paramValue>
    <paramValue name="dbESTVersion">$$dbESTVersion$$</paramValue>
    <paramValue name="ncbiTaxonVersion">$$ncbiTaxonVersion$$</paramValue>
    <paramValue name="ncbiTaxonId">36329</paramValue>    
    <paramValue name="parentNcbiTaxonId">5833</paramValue>
    <paramValue name="makePredictedTranscripts">true</paramValue>
    <paramValue name="pdbFile">$$pdbFile$$</paramValue>
    <paramValue name="nrdbFile">$$nrdbFile$$</paramValue>
    <paramValue name="nrdbShortDeflineFile">$$nrdbShortDeflineFile$$</paramValue>
    <paramValue name="maxIntronSize">20000</paramValue>
    <paramValue name="calculateTranslatedProteins">false</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">Eukaryota Apicomplexa Plasmodium</paramValue>
    <paramValue name="runSplign">false</paramValue>
    <paramValue name="nrdbExtDbRlsSpec">NRDB|$$nrdbVersion$$</paramValue>
    <paramValue name="repeatMaskSpecies">-species Plasmodium</paramValue>
    <paramValue name="pdbExtDbRlsSpec">$$pdbExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeSize">23267177</paramValue>
    <depends name="common"/>
  </subgraph>





  <subgraph name="MercatorAnalysis" xmlFile="mercatorAnalysis.xml">
    <paramValue name="mercator_genomes"></paramValue>
    <paramValue name="mercator_reference_genome"></paramValue>
    <paramValue name="mercator_draft_genomes"></paramValue>
    <paramValue name="mercator_nondraft_genomes"></paramValue>
    <paramValue name="mercator_tree"></paramValue>
    <paramValue name="syntenyExtDbRlsSpec"></paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="genomeExtDbRlsSpec"></paramValue>
    <paramValue name="mercatorDir">$$mercatorDir$$</paramValue>
    <depends name="TcruziAnalysis"/>
    <depends name="TbruceiAnalysis"/>
  </subgraph>

</workflowGraph>
