    <paramValue name="ncbiTaxonVersion">$$ncbiTaxonVersion$$</paramValue>
    <paramValue name="ncbiTaxonId">5671</paramValue>
    <paramValue name="parentNcbiTaxonId">5671</paramValue>
    <paramValue name="predictedTranscriptsSql"></paramValue>
    <paramValue name="pdbFile">$$pdbFile$$</paramValue>
    <paramValue name="nrdbFile">$$nrdbFile$$</paramValue>
    <paramValue name="nrdbShortDeflineFile">$$nrdbShortDeflineFile$$</paramValue>
    <paramValue name="maxIntronSize">20000</paramValue>
    <paramValue name="calculateTranslatedProteins">true</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">Eukaryota, Kinetoplastida, Leishmania</paramValue>
    <paramValue name="runSplign">false</paramValue>
    <paramValue name="nrdbExtDbRlsSpec">NRDB|$$nrdbVersion$$</paramValue>
