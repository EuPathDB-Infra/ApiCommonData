<workflowGraph name="trypWorkflow">
  <param name="tcruziExtDbRlsSpec"/>
  <param name="tcruziVirtualSeqsExtDbRlsSpecList"/>
  <param name="tcruziGenomeSource"/>
  <param name="tbruceiExtDbRlsSpec"/>
  <param name="tbruceiGenomeSource"/>
  <param name="tbruceiVirtualSeqsExtDbRlsSpec"/>
  <param name="linfantumExtDbRlsSpec"/>
  <param name="linfantumVirtualSeqsExtDbRlsSpec"/>
  <param name="linfantumGenomeSource"/>
  <param name="lmajorExtDbRlsSpec"/>
  <param name="lmajorVirtualSeqsExtDbRlsSpec"/>
  <param name="lmajorGenomeSource"/>
  <param name="lbraziliensisExtDbRlsSpec"/>
  <param name="lbraziliensisVirtualSeqsExtDbRlsSpec"/>
