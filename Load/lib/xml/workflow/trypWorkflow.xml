<workflowGraph name="trypWorkflow">
  <param name="tcruziExtDb"/>
  <param name="tcruziExtDbRls"/>
  <param name="tcruziGenomeSource"/>
  <param name="tcruziVirtualSeqExtDb"/>
  <param name="tcruziVirtualSeqExtDbRls"/>
  <param name="tbruceiExtDb"/>
  <param name="tbruceiExtDbRls"/>
  <param name="tbruceiGenomeSource"/>
  <param name="tbruceiVirtualSeqExtDb"/>
  <param name="tbruceiVirtualSeqExtDbRls"/>
  <param name="interproVersion"/>
  <param name="iedbVersion"/>
  <param name="nrdbVersion"/>
  <param name="dbESTVersion"/>
  <param name="soVersion"/>
  <param name="tigrVersion"/>
  <param name="ncbiTaxonVersion"/>
  <param name="projectDB"/>
  <param name="projectRelease"/>

  <constant name="pdbFile">common/pdb.fsa</constant>
  <constant name="nrdbFile">common/nrdb.fsa</constant>
  <constant name="nrdbShortDeflineFile">common/nrdb-short-defline.fsa</constant>

  <subgraph name="common" xmlFile="commonAnalysis.xml">
    <paramValue name="nrdbExtDbRlsSpec">nrdb|$$nrdbVersion$$</paramValue>
    <paramValue name="nrdbOutputFile">$$nrdbFile$$</paramValue>
    <paramValue name="nrdbShortDeflineOutputFile">$$nrdbShortDeflineFile$$</paramValue>
    <paramValue name="pdbOutputFile">$$pdbFile$$</paramValue>
    <paramValue name="projectDB">$$projectDB$$</paramValue>
    <paramValue name="projectRelease">$$projectRelease$$</paramValue>
  </subgraph>

  <subgraph name="TcruziAnalysis" xmlFile="organismAnalysis.xml">
    <paramValue name="projectDB">$$projectDB$$</paramValue>
    <paramValue name="projectRelease">$$projectRelease$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Tcruzi</paramValue>
    <paramValue name="organismTwoLetterAbbrev">Tc</paramValue>
    <paramValue name="genomeExtDb">$$tcruziExtDb$$</paramValue>
    <paramValue name="genomeExtDbRls">$$$cruziExtDbRls$</paramValue>
    <paramValue name="genomeVirtualSeqExtDb">$$tcruziVirtualSeqExtDb$$</paramValue>
    <paramValue name="genomeVirtualSeqExtDbRls">$$tcruziVirtualSeqExtDbRls$$</paramValue>
    <paramValue name="genomeSource">$$tcruziGenomeSource$$</paramValue>
    <paramValue name="interproVersion">$$interproVersion$$</paramValue>
    <paramValue name="iedbVersion">$$interproVersion$$</paramValue>
    <paramValue name="soVersion">$$soVersion$$</paramValue>
    <paramValue name="dbESTVersion">$$dbESTVersion$$</paramValue>
    <paramValue name="tigrVersion">$$tigrVersion$$</paramValue>
    <paramValue name="ncbiTaxonVersion">$$ncbiTaxonVersion$$</paramValue>
    <paramValue name="ncbiTaxonId">137566</paramValue>    
    <paramValue name="parentNcbiTaxonId">5691</paramValue>
    <paramValue name="predictedTranscriptsSql"></paramValue>
    <paramValue name="pdbOutputFile">$$pdbFile$$</paramValue>
    <paramValue name="nrdbOutputFile">$$nrdbFile$$</paramValue>
    <paramValue name="nrdbShortDeflineOutputFile">$$nrdbShortDeflineFile$$</paramValue>
    <paramValue name="maxIntronSize">20000</paramValue>
    <paramValue name="calculateTranslatedProteins">false</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">Eukaryota, Hexamitidae, Trypanosoma</paramValue>
    <paramValue name="runSplign">true</paramValue>
    <paramValue name="nrdbExtDbRlsSpec">nrdb|$$nrdbVersion$$</paramValue>
    <depends name="common"/>
  </subgraph>

  <subgraph name="TbruceiAnalysis" xmlFile="organismAnalysis.xml">
    <paramValue name="projectDB">$$projectDB$$</paramValue>
    <paramValue name="projectRelease">$$projectRelease$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Tbrucei</paramValue>
    <paramValue name="organismTwoLetterAbbrev">Tb</paramValue>
    <paramValue name="genomeExtDb">$$tbruceiExtDb$$</paramValue>
    <paramValue name="genomeExtDbRls">$$$tbruceiExtDbRls$</paramValue>
    <paramValue name="genomeVirtualSeqExtDb">$$tbruceiVirtualSeqExtDb$$</paramValue>
    <paramValue name="genomeVirtualSeqExtDbRls">$$tbruceiVirtualSeqExtDbRls$$</paramValue>
    <paramValue name="genomeSource">$$tbruceiGenomeSource$$</paramValue>
    <paramValue name="genomeVirtualSeqExtDb">$$tbruceiVirtualSeqExtDb$$</paramValue>
    <paramValue name="genomeVirtualSeqExtDbRls">$$tbruceiVirtualSeqExtDbRls$$</paramValue>
    <paramValue name="genomeSource">$$tbruceiGenomeSource$$</paramValue>
    <paramValue name="interproVersion">$$interproVersion$$</paramValue>
    <paramValue name="iedbVersion">$$interproVersion$$</paramValue>
    <paramValue name="soVersion">$$soVersion$$</paramValue>
    <paramValue name="dbESTVersion">$$dbESTVersion$$</paramValue>
    <paramValue name="tigrVersion">$$tigrVersion$$</paramValue>
    <paramValue name="ncbiTaxonVersion">$$ncbiTaxonVersion$$</paramValue>
    <paramValue name="ncbiTaxonId">4339566</paramValue>
    <paramValue name="predictedTranscriptsSql"></paramValue>
    <paramValue name="pdbOutputFile">$$pdbFile$$</paramValue>
    <paramValue name="nrdbOutputFile">$$nrdbFile$$</paramValue>
    <paramValue name="nrdbShortDeflineOutputFile">$$nrdbShortDeflineFile$$</paramValue>
    <paramValue name="maxIntronSize">20000</paramValue>
    <paramValue name="calculateTranslatedProteins">false</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">Eukaryota, Hexamitidae, Trypanosoma</paramValue>
    <paramValue name="runSplign">true</paramValue>
    <paramValue name="nrdbExtDbRlsSpec">nrdb|$$nrdbVersion$$</paramValue>
    <depends name="common"/>
  </subgraph>

  <subgraph name="mercator" xmlFile="mercatorAnalysis.xml">
    <paramValue name="mercator_genomes"></paramValue>
    <paramValue name="mercator_reference_genome"></paramValue>
    <paramValue name="mercator_draft_genomes"></paramValue>
    <paramValue name="mercator_nondraft_genomes"></paramValue>
    <paramValue name="mercator_tree"></paramValue>
    <paramValue name="syntenyExtDbRlsSpec"></paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec">$$tcruziVirtualSeqExtDbRlsSpec$$,$$tbruceiVirtualSeqExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$tcruziExtDbRlsSpec$$,$$tbruceiExtDbRlsSpec$$</paramValue>
  </subgraph>

</workflowGraph>
