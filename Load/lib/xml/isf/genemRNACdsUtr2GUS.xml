  </feature>

  <feature name="three_prime_utr" table="DoTS::UTRFeature" so="three_prime_UTR">
    <qualifier name="ID" column="source_id"/>
   	<qualifier name="controlled_curation" ignore="true"/>
    <qualifier name="note"       ignore="true"/>
