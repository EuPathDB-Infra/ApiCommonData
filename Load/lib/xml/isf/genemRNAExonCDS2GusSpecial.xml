  <bioperlSeqPreprocessor class="ApiCommonData::Load::genemRNAExonCDS2BioperlTree"/>

  <gusSkeletonMaker class="ApiCommonData::Load::GusSkeletonMakerSpecial"
                    method="makeGeneSkeleton"/>

  <specialCaseQualifierHandler name="api" 
           class="ApiCommonData::Load::gus4SpecialCaseQualifierHandlers"/>

